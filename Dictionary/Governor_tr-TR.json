{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Governor.tr-TR",
      "VoicePackName": "Dictionary (Governor - tr-TR)",
      "Character": "Governor",
      "Language": "tr-TR",
      "Entries": [
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Elbette! Bir yıldır bu anı bekliyorum.",
          "AudioPath": "assets/tr-TR/Governor/1.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "...",
          "AudioPath": "assets/tr-TR/Governor/2.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_10",
          "DialogueText": "Şey... Aslında biraz midem bulandı. Sanırım bu sene çorbayı içemeyeceğim.",
          "AudioPath": "assets/tr-TR/Governor/3.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_11",
          "DialogueText": "Öğk! Bu iğrenç! Sanırım kusacağım...",
          "AudioPath": "assets/tr-TR/Governor/4.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_12",
          "DialogueText": "Biraz uzanmam gerekiyor...",
          "AudioPath": "assets/tr-TR/Governor/5.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_13",
          "DialogueText": "Çok memnun olurum! Öğleden beri çorbadan gözlerimi ayıramadım.",
          "AudioPath": "assets/tr-TR/Governor/6.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_15",
          "DialogueText": "Ne güzel bir etkinlik... Yıldızçiyi Vadisi'ni ziyaret etmek büyük bir zevk.Hanımla konuşayım da buradan yazlık alalım.",
          "AudioPath": "assets/tr-TR/Governor/7.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_16",
          "DialogueText": "Her yıl Yıldızçiyi Vadisi'ne yapacağım ziyareti sabırsızlıkla bekliyorum. Havada hissedilir bir çekicilik var!",
          "AudioPath": "assets/tr-TR/Governor/8.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_2",
          "DialogueText": "Hım... Keskin bir tadı var... ama aroması çok güzel!",
          "AudioPath": "assets/tr-TR/Governor/9.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_3",
          "DialogueText": "Bir dakika... kasemde bir şey var... bu da ne?",
          "AudioPath": "assets/tr-TR/Governor/10.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_4",
          "DialogueText": "Bu... bu ne terbiyesizlik!Hayatımda hiç bu kadar aşağılanmamıştım!",
          "AudioPath": "assets/tr-TR/Governor/11.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_5",
          "DialogueText": "Öğk... dilim şişmeye başladı... Sanırım kusacağım.",
          "AudioPath": "assets/tr-TR/Governor/12.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_6",
          "DialogueText": "Hım... Kötü değil fakat bir şeyler eksik...Kasabadaki herkes çorbaya katkı sağladı mı? Birinin özel dokunuşunun eksikliği hissediliyor.",
          "AudioPath": "assets/tr-TR/Governor/13.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_7",
          "DialogueText": "Amanın... Bu tattığım en iyi çorba!",
          "AudioPath": "assets/tr-TR/Governor/14.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_8",
          "DialogueText": "Ah... Harika bir çorba olmuş. Bu vadiden insanın önüne hiç kötü bir şey gelmez!",
          "AudioPath": "assets/tr-TR/Governor/15.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_9",
          "DialogueText": "Hım... Söylenecek pek bir şey yok. Ortalama bir çorba işte.",
          "AudioPath": "assets/tr-TR/Governor/16.ogg"
        }
      ]
    }
  ]
}