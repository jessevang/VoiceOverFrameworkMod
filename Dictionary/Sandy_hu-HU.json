{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Sandy.hu-HU",
      "VoicePackName": "Dictionary (Sandy - hu-HU)",
      "Character": "Sandy",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "accept_866",
          "DialogueText": "Egy Stardew Valley rózsa? Ez olyan csodálatos! Köszönöm!",
          "AudioPath": "assets/hu-HU/Sandy/1.ogg"
        },
        {
          "DialogueFrom": "accept_866_1",
          "DialogueText": "Amikor hiányzik a völgy, akkor meg fogom szagolni ezt a rózsát. Hehe.",
          "AudioPath": "assets/hu-HU/Sandy/2.ogg"
        },
        {
          "DialogueFrom": "accept_866_2",
          "DialogueText": "Tessék, ez a tiéd. Több, mint 10 éve van a polcomon, és még senki se vette meg. A tiéd lehet!",
          "AudioPath": "assets/hu-HU/Sandy/3.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)18",
          "DialogueText": "Ó váó! Semmi sem fejezi ki jobban a 'tavaszt', mint a Stardew Valley-beli nárcisz!",
          "AudioPath": "assets/hu-HU/Sandy/4.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)402",
          "DialogueText": "*szimat*... Mmm... a szagos bükkönynek van a legcsodálatosabb illata.",
          "AudioPath": "assets/hu-HU/Sandy/5.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)418",
          "DialogueText": "Áhh... Stardew Valleyben még télen is szép virágok teremnek.",
          "AudioPath": "assets/hu-HU/Sandy/6.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Váó, ez annyira ritka... El se hiszem, hogy ezt nekem adod! Köszönöm, édesem!",
          "AudioPath": "assets/hu-HU/Sandy/7.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Nem lenne sok szerencséd, ha idekint gazdálkodnál, édes.",
          "AudioPath": "assets/hu-HU/Sandy/8.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Nos, hacsak nem kaktuszt termesztenél.",
          "AudioPath": "assets/hu-HU/Sandy/9.ogg"
        },
        {
          "DialogueFrom": "Fri4",
          "DialogueText": "Tudod, egyébként nem is Sandy a nevem. Csak jót tesz az üzletnek.",
          "AudioPath": "assets/hu-HU/Sandy/10.ogg"
        },
        {
          "DialogueFrom": "Fri4_1",
          "DialogueText": "De mivel mindenki Sandynek szólít, így már teljesen hozzászoktam.",
          "AudioPath": "assets/hu-HU/Sandy/11.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Úgy hallottam, hogy egy ritka időjárási jelenség van a völgyben.",
          "AudioPath": "assets/hu-HU/Sandy/12.ogg"
        },
        {
          "DialogueFrom": "GreenRain_1",
          "DialogueText": "Bárcsak néhanapján itt is esne...",
          "AudioPath": "assets/hu-HU/Sandy/13.ogg"
        },
        {
          "DialogueFrom": "married_Emily",
          "DialogueText": "Milyen volt a nagy nap? Emily nemrég itt járt és mesélte, hogy mik a terveitek.",
          "AudioPath": "assets/hu-HU/Sandy/14.ogg"
        },
        {
          "DialogueFrom": "married_Emily_1",
          "DialogueText": "Valamikor meg kellene látogatnom kettőtöket a farmon!",
          "AudioPath": "assets/hu-HU/Sandy/15.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Helló-belló, édeském!",
          "AudioPath": "assets/hu-HU/Sandy/16.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Annyira örülök, hogy ismét működik a busz. Már kezdtem nagyon magányos lenni idekint.",
          "AudioPath": "assets/hu-HU/Sandy/17.ogg"
        },
        {
          "DialogueFrom": "Mon_2",
          "DialogueText": "...Majdnem tönkrement az üzletem.",
          "AudioPath": "assets/hu-HU/Sandy/18.ogg"
        },
        {
          "DialogueFrom": "Mon_3",
          "DialogueText": "Hé, de most itt vagy te, hogy egy halom magvat vegyél... ugye, édeském?",
          "AudioPath": "assets/hu-HU/Sandy/19.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Üdv! Üdvözöllek téged Sandy Oázisában, a Calico-sivatag első számú úticéljában!",
          "AudioPath": "assets/hu-HU/Sandy/20.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "*suttogva* Úgy beszéltem mint egy profi?",
          "AudioPath": "assets/hu-HU/Sandy/21.ogg"
        },
        {
          "DialogueFrom": "Sat_2",
          "DialogueText": "Kérjük nézz rá a szezonális magválasztékunkra.",
          "AudioPath": "assets/hu-HU/Sandy/22.ogg"
        },
        {
          "DialogueFrom": "Sat_3",
          "DialogueText": "Nagyon szomjasnak tűnsz. Tessék, kóstold meg ezt a jéghideg tejet.",
          "AudioPath": "assets/hu-HU/Sandy/23.ogg"
        },
        {
          "DialogueFrom": "Sat_4",
          "DialogueText": "Ugye milyen finom?",
          "AudioPath": "assets/hu-HU/Sandy/24.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Itt mindig napos az idő, még télen is!",
          "AudioPath": "assets/hu-HU/Sandy/25.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Tulajdonképpen számomra túlságosan nagy a forróság... gyengéd bőröm gyorsan leég a napon.",
          "AudioPath": "assets/hu-HU/Sandy/26.ogg"
        },
        {
          "DialogueFrom": "Sun8",
          "DialogueText": "Üdv néked, édesem. Igen, csak én vagyok itt, egyedül ebben a nagy, öreg boltban. Hmmm...",
          "AudioPath": "assets/hu-HU/Sandy/27.ogg"
        },
        {
          "DialogueFrom": "Sun8_1",
          "DialogueText": "Nos, kivéve még azt az ijesztő fickót ott hátul. De ő egy tapodtat se mozdul, szóval elég sűrűn feledkezem meg róla.",
          "AudioPath": "assets/hu-HU/Sandy/28.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Az a sötét fickó ott hátul? Róla nem beszélhetek.",
          "AudioPath": "assets/hu-HU/Sandy/29.ogg"
        },
        {
          "DialogueFrom": "Thu_1",
          "DialogueText": "Egyébként... milyen az idő a völgyben?",
          "AudioPath": "assets/hu-HU/Sandy/30.ogg"
        },
        {
          "DialogueFrom": "Thu4",
          "DialogueText": "Hé... azt hiszem benned megbízhatok. Tudsz titkot tartani, ugye?",
          "AudioPath": "assets/hu-HU/Sandy/31.ogg"
        },
        {
          "DialogueFrom": "Thu4_1",
          "DialogueText": "*suttogva* Az a fickó ott hátul... az ott valamilyen exkluzív klub. Én még sose voltam odabent, de mivel elég szép bérleti díjat fizetnek havonta, így nem is kérdezősködöm.",
          "AudioPath": "assets/hu-HU/Sandy/32.ogg"
        },
        {
          "DialogueFrom": "Thu4_2",
          "DialogueText": "SZÓÓÓóóval! Milyen most az idő Stardew Valleyben?",
          "AudioPath": "assets/hu-HU/Sandy/33.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "Odakint kókuszdiót és kaktuszgyümölcsöket találhatsz. Nyugodtan vigyél el annyit amennyit csak akarsz.",
          "AudioPath": "assets/hu-HU/Sandy/34.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "Azok annnnnyira finomak. Heeee!~",
          "AudioPath": "assets/hu-HU/Sandy/35.ogg"
        },
        {
          "DialogueFrom": "Tue10",
          "DialogueText": "Szió!~ Maradj amíg csak kedved tartja.",
          "AudioPath": "assets/hu-HU/Sandy/36.ogg"
        },
        {
          "DialogueFrom": "Tue10_1",
          "DialogueText": "Egész jó idebent a légkondi, nem?",
          "AudioPath": "assets/hu-HU/Sandy/37.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "Nos, üdv néked! Ha látnád a városban a barátomat, Emilyt, köszönnél neki a nevemben?",
          "AudioPath": "assets/hu-HU/Sandy/38.ogg"
        },
        {
          "DialogueFrom": "Wed_1",
          "DialogueText": "Köszönöm!",
          "AudioPath": "assets/hu-HU/Sandy/39.ogg"
        },
        {
          "DialogueFrom": "Wed10",
          "DialogueText": "Hé, itt van ! Örülök, hogy látlak, kölyök. Már kezdtem azt hinni, hogy sose térsz vissza!",
          "AudioPath": "assets/hu-HU/Sandy/40.ogg"
        },
        {
          "DialogueFrom": "Wed10_1",
          "DialogueText": "Ha nem lenne itt ez a bolt, akkor visszamennék veled a völgybe egy-két nap erejéig... csak te és én, meg a régi jó farm.",
          "AudioPath": "assets/hu-HU/Sandy/41.ogg"
        },
        {
          "DialogueFrom": "Wed10_2",
          "DialogueText": "Gyakrabban kell meglátogatnod engem, hogy ne legyek depressziós.",
          "AudioPath": "assets/hu-HU/Sandy/42.ogg"
        },
        {
          "DialogueFrom": "Wed8",
          "DialogueText": "Nem túl drága eljönni a sivatagba ilyen gyakran, mint ahogyan te teszed?",
          "AudioPath": "assets/hu-HU/Sandy/43.ogg"
        },
        {
          "DialogueFrom": "Wed8_1",
          "DialogueText": "Biztosan szeretsz velem találkozni, mi? Hmmm... ||*sóhaj*... Nekem annyira hiányoznak Stardew Valley csodálatos virágai...",
          "AudioPath": "assets/hu-HU/Sandy/44.ogg"
        },
        {
          "DialogueFrom": "Event:SandyHouse/67/m 10",
          "DialogueText": "Egy... vendég?",
          "AudioPath": "assets/hu-HU/Sandy/45.ogg"
        },
        {
          "DialogueFrom": "Event:SandyHouse/67/m 10_1",
          "DialogueText": "Üdv! Üdvözöllek 'Sandy oázisában'!",
          "AudioPath": "assets/hu-HU/Sandy/46.ogg"
        },
        {
          "DialogueFrom": "Event:SandyHouse/67/m 10_1_1",
          "DialogueText": "Hé, te pont úgy nézel ki, mint az új gazda, akiről Emily írt nekem!",
          "AudioPath": "assets/hu-HU/Sandy/47.ogg"
        },
        {
          "DialogueFrom": "Event:SandyHouse/67/m 10_1_2",
          "DialogueText": "Akkor... ismét működik a buszjárat Stardew Valley felé!",
          "AudioPath": "assets/hu-HU/Sandy/48.ogg"
        },
        {
          "DialogueFrom": "Event:SandyHouse/67/m 10_1_3",
          "DialogueText": "Ó, olyan boldog vagyok!",
          "AudioPath": "assets/hu-HU/Sandy/49.ogg"
        },
        {
          "DialogueFrom": "Event:SandyHouse/67/m 10_implied",
          "DialogueText": "Kérlek jöjj máskor is, nagyon hamar elunom magam idekint.",
          "AudioPath": "assets/hu-HU/Sandy/50.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue",
          "DialogueText": "Mi történt? Te klubtag vagy?!",
          "AudioPath": "assets/hu-HU/Sandy/51.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Úgy hallottam, hogy odahaza esik. Ezért jöttél ide?Nekem valójában kissé hiányzik az eső...",
          "AudioPath": "assets/hu-HU/Sandy/52.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Stardew Valley közössége tényleg egyedülálló...",
          "AudioPath": "assets/hu-HU/Sandy/53.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Úgy hallottam, hogy odahaza esik. Ezért jöttél ide?Valójában nekem egy kicsit hiányzik az eső...",
          "AudioPath": "assets/hu-HU/Sandy/54.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "Óóó! Szeretem a völgyben termő virágokat. Olyan boldog vagyok!~",
          "AudioPath": "assets/hu-HU/Sandy/55.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Ez csodálatosan néz ki. Nagyon szépen köszönöm!",
          "AudioPath": "assets/hu-HU/Sandy/56.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Ó... Őőő. Köszönöm.",
          "AudioPath": "assets/hu-HU/Sandy/57.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "*szimat* ...ez meg micsoda?",
          "AudioPath": "assets/hu-HU/Sandy/58.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Ez az enyém? Köszönöm!",
          "AudioPath": "assets/hu-HU/Sandy/59.ogg"
        }
      ]
    }
  ]
}