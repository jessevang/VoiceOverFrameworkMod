      "Entries": [
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900",
          "DialogueText": "Ese olor... Acércate un poco...",
          "AudioPath": "assets/es-ES/Bear/1.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_1",
          "DialogueText": "Je... No todos los días tengo ocasión de hablar con humanos. Parece que tienes la magia del bosque...",
          "AudioPath": "assets/es-ES/Bear/2.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_2",
          "DialogueText": "Bueno... hablemos de mi nota... ¿Has traído mercancía de calidad?",
          "AudioPath": "assets/es-ES/Bear/3.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_3",
          "DialogueText": "Uooooo... Ese olorcillo... ¡es la dulzura personificada!",
          "AudioPath": "assets/es-ES/Bear/4.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_3_1",
          "DialogueText": "Es mi salsa favorita... Me haces tan feliz...",
          "AudioPath": "assets/es-ES/Bear/5.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_4",
          "DialogueText": "Hoy has hecho un nuevo amigo, .",
          "AudioPath": "assets/es-ES/Bear/6.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_5",
          "DialogueText": "Tengo algo especial que quiero enseñarte...",
          "AudioPath": "assets/es-ES/Bear/7.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_alt",
          "DialogueText": "El oso se va dejándote su sabiduría especial...",
          "AudioPath": "assets/es-ES/Bear/8.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_alt_1",
          "DialogueText": "¡Ahora, las frambuesas y las moras valdrán el triple de oro!",
          "AudioPath": "assets/es-ES/Bear/9.ogg"
        }
      ]
    }
  ]
