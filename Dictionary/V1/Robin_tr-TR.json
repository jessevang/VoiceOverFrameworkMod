        },
        {
          "DialogueFrom": "Event:ScienceHouse/33/f Robin 1500/p Robin_alt",
          "DialogueText": "'Davul Düğmesi ve Kaval Düğmesi' tarifleri alındı.",
          "AudioPath": "assets/tr-TR/Robin/130.ogg"
