        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Merhaba . Takılmak istersen ailemizin masasına buyur.",
          "AudioPath": "assets/tr-TR/Robin/184.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "Eh... Eşim bu sene bana bir elektrikli testere aldı...İki sene önce de aynı hediyeyi aldığını unutmuş olmalı!",
          "AudioPath": "assets/tr-TR/Robin/185.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Üzgünüm, buzdan korkuluk yapmak için dikkatimi ayırmamalıyım.",
          "AudioPath": "assets/tr-TR/Robin/186.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "Buzdan heykeller yapmak, benim için güzel bir uğraş. Bu malzemeye alışkın değilim!",
          "AudioPath": "assets/tr-TR/Robin/187.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "Bu benim için mi? Vay canına, buna bayıldım!",
          "AudioPath": "assets/tr-TR/Robin/188.ogg"
        },
