        {
          "DialogueFrom": "Event:SebastianRoom/enterRobin",
          "DialogueText": "Ah, merhaba .",
          "AudioPath": "assets/tr-TR/Robin/131.ogg"
        },
        {
          "DialogueFrom": "Event:SebastianRoom/enterRobin_1",
          "DialogueText": "Sebby, buraya gelmemi savmediğini biliyorum... ama mağazada Abigail'e rastladım ve seni aradığını söyledi.",
          "AudioPath": "assets/tr-TR/Robin/132.ogg"
        },
        {
          "DialogueFrom": "Event:SebastianRoom/enterRobin_2",
          "DialogueText": "Söyledim... ama yine de uğrayacakmış.",
          "AudioPath": "assets/tr-TR/Robin/133.ogg"
        },
        {
          "DialogueFrom": "Event:Town/15389722/j 57",
          "DialogueText": "Şey... Lewis? Sen bir tek çivileri tuttun.",
          "AudioPath": "assets/tr-TR/Robin/134.ogg"
        },
        {
          "DialogueFrom": "Event:Town/15389722/j 57_1",
          "DialogueText": "Örneğin ben yapmak istediğim bir yatak için büyük çaplı bir kereste ilanı vermek istiyorum.",
