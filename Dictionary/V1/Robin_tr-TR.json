{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Robin.tr-TR",
      "VoicePackName": "Dictionary (Robin - tr-TR)",
      "Character": "Robin",
      "Language": "tr-TR",
      "Entries": [
        {
          "DialogueFrom": "AcceptGift_(O)Book_Woodcutting",
          "DialogueText": "Bu da ne... 'Ağaçkakanların Sırrı' mı?.. Ne tür bir kitap ki bu ?",
          "AudioPath": "assets/tr-TR/Robin/1.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)Book_Woodcutting_1",
          "DialogueText": "Ah, odunculukla ilgiliymiş! Hahaha... Evet, bunu seveceğim. Teşekkürler!",
          "AudioPath": "assets/tr-TR/Robin/2.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)SkillBook_2",
          "DialogueText": "Ah, 'Oduncunun Haftalığı'... Yeni sayısını almayı düşünüyordum! Burada en yeni teknikler var!",
          "AudioPath": "assets/tr-TR/Robin/3.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)SkillBook_2_1",
          "DialogueText": "Bir oduncudan diğerine... Teşekkürler.",
          "AudioPath": "assets/tr-TR/Robin/4.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Bir saniye... Bunu bana mı veriyorsun? Ne kadar özel bir sürpriz! Teşekkürler.",
          "AudioPath": "assets/tr-TR/Robin/5.ogg"
        },
        {
          "DialogueFrom": "cc_Begin",
          "DialogueText": "Hey! Lewis'in sana Halkevi'ni gösterdiğini duydum, umarım orayı onarabilirsin. Eskiden çok güzel bir yerdi.",
          "AudioPath": "assets/tr-TR/Robin/6.ogg"
        },
        {
          "DialogueFrom": "cc_Bridge",
          "DialogueText": "Hey! Dün akşam garip garip sesler duydum. Sabah kalktığımda taş ocağına giden köprü tamamen onarılmıştı!",
          "AudioPath": "assets/tr-TR/Robin/7.ogg"
        },
        {
          "DialogueFrom": "cc_Bridge_1",
          "DialogueText": "Köprü yeni haliyle harika duruyor!",
          "AudioPath": "assets/tr-TR/Robin/8.ogg"
        },
        {
          "DialogueFrom": "cc_Complete",
          "DialogueText": "O otobüsten indiğin anda kasabamızın kahramanı olacağını biliyordum, ...",
          "AudioPath": "assets/tr-TR/Robin/9.ogg"
        },
        {
          "DialogueFrom": "divorced_Maru",
          "DialogueText": "Maru tüm gece boyunca ağlıyordu... Hepimiz yıkılmış durumdayız...",
          "AudioPath": "assets/tr-TR/Robin/10.ogg"
        },
        {
          "DialogueFrom": "divorced_Sebastian",
          "DialogueText": "Sebastian şu anda bodrumda... Aranızda neler oldu?",
          "AudioPath": "assets/tr-TR/Robin/11.ogg"
        },
        {
          "DialogueFrom": "event_wood1",
          "DialogueText": "Harika, bu taslağı tamamlamak senin için çocuk oyuncağı olmalı!",
          "AudioPath": "assets/tr-TR/Robin/12.ogg"
        },
        {
          "DialogueFrom": "event_wood2",
          "DialogueText": "Başlaman için gereken taslak bende var, muhteşem!",
          "AudioPath": "assets/tr-TR/Robin/13.ogg"
        },
        {
          "DialogueFrom": "fall_Fri",
          "DialogueText": "Evimizin arkasındaki yabani adam ile tanıştın mı?",
          "AudioPath": "assets/tr-TR/Robin/14.ogg"
        },
        {
          "DialogueFrom": "fall_Fri_1",
          "DialogueText": "Sanırım bizi rahatsız etmediği sürece umurumda olmayacak.",
          "AudioPath": "assets/tr-TR/Robin/15.ogg"
        },
        {
          "DialogueFrom": "fall_Mon",
          "DialogueText": "Maru mücevherleri seviyor, onları deneylerinde kullanıyor.",
          "AudioPath": "assets/tr-TR/Robin/16.ogg"
        },
        {
          "DialogueFrom": "fall_Mon_1",
          "DialogueText": "Maru ile anlaşabiliyor musun?",
          "AudioPath": "assets/tr-TR/Robin/17.ogg"
        },
        {
          "DialogueFrom": "fall_Thu",
          "DialogueText": "Binaları geliştirmek için gelmediysen bile evimize hoş geldin!",
          "AudioPath": "assets/tr-TR/Robin/18.ogg"
        },
        {
          "DialogueFrom": "fall_Thu_1",
          "DialogueText": "Dağlarda insan yalnız hissedebiliyor.",
          "AudioPath": "assets/tr-TR/Robin/19.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Bodrumda yaşayan oğlum Sebastian ile tanıştın değil mi?",
          "AudioPath": "assets/tr-TR/Robin/20.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Biraz utangaçtır ama ona iyi davranırsan sana alışacağına eminim.",
          "AudioPath": "assets/tr-TR/Robin/21.ogg"
        },
        {
          "DialogueFrom": "Fri_inlaw_Sebastian",
          "DialogueText": "Küçük Sebby'mi özledim... hep yanlış anlaşılıyor, ama ben ona güveniyordum.",
          "AudioPath": "assets/tr-TR/Robin/22.ogg"
        },
        {
          "DialogueFrom": "Fri4",
          "DialogueText": "Sebastian'ın odasına çok garip kokan bir kül tablası buldum. Bu beni endişelendirmeli mi?",
          "AudioPath": "assets/tr-TR/Robin/23.ogg"
        },
        {
          "DialogueFrom": "Fri4_1",
          "DialogueText": "Bazen Sebastian için endişeleniyorum...Pek arkadaşı yok ve geleceğini hiç düşünmüyor...",
          "AudioPath": "assets/tr-TR/Robin/24.ogg"
        },
        {
          "DialogueFrom": "Fri4_2",
          "DialogueText": "Onunla konuşurdum ama bana içini hiç dökmez.",
          "AudioPath": "assets/tr-TR/Robin/25.ogg"
        },
        {
          "DialogueFrom": "Fri4_inlaw_Sebastian",
          "DialogueText": "Sebastian bana sigarayı bırakmaya çalıştığını söyledi. Onunla gurur duyuyorum.",
          "AudioPath": "assets/tr-TR/Robin/26.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Deli adam! Neden dışarı çıkıyor ki? Bu yağmurun tehlikeli olup olmadığını bilmiyoruz!",
          "AudioPath": "assets/tr-TR/Robin/27.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "Bugün dışarıda her çeşit kaliteli odun var. Kesmeye başlamak lazım, ne dersin?",
          "AudioPath": "assets/tr-TR/Robin/28.ogg"
        },
        {
          "DialogueFrom": "GreenRainFinished",
          "DialogueText": "Umarım Demetrius, topladığı 'örneklerden' memnundur. Küçük bilim projesini kendi karısından daha önemli görüyor. Of.",
          "AudioPath": "assets/tr-TR/Robin/29.ogg"
        },
        {
          "DialogueFrom": "houseUpgrade_1",
          "DialogueText": "Evinde çalışmak eğlenceliydi. Bu, şu ana kadar yaptığım en iyi işçilik olabilir!",
          "AudioPath": "assets/tr-TR/Robin/30.ogg"
        },
        {
          "DialogueFrom": "houseUpgrade_1_1",
          "DialogueText": "Umarım yeni mutfağının tadını çıkarırsın.",
          "AudioPath": "assets/tr-TR/Robin/31.ogg"
        },
        {
          "DialogueFrom": "houseUpgrade_2",
          "DialogueText": "Evinde açtığım tüm bu boş alanla ne yapacaksın?",
          "AudioPath": "assets/tr-TR/Robin/32.ogg"
        },
        {
          "DialogueFrom": "houseUpgrade_2_1",
          "DialogueText": "Gerçi burası ev değil, malikane sayılır.",
          "AudioPath": "assets/tr-TR/Robin/33.ogg"
        },
        {
          "DialogueFrom": "houseUpgrade_3",
          "DialogueText": "Yıllandırılmış şarap işine mi giriyorsun? Kazançlı olduğunu duydum... Tabii sabredebilirsen.",
          "AudioPath": "assets/tr-TR/Robin/34.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "Kasabadaki herkes ile tanıştın mı? Yorulmuş olmalısın.",
          "AudioPath": "assets/tr-TR/Robin/35.ogg"
        },
        {
          "DialogueFrom": "married_Maru",
          "DialogueText": "Evde Maru'yu arayacağız. Eminim sık sık görüşeceğiz!",
          "AudioPath": "assets/tr-TR/Robin/36.ogg"
        },
        {
          "DialogueFrom": "married_Sebastian",
          "DialogueText": "Ah, merak etme... Bunlar mutluluk gözyaşları. Eminim ki Sebastian, Çiftliği'nde mutlu olacak!",
          "AudioPath": "assets/tr-TR/Robin/37.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Evimizi baştan aşağı ben inşa ettim, biliyor musun?",
          "AudioPath": "assets/tr-TR/Robin/38.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Kesinlikle kariyerimin dönüm noktası o andı.",
          "AudioPath": "assets/tr-TR/Robin/39.ogg"
        },
        {
          "DialogueFrom": "MovieInvitation",
          "DialogueText": "Vay... Bugünkü filme iki bilet demek? Olur! Orada buluşuruz.",
          "AudioPath": "assets/tr-TR/Robin/40.ogg"
        },
        {
          "DialogueFrom": "pamHouseUpgrade",
          "DialogueText": "Küçük planımız harika sonuçlandı, haksız mıyım? Pam ve Penny çok mutlular.",
          "AudioPath": "assets/tr-TR/Robin/41.ogg"
        },
        {
          "DialogueFrom": "reject_865",
          "DialogueText": "Vejeteryan tuzu mu? İyiymiş ama istemiyorum.",
          "AudioPath": "assets/tr-TR/Robin/42.ogg"
        },
        {
          "DialogueFrom": "reject_866",
          "DialogueText": "Hım... Ne zaman çiçek görsem aklıma Evelyn geliyor. Bunu ona göstermeli miyim dersin?",
          "AudioPath": "assets/tr-TR/Robin/43.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Vay canına, şu yapılara baksana!",
          "AudioPath": "assets/tr-TR/Robin/44.ogg"
        },
        {
          "DialogueFrom": "Resort_1",
          "DialogueText": "İşinde usta biri tarafından yapıldıkları çok belli. Epey etkilendim!",
          "AudioPath": "assets/tr-TR/Robin/45.ogg"
        },
        {
          "DialogueFrom": "Resort_Bar",
          "DialogueText": "Ne kadar da dinlendirici...",
          "AudioPath": "assets/tr-TR/Robin/46.ogg"
        },
        {
          "DialogueFrom": "Resort_Chair",
          "DialogueText": "Selam . Düşünüyordum da, sen geldiğinden beri kasabamız çok ama çok büyüdü, ilerledi, gelişti.",
          "AudioPath": "assets/tr-TR/Robin/47.ogg"
        },
        {
          "DialogueFrom": "Resort_Chair_1",
          "DialogueText": "Yaptığın şeylerle gurur duymalısın!",
          "AudioPath": "assets/tr-TR/Robin/48.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Çok eğlendim!",
          "AudioPath": "assets/tr-TR/Robin/49.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Baksana! Şurada bir ada daha var.",
          "AudioPath": "assets/tr-TR/Robin/50.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Evim çok güzel bir yerde, sence de öyle değil mi?",
          "AudioPath": "assets/tr-TR/Robin/51.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Dağlardaki ferahlatıcı havayı seviyorum.",
          "AudioPath": "assets/tr-TR/Robin/52.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Barn_memory_oneweek",
          "DialogueText": "Yeni ahırından mutlu musun? Orada çiftlik hayvanlarını harika bir şekilde yetiştireceğine eminim!",
          "AudioPath": "assets/tr-TR/Robin/53.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Coop",
          "DialogueText": "Muhtemelen bu inşa ettiğim en sağlam kümes oldu. Umarım sana faydası dokunur!",
          "AudioPath": "assets/tr-TR/Robin/54.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Coop_memory_oneweek",
          "DialogueText": "Kümes işi nasıl gidiyor? Yeni hayvanlarınla tanışmak için sabırsızlanıyorum!",
          "AudioPath": "assets/tr-TR/Robin/55.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Fish Pond",
          "DialogueText": "Willy'ye yeni bir balık havuzun olduğundan bahsettim. Çok hoşuna gitti.",
          "AudioPath": "assets/tr-TR/Robin/56.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Fish Pond_1",
          "DialogueText": "Anlaşılan gençken yengeç yetiştirmiş. Buna 'zamanın onurlandırdığı gelenek' diyormuş.",
          "AudioPath": "assets/tr-TR/Robin/57.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Shed",
          "DialogueText": "Yeni barakanla ne yapacaksın? İhtimaller resmen sınırsız!",
          "AudioPath": "assets/tr-TR/Robin/58.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Slime Hutch",
          "DialogueText": "*öfler*... Şu Balçıkça Kümesini inşa etmek epey uğraştırdı... Toparlanmak için bir günümü hamamda geçirmem gerek!",
          "AudioPath": "assets/tr-TR/Robin/59.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Stable_memory_oneweek",
          "DialogueText": "Yeni atınla tüm kasabayı mı turladın?",
          "AudioPath": "assets/tr-TR/Robin/60.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Stable_memory_oneweek_1",
          "DialogueText": "Muhtemelen şu an ağrıların vardır ama zamanla alışacaksın, merak etme.",
          "AudioPath": "assets/tr-TR/Robin/61.ogg"
        },
        {
          "DialogueFrom": "summer_Fri",
          "DialogueText": "Yıldızçiyi Vadisi'nde dış dünyadan izole durumdayız.",
          "AudioPath": "assets/tr-TR/Robin/62.ogg"
        },
        {
          "DialogueFrom": "summer_Fri_1",
          "DialogueText": "Bunun iyi ve kötü yanları var.",
          "AudioPath": "assets/tr-TR/Robin/63.ogg"
        },
        {
          "DialogueFrom": "summer_Mon",
          "DialogueText": "Odun harika bir materyal... Ucuz, sert, birçok alanda kullanılabiliyor. Üstelik, her odun parçasının kendine özel bir karakteri var!",
          "AudioPath": "assets/tr-TR/Robin/64.ogg"
        },
        {
          "DialogueFrom": "summer_Tue",
          "DialogueText": "Çiftliğindeki herhangi bir binayı geliştirmen gerekirse bana uğra!",
          "AudioPath": "assets/tr-TR/Robin/65.ogg"
        },
        {
          "DialogueFrom": "summer_Tue_1",
          "DialogueText": "İhtiyacın olan taş ve odunu temin ederim. Parası karşılığında olmak üzere.",
          "AudioPath": "assets/tr-TR/Robin/66.ogg"
        },
        {
          "DialogueFrom": "summer_Tue_2",
          "DialogueText": "Ama bittiğinde memnun kalacağından emin olabilirsin!",
          "AudioPath": "assets/tr-TR/Robin/67.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Umarım Demetrius deneyleri ile evi patlatmaz.",
          "AudioPath": "assets/tr-TR/Robin/68.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Tam olarak ne üzerinde çalıştığını bilmiyorum ama bitkilerle ilgiliydi.",
          "AudioPath": "assets/tr-TR/Robin/69.ogg"
        },
        {
          "DialogueFrom": "Sun2",
          "DialogueText": "Kocam dün geceki deneyi ile neredeyse evi yakıyordu. Deney tüplerinden birisi patladı ve kirişlerden birini ateşe verdi! Yoba'ya şükürler olsun ki evi yaparken ateşe dayanıklı malzeme kullanmıştım.",
          "AudioPath": "assets/tr-TR/Robin/70.ogg"
        },
        {
          "DialogueFrom": "Sun2_1",
          "DialogueText": "Kötü kokunun kusuruna bakma, . Kocamın bilim projelerinden biri işte...",
          "AudioPath": "assets/tr-TR/Robin/71.ogg"
        },
        {
          "DialogueFrom": "Sun6",
          "DialogueText": "Selam, ! Seni görmek güzel.",
          "AudioPath": "assets/tr-TR/Robin/72.ogg"
        },
        {
          "DialogueFrom": "Sun6_1",
          "DialogueText": "Bu topluluğun bir parçası oldun artık. Sen ne düşünüyorsun?",
          "AudioPath": "assets/tr-TR/Robin/73.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Alışveriş yapmayacaksan bile bizi istediğin zaman ziyaret edebilirsin.",
          "AudioPath": "assets/tr-TR/Robin/74.ogg"
        },
        {
          "DialogueFrom": "Thu_1",
          "DialogueText": "Bu dağlarda yalnız kalabiliyorum.",
          "AudioPath": "assets/tr-TR/Robin/75.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "Eğer taslak veya malzeme satın almak istiyorsan mağazam tam sana göre!",
          "AudioPath": "assets/tr-TR/Robin/76.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "Ayrıca, satın alacağın her bir şey ile kasaba ekonomisine katkıda bulunmuş olursun!",
          "AudioPath": "assets/tr-TR/Robin/77.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "Selam, .",
          "AudioPath": "assets/tr-TR/Robin/78.ogg"
        },
        {
          "DialogueFrom": "Wed_1",
          "DialogueText": "Az önce birkaç marangozluk projesi ile ilgili hayaller kuruyordum.",
          "AudioPath": "assets/tr-TR/Robin/79.ogg"
        },
        {
          "DialogueFrom": "Wed_2",
          "DialogueText": "Evin nasıl? Barakandaki tahtalar sağlam mı?",
          "AudioPath": "assets/tr-TR/Robin/80.ogg"
        },
        {
          "DialogueFrom": "winter_Fri",
          "DialogueText": "Annemle babama marangoz olacağım dediğimde kafayı yemişlerdi.",
          "AudioPath": "assets/tr-TR/Robin/81.ogg"
        },
        {
          "DialogueFrom": "winter_Fri_1",
          "DialogueText": "Biraz eski topraklardır da...",
          "AudioPath": "assets/tr-TR/Robin/82.ogg"
        },
        {
          "DialogueFrom": "winter_Fri_inlaw_Sebastian",
          "DialogueText": "Çiftlik hayatı Sebby'e çok iyi geliyor.",
          "AudioPath": "assets/tr-TR/Robin/83.ogg"
        },
        {
          "DialogueFrom": "winter_Mon",
          "DialogueText": "Hey, şu sıralar odun toplamak için en iyi zamanlardır.",
          "AudioPath": "assets/tr-TR/Robin/84.ogg"
        },
        {
          "DialogueFrom": "winter_Mon_1",
          "DialogueText": "Belki sen de evini geliştirebilecek kadar odun toplayabilirsin!",
          "AudioPath": "assets/tr-TR/Robin/85.ogg"
        },
        {
          "DialogueFrom": "winter_Sat",
          "DialogueText": "Evimizin yeri çok iyi değil mi?",
          "AudioPath": "assets/tr-TR/Robin/86.ogg"
        },
        {
          "DialogueFrom": "winter_Sat_1",
          "DialogueText": "Kardan sonra her şey çok dingin duruyor.",
          "AudioPath": "assets/tr-TR/Robin/87.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0",
          "DialogueText": "Merhaba! Sen olmalısın.",
          "AudioPath": "assets/tr-TR/Robin/88.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0_1",
          "DialogueText": "Ben Robin, bölgenin marangozuyum. Başkan Lewis seni buraya getirmem ve yeni evini göstermem için beni gönderdi. O da çiftlikte, gelişin için hazırlık yapıyor.",
          "AudioPath": "assets/tr-TR/Robin/89.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0_2",
          "DialogueText": "Çiftlik hemen şurada. Beni izle.",
          "AudioPath": "assets/tr-TR/Robin/90.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0_1",
          "DialogueText": "Burası çiftliği.",
          "AudioPath": "assets/tr-TR/Robin/91.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0_2",
          "DialogueText": "Sorun ne?",
          "AudioPath": "assets/tr-TR/Robin/92.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0_2_1",
          "DialogueText": "Biraz büyümüş, ama bu çer çöpün altında verimli topraklar yatıyor!",
          "AudioPath": "assets/tr-TR/Robin/93.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0_2_2",
          "DialogueText": "Biraz özveri ile hemencecik toparlarsın.",
          "AudioPath": "assets/tr-TR/Robin/94.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0_3",
          "DialogueText": "...Burası da yeni evin.",
          "AudioPath": "assets/tr-TR/Robin/95.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0_4",
          "DialogueText": "Hoyrat mı? Yani...",
          "AudioPath": "assets/tr-TR/Robin/96.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0_4_1",
          "DialogueText": "'Haşin' demek daha uygun olabilir doğrusu.",
          "AudioPath": "assets/tr-TR/Robin/97.ogg"
        },
        {
          "DialogueFrom": "Event:BusStop/60367/u 0_alt",
          "DialogueText": "Hım.",
          "AudioPath": "assets/tr-TR/Robin/98.ogg"
        },
        {
          "DialogueFrom": "Event:ManorHouse/2123243/e 2111194_alt",
          "DialogueText": "Vay be...",
          "AudioPath": "assets/tr-TR/Robin/99.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26",
          "DialogueText": "Ah... Linus idi, değil mi?",
          "AudioPath": "assets/tr-TR/Robin/100.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_1",
          "DialogueText": "Aç mısın? Sana öğle yemeği yapabilirim... Dünden kalan mağara havucu güvecine ne dersin?",
