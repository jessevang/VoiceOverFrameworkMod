        {
          "DialogueFrom": "Event:Town/53/e 55/t 1500 1700_alt",
          "DialogueText": "Ahşabı harika kullanmışsın!",
          "AudioPath": "assets/tr-TR/Robin/140.ogg"
        },
        {
          "DialogueFrom": "Event:Town/611173/Hn pamHouseUpgrade",
          "DialogueText": "Merhaba, ! Evet... ne düşünüyorsun?",
