        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_implied",
          "DialogueText": "Linus'u benimle birlikte çiftlikte yaşamaya davet etmek istiyorum...",
          "AudioPath": "assets/tr-TR/Robin/110.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/linusWell",
          "DialogueText": "Ah... Ne kadar da güzel...",
          "AudioPath": "assets/tr-TR/Robin/111.ogg"
        },
        {
          "DialogueFrom": "Event:Saloon/3206194/n gusGiantOmelet",
          "DialogueText": "Bu yumurtalar öyle dolu ve sarı ki... Marnie, bunlar senin tavuklarından mı?",
          "AudioPath": "assets/tr-TR/Robin/112.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/1053978/n robinDeluxeBed",
          "DialogueText": "Buyur bakalım! En yeni yapıtım.",
          "AudioPath": "assets/tr-TR/Robin/113.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/1053978/n robinDeluxeBed_1",
          "DialogueText": "Bu sayvanlı yatağı vadimizin en iyi kerestelerinden yaptım.",
          "AudioPath": "assets/tr-TR/Robin/114.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/1053978/n robinDeluxeBed_1",
          "DialogueText": "Biz buna 'güzellik' diyoruz... İnsanların hoşuna gidiyor yani, beğeniyorlar.",
          "AudioPath": "assets/tr-TR/Robin/115.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/1053978/n robinDeluxeBed_2",
          "DialogueText": "Biliyor musun, bazen sırf beni gıcık etmek için yatakta geniş geniş yayıldığını düşünüyorum. Niyetin bu mu?",
          "AudioPath": "assets/tr-TR/Robin/116.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/1053978/n robinDeluxeBed_3",
          "DialogueText": "Merhaba ... Gelsene.",
          "AudioPath": "assets/tr-TR/Robin/117.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/1053978/n robinDeluxeBed_4",
          "DialogueText": "Şuna bir bak... Topladığın malzemelerle bu yatağı yaptım.",
          "AudioPath": "assets/tr-TR/Robin/118.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/1053978/n robinDeluxeBed_5",
          "DialogueText": "Ne düşünüyorsun?",
          "AudioPath": "assets/tr-TR/Robin/119.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/1053978/n robinDeluxeBed_6",
          "DialogueText": "Artık elimde bir prototip olduğundan dükkanda satmaya başlayabilirim.",
          "AudioPath": "assets/tr-TR/Robin/120.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/1053978/n robinDeluxeBed_7",
          "DialogueText": "... bundan bir tane senin evinde güzel durabilir. Ne dersin?.. Bir tane de sana yapayım mı?",
          "AudioPath": "assets/tr-TR/Robin/121.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/25/f Demetrius 1500/p Demetrius/p Robin",
          "DialogueText": "Demetrius, sana domates al demedim. Meyve al dedim.",
          "AudioPath": "assets/tr-TR/Robin/122.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/25/f Demetrius 1500/p Demetrius/p Robin_1",
          "DialogueText": "Normal bir insan meyve istediğini söylediğinde, asla domates istemez. Bilimsel sınıflandırmada ne yazdığını umursamaz.",
          "AudioPath": "assets/tr-TR/Robin/123.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/33/f Robin 1500/p Robin",
          "DialogueText": "Merhaba, . Testeremdeki tozları temizliyordum.",
          "AudioPath": "assets/tr-TR/Robin/124.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/33/f Robin 1500/p Robin_1",
          "DialogueText": "66 null 66 50 event_wood1 66 50 event_wood2#Hayır.",
          "AudioPath": "assets/tr-TR/Robin/125.ogg"
        },
        {
          "DialogueFrom": "Event:ScienceHouse/33/f Robin 1500/p Robin_2",
