{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Grandpa.ja-JP",
      "VoicePackName": "Dictionary (Grandpa - ja-JP)",
      "Character": "Grandpa",
      "Language": "ja-JP",
      "Entries": [
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H",
          "DialogueText": "おじいちゃんのかわいい孫よ…",
          "AudioPath": "assets/ja-JP/Grandpa/1_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "おじいちゃんのかわいい孫娘よ…",
          "AudioPath": "assets/ja-JP/Grandpa/2_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "さいごに話したときからずいぶん時がたったね。お前はまだほんの子供だった…覚えているかな？",
          "AudioPath": "assets/ja-JP/Grandpa/3.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_3",
          "DialogueText": "時がすぎるのは早いものだ！",
          "AudioPath": "assets/ja-JP/Grandpa/4.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_4",
          "DialogueText": "お前は忘れてしまっていたかもしれないが、おじいちゃんはずっとここにいたんだよ。",
          "AudioPath": "assets/ja-JP/Grandpa/5.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_5",
          "DialogueText": "ごらん…おじいちゃんの体はもうこの世にないけれど、心はいつもスターデューバレーにのこっていたんだよ。",
          "AudioPath": "assets/ja-JP/Grandpa/6.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "お前がこの谷で暮らしだしてから、もう2年になるね…",
          "AudioPath": "assets/ja-JP/Grandpa/7.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "牧場をどうしていくかは、あとはお前の手で決めなさい。",
          "AudioPath": "assets/ja-JP/Grandpa/8.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2_1",
          "DialogueText": "それでは、お別れだ！",
          "AudioPath": "assets/ja-JP/Grandpa/9.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H",
          "DialogueText": "おじいちゃんを呼んでくれてうれしいよ。まえに会ったときは、すこしきつく当たりすぎてしまったかもしれないね。",
          "AudioPath": "assets/ja-JP/Grandpa/10.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "ゆるしておくれ。お前はいっしょうけんめい働いてきたようだね。おじいちゃんは誇らしいよ。",
          "AudioPath": "assets/ja-JP/Grandpa/11.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "どれどれ…",
          "AudioPath": "assets/ja-JP/Grandpa/12.ogg"
        }
      ]
    }
  ]
}