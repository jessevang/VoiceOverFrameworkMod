      "Entries": [
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900",
          "DialogueText": "Bu koku... yaklaş...",
          "AudioPath": "assets/tr-TR/Bear/1.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_1",
          "DialogueText": "Heh... Her gün insanlarla konuşmak nasip olmuyor. Görünüşe göre sende biraz 'Orman Büyüsü' var...",
          "AudioPath": "assets/tr-TR/Bear/2.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_2",
          "DialogueText": "Şimdi... notumla alakalı... Dediğim şeyi getirdin mi?",
          "AudioPath": "assets/tr-TR/Bear/3.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_3",
          "DialogueText": "HOoOoOoooh... Bu koku... bunlar gerçekten harika şeyler!",
          "AudioPath": "assets/tr-TR/Bear/4.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_3_1",
          "DialogueText": "...En sevdiğim sos... bu beni çok sevindirdi.",
          "AudioPath": "assets/tr-TR/Bear/5.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_4",
          "DialogueText": "...Bugün kendine bir arkadaş edindin, .",
          "AudioPath": "assets/tr-TR/Bear/6.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_5",
          "DialogueText": "Sana göstermek istediğim özel bir şey var...",
          "AudioPath": "assets/tr-TR/Bear/7.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_alt",
          "DialogueText": "Ayı sana özel bilgisini bıraktı ve gitti...",
          "AudioPath": "assets/tr-TR/Bear/8.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_alt_1",
          "DialogueText": "Böğürtlen ve ahududular artık 3 katı fazla altın değerinde!",
          "AudioPath": "assets/tr-TR/Bear/9.ogg"
        }
      ]
