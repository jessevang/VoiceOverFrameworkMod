{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Kent.hu-HU",
      "VoicePackName": "Dictionary (Kent - hu-HU)",
      "Character": "Kent",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "accept_864",
          "DialogueText": "...!",
          "AudioPath": "assets/hu-HU/Kent/1.ogg"
        },
        {
          "DialogueFrom": "accept_864_1",
          "DialogueText": "...Ezt mégis hol találtad?",
          "AudioPath": "assets/hu-HU/Kent/2.ogg"
        },
        {
          "DialogueFrom": "accept_864_2",
          "DialogueText": "...",
          "AudioPath": "assets/hu-HU/Kent/3.ogg"
        },
        {
          "DialogueFrom": "accept_864_3",
          "DialogueText": "Ez a férfi... Együtt harcoltunk a háborúban, . Mintha csak a testvérem lett volna.",
          "AudioPath": "assets/hu-HU/Kent/4.ogg"
        },
        {
          "DialogueFrom": "accept_864_4",
          "DialogueText": "De... csak az egyikünk jutott vissza.",
          "AudioPath": "assets/hu-HU/Kent/5.ogg"
        },
        {
          "DialogueFrom": "accept_864_5",
          "DialogueText": "Soha nem vesztette el a bátorságát. Még a halál torkában sem. Egy igazi hős... sose fogom elfelejteni őt.",
          "AudioPath": "assets/hu-HU/Kent/6.ogg"
        },
        {
          "DialogueFrom": "accept_864_6",
          "DialogueText": "Köszönöm, hogy ezt elhoztad nekem... Sokat jelent számomra.",
          "AudioPath": "assets/hu-HU/Kent/7.ogg"
        },
        {
          "DialogueFrom": "accept_864_7",
          "DialogueText": "Tessék, ez a tiéd. Évek óta az egyik fiókunk legmélyén őriztük, de sose használtuk.",
          "AudioPath": "assets/hu-HU/Kent/8.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)Book_Bombs",
          "DialogueText": "Nos, én nem tudok Törpéül olvasni, de attól még értékes gyűjtemény lehet. Köszönöm.",
          "AudioPath": "assets/hu-HU/Kent/9.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Ez az enyém? Nos, erre nem számítottam... De nem utasítom el. Köszönöm!",
          "AudioPath": "assets/hu-HU/Kent/10.ogg"
        },
        {
          "DialogueFrom": "event_popcorn1",
          "DialogueText": "Te maradj csöndben! Ne akarj a feleségem ellen fordítani engem.",
          "AudioPath": "assets/hu-HU/Kent/11.ogg"
        },
        {
          "DialogueFrom": "event_popcorn2",
          "DialogueText": "Teljesen igazad van, .",
          "AudioPath": "assets/hu-HU/Kent/12.ogg"
        },
        {
          "DialogueFrom": "event_popcorn3",
          "DialogueText": "Ne hazudj nekem! A hazugságokat mindennél jobban utálom!",
          "AudioPath": "assets/hu-HU/Kent/13.ogg"
        },
        {
          "DialogueFrom": "eventSeen_100_memory_oneday",
          "DialogueText": "Elnézést kérek a tegnapi miatt.",
          "AudioPath": "assets/hu-HU/Kent/14.ogg"
        },
        {
          "DialogueFrom": "eventSeen_100_memory_oneday_1",
          "DialogueText": "Nincs szükségem különleges bánásmódra. Csak normális ember szeretnék lenni, mint bárki más.",
          "AudioPath": "assets/hu-HU/Kent/15.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Úgy tűnik ma remek napunk van a horgászatra, nemde?",
          "AudioPath": "assets/hu-HU/Kent/16.ogg"
        },
        {
          "DialogueFrom": "Fri4",
          "DialogueText": "Elkezdhetnék horgászni. Úgy lenne mit csinálnom és lefoglalhatnám magamat.",
          "AudioPath": "assets/hu-HU/Kent/17.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "Heh... Az eső egy áldás. Emlékeztet arra, hogy mindig is lesznek hatalmasabb erők annál, mint amit bármilyen ember is készíthet.",
          "AudioPath": "assets/hu-HU/Kent/18.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2_1",
          "DialogueText": "Ez valójában megnyugtató, .",
          "AudioPath": "assets/hu-HU/Kent/19.ogg"
        },
        {
          "DialogueFrom": "married_Sam",
          "DialogueText": "Egy apát büszkévé teszi, ha azt látja, hogy a fiából férfi lett. Szívből jövő áldásomat adom rátok!",
          "AudioPath": "assets/hu-HU/Kent/20.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Üdv, gazda.",
          "AudioPath": "assets/hu-HU/Kent/21.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Oly sokáig voltam távol... Idegennek érzem magam.",
          "AudioPath": "assets/hu-HU/Kent/22.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "Nem tudom, hogy valaha is meg fogom szokni azt, hogy hazatértem.",
          "AudioPath": "assets/hu-HU/Kent/23.ogg"
        },
        {
          "DialogueFrom": "Mon4_1",
          "DialogueText": "A város békessége maszknak tűnik. Aztán lehet, hogy csak én gondolom így.",
          "AudioPath": "assets/hu-HU/Kent/24.ogg"
        },
        {
          "DialogueFrom": "MovieInvitation",
          "DialogueText": "Öhm... oké. Köszönöm. Ott találkozunk.",
          "AudioPath": "assets/hu-HU/Kent/25.ogg"
        },
        {
          "DialogueFrom": "reject_865",
          "DialogueText": "Igen... Alaposan le van zárva, így még erős lehet az íze.",
          "AudioPath": "assets/hu-HU/Kent/26.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Jó móka volt a hajóút. Még a tengerészeti kiképzésemet is fel tudtam használni, hogy segítsek a navigálásban.",
          "AudioPath": "assets/hu-HU/Kent/27.ogg"
        },
        {
          "DialogueFrom": "Resort_Bar",
          "DialogueText": "Csak szerzek egy kis vizet.",
          "AudioPath": "assets/hu-HU/Kent/28.ogg"
        },
        {
          "DialogueFrom": "Resort_Chair",
          "DialogueText": "Ez remek hely ahhoz, hogy összejöjjek a családdal. Nagyra értékelem, hogy feljavítottad.",
          "AudioPath": "assets/hu-HU/Kent/29.ogg"
        },
        {
          "DialogueFrom": "Resort_Entering",
          "DialogueText": "Hé, . Örülök, hogy látlak.",
          "AudioPath": "assets/hu-HU/Kent/30.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Ideje hazamenni.",
          "AudioPath": "assets/hu-HU/Kent/31.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Ezen a strandon álldogálva... ritka békés érzéssel tölt el.",
          "AudioPath": "assets/hu-HU/Kent/32.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore_1",
          "DialogueText": "Az óceán egy nagyon különös dolog.",
          "AudioPath": "assets/hu-HU/Kent/33.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Jodi mondta, hogy valóban a közösség részévé váltál. Ezt örömmel hallom.",
          "AudioPath": "assets/hu-HU/Kent/34.ogg"
        },
        {
          "DialogueFrom": "Sat8",
          "DialogueText": "Manapság nem megy olyan jól az alvás, szóval elnézést, ha fáradtnak tűnnék.",
          "AudioPath": "assets/hu-HU/Kent/35.ogg"
        },
        {
          "DialogueFrom": "Sat8_1",
          "DialogueText": "Biztos vagyok benne, hogy megérted.",
          "AudioPath": "assets/hu-HU/Kent/36.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Nem tudok mihez kezdeni magammal most, hogy hazatértem.",
          "AudioPath": "assets/hu-HU/Kent/37.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Megszoktam, hogy egy napirendhez vagyok kötve.",
          "AudioPath": "assets/hu-HU/Kent/38.ogg"
        },
        {
          "DialogueFrom": "Sun4",
          "DialogueText": "Neked van egy rutinod, amivel lefoglalhatod magadat, ugye? Az jó dolog.",
          "AudioPath": "assets/hu-HU/Kent/39.ogg"
        },
        {
          "DialogueFrom": "Sun4_1",
          "DialogueText": "A túl sok szabadság fullasztó lehet. Mindenkinek hasznos, ha az életének van némi felépítése.",
          "AudioPath": "assets/hu-HU/Kent/40.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Te a fiam barátja vagy?",
          "AudioPath": "assets/hu-HU/Kent/41.ogg"
        },
        {
          "DialogueFrom": "Thu_1",
          "DialogueText": "Te talán jobban ismered őt, mint én...",
          "AudioPath": "assets/hu-HU/Kent/42.ogg"
        },
        {
          "DialogueFrom": "Thu_inlaw_Sam",
          "DialogueText": "Hé, . Élvezed a házaséletet?",
          "AudioPath": "assets/hu-HU/Kent/43.ogg"
        },
        {
          "DialogueFrom": "Thu6",
          "DialogueText": "Hajnal 4 óta ébren vagyok... néha azon tűnődöm, hogy vajon vissza fogok térni valaha a normális rutinhoz.",
          "AudioPath": "assets/hu-HU/Kent/44.ogg"
        },
        {
          "DialogueFrom": "Thu6_1",
          "DialogueText": "Sam tényleg nagyot nőtt, mióta elmentem. Most már igazi férfi.",
          "AudioPath": "assets/hu-HU/Kent/45.ogg"
        },
        {
          "DialogueFrom": "Thu6_2",
          "DialogueText": "Bárcsak itt lehettem volna neki.",
          "AudioPath": "assets/hu-HU/Kent/46.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "A dolgok nem változtak túl sokat, amíg távol voltam.",
          "AudioPath": "assets/hu-HU/Kent/47.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "Nos, kivéve azt, hogy te itt vagy.",
          "AudioPath": "assets/hu-HU/Kent/48.ogg"
        },
        {
          "DialogueFrom": "Tue2",
          "DialogueText": "Kivéve azt, hogy egy új gazda van a városban.",
          "AudioPath": "assets/hu-HU/Kent/49.ogg"
        },
        {
          "DialogueFrom": "Tue6",
          "DialogueText": "Úgy érzem, hogy egész jól visszarázódom. Jó, hogy van egy barát a városban.",
          "AudioPath": "assets/hu-HU/Kent/50.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "Egy ideje már itt vagy, ugye? A farmod biztosan jól fejlődik.",
          "AudioPath": "assets/hu-HU/Kent/51.ogg"
        },
        {
          "DialogueFrom": "Wed10",
          "DialogueText": "Az, hogy életben vagyunk egy egészen különleges ajándék, ami szempillantás alatt jön-megy... ezt soha ne téveszd szem elől.",
          "AudioPath": "assets/hu-HU/Kent/52.ogg"
        },
        {
          "DialogueFrom": "Wed6",
          "DialogueText": "...Rengeteg barátomat vesztettem el harc közben.",
          "AudioPath": "assets/hu-HU/Kent/53.ogg"
        },
        {
          "DialogueFrom": "Wed6_1",
          "DialogueText": "A legszomorúbb az, hogy nem emlékszek az arcukra.",
          "AudioPath": "assets/hu-HU/Kent/54.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/63/y 2/t 600 1800/H",
          "DialogueText": "Izé, üdv. A nevem Kent. Tegnap este értem haza... a tengeren túlról. A feleségem mondta, hogy egy új gazda költözött ide a távollétem alatt.",
          "AudioPath": "assets/hu-HU/Kent/55.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/63/y 2/t 600 1800/H_1",
          "DialogueText": "Nos, csak be akartam mutatkozni... Majd még találkozunk.",
          "AudioPath": "assets/hu-HU/Kent/56.ogg"
        },
        {
          "DialogueFrom": "Event:Saloon/3917590/n saloonSportsRoom/O Alex",
          "DialogueText": "Örülök, hogy van kibúvóm a házból, és ez segít elterelni a figyelmemet.",
          "AudioPath": "assets/hu-HU/Kent/57.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/100/f Kent 750/p Kent/p Jodi",
          "DialogueText": "AAAAAHHHHHHH!!!",
          "AudioPath": "assets/hu-HU/Kent/58.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/100/f Kent 750/p Kent/p Jodi_1",
          "DialogueText": "Ez a hang...",
          "AudioPath": "assets/hu-HU/Kent/59.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/100/f Kent 750/p Kent/p Jodi_2",
          "DialogueText": "Tudhattad volna, hogy ez a hang a háborúra emlékeztet engem!",
          "AudioPath": "assets/hu-HU/Kent/60.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/100/f Kent 750/p Kent/p Jodi_3",
          "DialogueText": "...Rengeteg jó barátomat vesztettem el azokban az átkozott lövészárkokban.",
          "AudioPath": "assets/hu-HU/Kent/61.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/100/f Kent 750/p Kent/p Jodi_4",
          "DialogueText": "...Változnak az idők.",
          "AudioPath": "assets/hu-HU/Kent/62.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/100/f Kent 750/p Kent/p Jodi_5",
          "DialogueText": "215 null 215 -25 event_popcorn1 215 50 event_popcorn2 215 -50 event_popcorn3#(Hazudj) Engem hibáztass... Én kértem pattogatott kukoricát.",
          "AudioPath": "assets/hu-HU/Kent/63.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/100/f Kent 750/p Kent/p Jodi_6",
          "DialogueText": "Sajnálom édesem. Nem tudhattad, hogy a pattogatott kukorica hangjától ideges leszek.",
          "AudioPath": "assets/hu-HU/Kent/64.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/95/e 93/k 94/t 1800 1950/i 136/y 2",
          "DialogueText": "Üdv . Kérlek, kerülj beljebb.",
          "AudioPath": "assets/hu-HU/Kent/65.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/95/e 93/k 94/t 1800 1950/i 136/y 2_1",
          "DialogueText": "Heh heh.",
          "AudioPath": "assets/hu-HU/Kent/66.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/95/e 93/k 94/t 1800 1950/i 136/y 2_alt",
          "DialogueText": "Mmm... az a ropogós sügér nagyon finom volt! Élek-halok az ilyen ízekért...",
          "AudioPath": "assets/hu-HU/Kent/67.ogg"
        },
        {
          "DialogueFrom": "Event:SamHouse/95/e 93/k 94/t 1800 1950/i 136/y 2_alt_1",
          "DialogueText": "Úgy érzem, mintha én is a család része volnék.",
          "AudioPath": "assets/hu-HU/Kent/68.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Arra várok, hogy ez a bohóc valami vicceset is csináljon.Eléggé kiábrándító.",
          "AudioPath": "assets/hu-HU/Kent/69.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "*kacag* A fiam egész pontos tud lenni. Lehet, hogy tőlem örökölte.",
          "AudioPath": "assets/hu-HU/Kent/70.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "Hé . Szép esténk van, nemde?",
          "AudioPath": "assets/hu-HU/Kent/71.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Ezek a valamik nem tűnnek túl keménynek.",
          "AudioPath": "assets/hu-HU/Kent/72.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "Az igazat megvallva nem nagyon rajongom a tojásokért.",
          "AudioPath": "assets/hu-HU/Kent/73.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "Ez a nyílt terep... nyugtalanná tesz.*suttogva* Boldog gondolatok, Kent. Boldog gondolatok.",
          "AudioPath": "assets/hu-HU/Kent/74.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "Elgondolkoztál már azon... hogy miért találjuk a virágokat gyönyörűnek?Ha Jodinak és nekem valaha lányunk születne, akkor talán egy virág után neveznénk el őt.",
          "AudioPath": "assets/hu-HU/Kent/75.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Hé, jó látni téged.",
          "AudioPath": "assets/hu-HU/Kent/76.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "*szimat* ...az illatok nagyon hatékonyan hozzák vissza az emlékeket...Ez lehet jó és rossz is...",
          "AudioPath": "assets/hu-HU/Kent/77.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "Jó estét, . Remélem ki tudsz majd kapcsolódni a nagy aratási szezon előtt.",
          "AudioPath": "assets/hu-HU/Kent/78.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "Hé. Nagy terveid vannak az őszre?",
          "AudioPath": "assets/hu-HU/Kent/79.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Jó estét, . Kellemes ünnepeket!",
          "AudioPath": "assets/hu-HU/Kent/80.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "Én azért vagyok hálás, mert a kicsi fiam boldog és egészséges.",
          "AudioPath": "assets/hu-HU/Kent/81.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Szeretem a telet. Olyan békés.",
          "AudioPath": "assets/hu-HU/Kent/82.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "Készen állsz a versenyre? Mind úgy véljük, hogy te fogsz győzni.",
          "AudioPath": "assets/hu-HU/Kent/83.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "Ó...! Az anyám fiatal koromban mindig ilyet adott nekem. Régi szép emlékeket idéz fel. Köszönöm.",
          "AudioPath": "assets/hu-HU/Kent/84.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Hé, na ez aztán a jó ajándék. Köszönöm.",
          "AudioPath": "assets/hu-HU/Kent/85.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Ah... Ez egy... mi is ez tulajdonképpen? Hmm...",
          "AudioPath": "assets/hu-HU/Kent/86.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "Ez... Ilyet adtak nekem a Gotoroi börtöntáborban. Azóta is próbálom elfelejteni. *reszket*",
          "AudioPath": "assets/hu-HU/Kent/87.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Ez kedves tőled. A családnak is tetszeni fog.",
          "AudioPath": "assets/hu-HU/Kent/88.ogg"
        }
      ]
    }
  ]
}