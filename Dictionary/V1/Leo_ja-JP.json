        },
        {
          "DialogueFrom": "Event:Mountain/8959199/w sunny/t 600 1900/e 6497428/f Leo 2250/F_alt",
          "DialogueText": "できたよ！",
          "AudioPath": "assets/ja-JP/Leo/85.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue",
          "DialogueText": "どうやら生きてるみたいだね。",
          "AudioPath": "assets/ja-JP/Leo/86.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue_1",
          "DialogueText": "キミを見つけた鳥の友達から、危なそうだって聞いたから、ここまで引っぱってきたんだ。",
          "AudioPath": "assets/ja-JP/Leo/87.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue_2",
          "DialogueText": "外では十分気を付けてね。もう悲しい思いはしたくないんだ…",
