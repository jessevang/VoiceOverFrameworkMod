{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Grandpa.zh-CN",
      "VoicePackName": "Dictionary (Grandpa - zh-CN)",
      "Character": "Grandpa",
      "Language": "zh-CN",
      "Entries": [
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H",
          "DialogueText": "我亲爱的孩子……",
          "AudioPath": "assets/zh-CN/Grandpa/1_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "我亲爱的孙女……",
          "AudioPath": "assets/zh-CN/Grandpa/2_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "自从我们最后一次讲话，已经过去了很多年。你那时还很小……记得吗？",
          "AudioPath": "assets/zh-CN/Grandpa/3_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_3",
          "DialogueText": "自从我们最后一次讲话，已经过去了很多年。你那时还是个小姑娘……记得吗？",
          "AudioPath": "assets/zh-CN/Grandpa/4_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_4",
          "DialogueText": "你现在已经长大了啊！",
          "AudioPath": "assets/zh-CN/Grandpa/5.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_5",
          "DialogueText": "虽然你也许已经忘了我，但我一直都陪着你。",
          "AudioPath": "assets/zh-CN/Grandpa/6.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_6",
          "DialogueText": "虽然……我的身体离开了这个世界，但我的心永远留在星露谷。",
          "AudioPath": "assets/zh-CN/Grandpa/7.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "你来到这里已经两年了……",
          "AudioPath": "assets/zh-CN/Grandpa/8.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "的未来掌握在你手里。",
          "AudioPath": "assets/zh-CN/Grandpa/9.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2_1",
          "DialogueText": "再见！",
          "AudioPath": "assets/zh-CN/Grandpa/10.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H",
          "DialogueText": "我很高兴你召唤了我。也许上次我对你太严厉了。",
          "AudioPath": "assets/zh-CN/Grandpa/11.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "请原谅我。你一直那么努力，我真为你感到骄傲。",
          "AudioPath": "assets/zh-CN/Grandpa/12.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "那么……",
          "AudioPath": "assets/zh-CN/Grandpa/13.ogg"
        }
      ]
    }
  ]
}