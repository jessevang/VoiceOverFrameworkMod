      "Entries": [
        {
          "DialogueFrom": "AcceptBirthdayGift_Negative",
          "DialogueText": "Egy szülinapi ajándék? Ó... ez nem olyan vicces.",
          "AudioPath": "assets/hu-HU/Vincent/1.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive",
          "DialogueText": "Az egy szülinapi ajándék? Kinyithatom?",
          "AudioPath": "assets/hu-HU/Vincent/2.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive_1",
          "DialogueText": "Ó, váó! Pont erre vágytam! Yayayaya!",
          "AudioPath": "assets/hu-HU/Vincent/3.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Váó! Annyira édes! Mmmmm....",
          "AudioPath": "assets/hu-HU/Vincent/4.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(TR)FrogEgg",
          "DialogueText": "*Placcs* *placcs* ... Fúúúj! Megkaphatom? Megkaphatom? Hurrááá!",
          "AudioPath": "assets/hu-HU/Vincent/5.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Negative_alcohol_item",
          "DialogueText": "*szimat*... ez meg micsoda? Borzasztó a szaga... Anyaaaaaa!!!",
          "AudioPath": "assets/hu-HU/Vincent/6.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Fúj, már megint főtt cékla van...",
          "AudioPath": "assets/hu-HU/Vincent/7.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Jaj ne... anyu estére lencselevest készít.",
          "AudioPath": "assets/hu-HU/Vincent/8.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Anya! Ki akarok menni játszani!",
          "AudioPath": "assets/hu-HU/Vincent/9.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "teljesen beborította a moha.",
          "AudioPath": "assets/hu-HU/Vincent/10.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "Ó, egy idegen! A nevem Vincent.",
          "AudioPath": "assets/hu-HU/Vincent/11.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "Anyukám azt mondta, hogy ne álljak szóba idegenekkel... De te jónak tűnsz.",
          "AudioPath": "assets/hu-HU/Vincent/12.ogg"
        },
        {
          "DialogueFrom": "married_Sam",
          "DialogueText": "Ne vidd el az én Sammymet!",
          "AudioPath": "assets/hu-HU/Vincent/13.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Ha felnövök, olyan akarok lenni, mint a bátyám!",
          "AudioPath": "assets/hu-HU/Vincent/14.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Oké! Szia!",
          "AudioPath": "assets/hu-HU/Vincent/15.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "kent#Örülök, hogy apu visszatért... de más, mint amilyenre emlékszem...|Apu nincs olyan sokat itt... Hiányzik a vele való játszás.",
          "AudioPath": "assets/hu-HU/Vincent/16.ogg"
        },
        {
          "DialogueFrom": "reject_869",
          "DialogueText": "Óóó! Remek lelet!",
          "AudioPath": "assets/hu-HU/Vincent/17.ogg"
        },
        {
          "DialogueFrom": "reject_869_1",
          "DialogueText": "Bár anya nem hagyná, hogy megtartsam...",
          "AudioPath": "assets/hu-HU/Vincent/18.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Ha jól megfigyeled, láthatod, ahogy kis bogarak ugranak ki a homokból.",
          "AudioPath": "assets/hu-HU/Vincent/19.ogg"
        },
        {
          "DialogueFrom": "Resort_Entering",
          "DialogueText": "Ez jó móka volt! Egy napon én is hajóskapitány akarok lenni!",
          "AudioPath": "assets/hu-HU/Vincent/20.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Hamarosan vissza akarok térni!",
          "AudioPath": "assets/hu-HU/Vincent/21.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Akarok látni egy cápát!",
          "AudioPath": "assets/hu-HU/Vincent/22.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Penny néni minden héten új könyvet olvastat velem.",
          "AudioPath": "assets/hu-HU/Vincent/23.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Legalább néha hagyja, hogy én válasszak!",
          "AudioPath": "assets/hu-HU/Vincent/24.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Apu hamarosan hazatér!",
          "AudioPath": "assets/hu-HU/Vincent/25.ogg"
        },
        {
          "DialogueFrom": "Sat_1_1",
          "DialogueText": "Remélem hoz nekem valamilyen játékot.",
          "AudioPath": "assets/hu-HU/Vincent/26.ogg"
        },
        {
          "DialogueFrom": "Sat8",
          "DialogueText": "kent#Apu elég fura, mióta hazatért. Vajon mi történhetett vele?",
          "AudioPath": "assets/hu-HU/Vincent/27.ogg"
        },
        {
          "DialogueFrom": "Sat8_1",
          "DialogueText": "Többé már nem játszik velem.|Vajon apu haza fog jönni valaha...",
          "AudioPath": "assets/hu-HU/Vincent/28.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Szia bácsi!",
          "AudioPath": "assets/hu-HU/Vincent/29_male.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Szia néni!",
          "AudioPath": "assets/hu-HU/Vincent/30_female.ogg"
        },
        {
          "DialogueFrom": "Sun10",
          "DialogueText": "Üdv bácsi!",
          "AudioPath": "assets/hu-HU/Vincent/31_male.ogg"
        },
        {
          "DialogueFrom": "Sun10_1",
          "DialogueText": "Üdv néni!",
          "AudioPath": "assets/hu-HU/Vincent/32_female.ogg"
        },
        {
          "DialogueFrom": "Sun10_2",
          "DialogueText": "Ne mondd el anyunak... de te vagy a kedvenc felnőttem.",
          "AudioPath": "assets/hu-HU/Vincent/33.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Éhes vagyok... hol van anyu?",
          "AudioPath": "assets/hu-HU/Vincent/34.ogg"
        },
        {
          "DialogueFrom": "Thu2",
          "DialogueText": "Te nem vagy olyan uncsi, mint a többi felnőtt!",
          "AudioPath": "assets/hu-HU/Vincent/35.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "*sóhaj*... Anyu nem hagyja, hogy ma több gumicukrot egyek.",
          "AudioPath": "assets/hu-HU/Vincent/36.ogg"
        },
        {
          "DialogueFrom": "Tue6",
          "DialogueText": "Bácsi, tudsz titkot tartani?",
          "AudioPath": "assets/hu-HU/Vincent/37_male.ogg"
        },
        {
          "DialogueFrom": "Tue6_1",
          "DialogueText": "Néni, tudsz titkot tartani?",
          "AudioPath": "assets/hu-HU/Vincent/38_female.ogg"
        },
        {
          "DialogueFrom": "Tue6_2",
          "DialogueText": "Feleségül akarom venni Penny nénit, ha felnövök.",
          "AudioPath": "assets/hu-HU/Vincent/39.ogg"
        },
        {
          "DialogueFrom": "Tue6_3",
          "DialogueText": "De ezt senkinek se mondd el, oké?",
          "AudioPath": "assets/hu-HU/Vincent/40.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "Bogarakat akarok keresni, de anyu mérges lesz rám, ha piszkos leszek.",
          "AudioPath": "assets/hu-HU/Vincent/41.ogg"
        },
        {
          "DialogueFrom": "Wed_1",
          "DialogueText": "Nehéz választás.",
          "AudioPath": "assets/hu-HU/Vincent/42.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1",
          "DialogueText": "Sam...?",
          "AudioPath": "assets/hu-HU/Vincent/43.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_1",
          "DialogueText": "Apu haza fog jönni valaha?",
          "AudioPath": "assets/hu-HU/Vincent/44.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_2",
          "DialogueText": "De azt hallottam pár felnőttől, hogy... hogy minden katonánkat szétlőtték.",
          "AudioPath": "assets/hu-HU/Vincent/45.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_3",
          "DialogueText": "Oké!",
          "AudioPath": "assets/hu-HU/Vincent/46.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L",
          "DialogueText": "Nagyon izgatott vagyok, Sam!",
          "AudioPath": "assets/hu-HU/Vincent/47.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L_alt_1",
          "DialogueText": "Nekem is!",
          "AudioPath": "assets/hu-HU/Vincent/48.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19",
          "DialogueText": "bácsi, te hozzá fogsz menni Penny nénihez?",
          "AudioPath": "assets/hu-HU/Vincent/49_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19_1",
          "DialogueText": "néni... Őőő, neked van barátod?",
          "AudioPath": "assets/hu-HU/Vincent/50_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat",
          "DialogueText": "...",
          "AudioPath": "assets/hu-HU/Vincent/51.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Ez miért van zárva? Fel akarom fedezni a csatornát!",
          "AudioPath": "assets/hu-HU/Vincent/52.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Gunther professzornál? Aki a múzeumot vezeti? Mégis miből gondolod, hogy nála van?",
          "AudioPath": "assets/hu-HU/Vincent/53.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny",
          "DialogueText": "Szia, bácsi!",
          "AudioPath": "assets/hu-HU/Vincent/54_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "Szia, néni!",
          "AudioPath": "assets/hu-HU/Vincent/55_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "Új... újhagyma. Anyukám gyakran küld ki engem ezek begyűjtésére...",
          "AudioPath": "assets/hu-HU/Vincent/56.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_2",
          "DialogueText": "Felnőtt létedre elég kedves vagy... szóval mutatok neked egy titkos dolgot.",
          "AudioPath": "assets/hu-HU/Vincent/57.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3",
          "DialogueText": "Mikor ilyennek látod, akkor jól néz ki, nem?",
          "AudioPath": "assets/hu-HU/Vincent/58.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_4",
          "DialogueText": "De ha egy kicsit meghámozod...",
          "AudioPath": "assets/hu-HU/Vincent/59.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_5",
          "DialogueText": "...Akkor tele van rovarokkal!",
          "AudioPath": "assets/hu-HU/Vincent/60.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_6",
          "DialogueText": "Menő, ugye?",
          "AudioPath": "assets/hu-HU/Vincent/61.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_7",
          "DialogueText": "Általában szétlapítom őket!",
          "AudioPath": "assets/hu-HU/Vincent/62.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseAnimals",
          "DialogueText": "gazda? Rakhatok nyerget egy tehenedre és meglovagolhatom a városba menet? Igazi tehenészfiú akarok lenni!",
          "AudioPath": "assets/hu-HU/Vincent/63.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseFarming",
          "DialogueText": "Izé, gazda? Ha a te zöldségeidet eszem, akkor olyan nagy és erős leszek, mint a bátyám, Sam?",
          "AudioPath": "assets/hu-HU/Vincent/64.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseFarming_1",
          "DialogueText": "Annyira erős, hogy láttam, ahogy Penny nénit csak úgy felkapta a földről tegnap este, mikor felmásztak egy fára.",
          "AudioPath": "assets/hu-HU/Vincent/65.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseMinerals",
          "DialogueText": "gazda? Igaz, hogy valódi goblinok élnek az ilyen mély barlangokban... és hogy kislányokat rabolnak el, hogy megegyék őket vacsorára?",
          "AudioPath": "assets/hu-HU/Vincent/66.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny",
          "DialogueText": "Vagyis akkor nem kell megcsinálnom a házi feladatomat?",
          "AudioPath": "assets/hu-HU/Vincent/67.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny_implied",
          "DialogueText": "Mostanság sokat gondolkoztam a tortákon...",
          "AudioPath": "assets/hu-HU/Vincent/68.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Múúúú!",
          "AudioPath": "assets/hu-HU/Vincent/69.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Játszhatok megint a csúzlis játékkal apa!? Kérlek?",
          "AudioPath": "assets/hu-HU/Vincent/70.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "Anya nem enged be engem az útvesztőbe. *sír*",
          "AudioPath": "assets/hu-HU/Vincent/71.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Legyőztük az útvesztőt? Hol van a jutalmam?",
          "AudioPath": "assets/hu-HU/Vincent/72.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "Mikor kezdődik már a tojásvadászat?! Annyira izgatott vagyok!!",
          "AudioPath": "assets/hu-HU/Vincent/73.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13_1",
          "DialogueText": "Egy nagy tojásban akarok lakni!",
          "AudioPath": "assets/hu-HU/Vincent/74.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "Ez nem igazságos... Én miért nem lehetek virágkirálynő?",
          "AudioPath": "assets/hu-HU/Vincent/75.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "Ha van virágkirálynő... akkor biztos van virágkirály is!",
          "AudioPath": "assets/hu-HU/Vincent/76.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Unatkozom...",
          "AudioPath": "assets/hu-HU/Vincent/77.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "Miért nem játszhatok a tócsák mellett?",
          "AudioPath": "assets/hu-HU/Vincent/78.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "Látni akarom a ritka zöld medúzát!",
          "AudioPath": "assets/hu-HU/Vincent/79.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "Egyszer megérintettem egy medúzát és megcsípte a kezemet.",
          "AudioPath": "assets/hu-HU/Vincent/80.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Most már kinyithatjuk az ajándékokat?!",
          "AudioPath": "assets/hu-HU/Vincent/81.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "Megkaptam azt a dinoszaurusz figurámat amire vágytam! Látod!?Ez egy öhm... egy szte- ...sztego-... sztegoszonaurusz! ...Azt hiszem.",
          "AudioPath": "assets/hu-HU/Vincent/82.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Láttam egy nagy halat odalent!",
          "AudioPath": "assets/hu-HU/Vincent/83.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "A bátyám segít nekem hóembert építeni!",
          "AudioPath": "assets/hu-HU/Vincent/84.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "Váó, nagyon szépen köszönöm! Ez óriási!",
          "AudioPath": "assets/hu-HU/Vincent/85.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Ez az enyém? Váó, köszönöm!",
          "AudioPath": "assets/hu-HU/Vincent/86.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Ez nem volt vicces...",
          "AudioPath": "assets/hu-HU/Vincent/87.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "Fúj, ez meg micsoda?",
          "AudioPath": "assets/hu-HU/Vincent/88.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Köszönöm!",
          "AudioPath": "assets/hu-HU/Vincent/89.ogg"
        }
      ]
    }
  ]
