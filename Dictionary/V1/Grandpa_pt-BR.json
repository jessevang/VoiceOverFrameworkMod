        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Minha querida netinha...",
          "AudioPath": "assets/pt-BR/Grandpa/2_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "Faz muitos anos desde que nos falamos. Você era só um garotinho... Você se lembra?",
          "AudioPath": "assets/pt-BR/Grandpa/3_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_3",
