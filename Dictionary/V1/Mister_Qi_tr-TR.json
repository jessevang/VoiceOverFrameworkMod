{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Mister Qi.tr-TR",
      "VoicePackName": "Dictionary (Mister Qi - tr-TR)",
      "Character": "Mister Qi",
      "Language": "tr-TR",
      "Entries": [
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Mükemmelliğe ulaşmayı istemekten vazgeçme.",
          "AudioPath": "assets/tr-TR/Mister Qi/1.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Yaptığın her şeyde mükemmelliği hedefle.",
          "AudioPath": "assets/tr-TR/Mister Qi/2.ogg"
        },
        {
          "DialogueFrom": "married",
          "DialogueText": "Çok özel bir düğün töreniydi .",
          "AudioPath": "assets/tr-TR/Mister Qi/3.ogg"
        },
        {
          "DialogueFrom": "married_1",
          "DialogueText": "...Ne? Orada olduğumu görmedin mi?",
          "AudioPath": "assets/tr-TR/Mister Qi/4.ogg"
        },
        {
          "DialogueFrom": "married_2",
          "DialogueText": "Hahaha... Evlat... Bay Qi'den sır saklayamazsın.",
          "AudioPath": "assets/tr-TR/Mister Qi/5.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Çalışmaya devam et evlat. İleride yıldız olacaksın.",
          "AudioPath": "assets/tr-TR/Mister Qi/6.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Bir şeye ihtiyacın olursa bana gel.",
          "AudioPath": "assets/tr-TR/Mister Qi/7.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Fri",
          "DialogueText": "Gözlerini en uzak yıldıza dik ve onu hedefin yap.",
          "AudioPath": "assets/tr-TR/Mister Qi/8.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Fri_1",
          "DialogueText": "Sahip olduğun kudret düşündüğünün bir adım ötesine ulaşmanı sağlar.",
          "AudioPath": "assets/tr-TR/Mister Qi/9.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Mon",
          "DialogueText": "Evlat, bu dünyada gözünün görebildiğinden çok daha fazlası var. Bir gün ne demek istediğimi anlayacaksın.",
          "AudioPath": "assets/tr-TR/Mister Qi/10.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Mon_1",
          "DialogueText": "Şu an için her şeyi en iyi şekilde yapmaya bak.",
          "AudioPath": "assets/tr-TR/Mister Qi/11.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Sat",
          "DialogueText": "Evlat, kimi şeyler zamanı ve yerin ötesindedir. Kalbinde eşsiz bir his uyandırırlar... ötedeki şeylerin fısıltısını taşırlar.",
          "AudioPath": "assets/tr-TR/Mister Qi/12.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Sun",
          "DialogueText": "Sol tarafta özel mücadelelerin olduğu bir pano göreceksin... Bunlar sana özel hazırlandı.",
          "AudioPath": "assets/tr-TR/Mister Qi/13.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Sun_1",
          "DialogueText": "Sağ tarafta ise mücadelelerimi tamamladıkça erişeceğin bir dizi fantastik ödül var...",
          "AudioPath": "assets/tr-TR/Mister Qi/14.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Sun_2",
          "DialogueText": "Bol şans evlat.",
          "AudioPath": "assets/tr-TR/Mister Qi/15.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Thu",
          "DialogueText": "Nereye gidersen git... ben her zaman perdenin arkasında durup gösterinin ilgi çekici olmasını sağlayacağım. Hehehe...",
          "AudioPath": "assets/tr-TR/Mister Qi/16.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Thu_1",
          "DialogueText": "Yoksa... sağlamayacak mıyım?",
          "AudioPath": "assets/tr-TR/Mister Qi/17.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Tue",
          "DialogueText": "Evlat, şu anda bulunduğum yere saf iradenin gücüyle geldim.",
          "AudioPath": "assets/tr-TR/Mister Qi/18.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Tue_1",
          "DialogueText": "Görebildiğin üzere... Zihin çok güçlü bir araçtır.",
          "AudioPath": "assets/tr-TR/Mister Qi/19.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Tue_2",
          "DialogueText": "Hatta, gerçeklik ve ötesindeki en güçlü araçtır.",
          "AudioPath": "assets/tr-TR/Mister Qi/20.ogg"
        },
        {
          "DialogueFrom": "QiNutRoom_Wed",
          "DialogueText": "Azmine hayran kaldım, evlat... Buraya gelebilmek için çok ceviz aştın.",
          "AudioPath": "assets/tr-TR/Mister Qi/21.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Ne zaman rahatlamak istersen uğra.",
          "AudioPath": "assets/tr-TR/Mister Qi/22.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Sıkı çalışmaya devam edersen belki bir gün benim gibi olabilirsin.",
          "AudioPath": "assets/tr-TR/Mister Qi/23.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Hey, buradan kimseye bahsetme, tamam mı? Bana özel olmasını istiyorum.",
          "AudioPath": "assets/tr-TR/Mister Qi/24.ogg"
        },
        {
          "DialogueFrom": "Thu_1",
          "DialogueText": "Dostun Bay Qi'nin sırrını tutarsın, değil mi?",
          "AudioPath": "assets/tr-TR/Mister Qi/25.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "Bir gün buraya geleceğini biliyordum. Elbette işini zorlaştırdım. Yapmak için motive olmanı istedim.",
          "AudioPath": "assets/tr-TR/Mister Qi/26.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "Sıradan bir çocukluk geçirirken, verdiğim bir karar beni sıra dışı yaptı.",
          "AudioPath": "assets/tr-TR/Mister Qi/27.ogg"
        }
      ]
    }
  ]
}