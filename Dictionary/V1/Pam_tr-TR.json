{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Pam.tr-TR",
      "VoicePackName": "Dictionary (Pam - tr-TR)",
      "Character": "Pam",
      "Language": "tr-TR",
      "Entries": [
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Vay be... Böyle bir hediyeyi hiç beklemiyordum. Günümü güzelleştirdin evlat!",
          "AudioPath": "assets/tr-TR/Pam/1.ogg"
        },
        {
          "DialogueFrom": "cc_Bus",
          "DialogueText": "Hey, eski otobüsü gördün mü?",
          "AudioPath": "assets/tr-TR/Pam/2.ogg"
        },
        {
          "DialogueFrom": "cc_Bus_1",
          "DialogueText": "Aynen... Yeniden aygırıma binebileceğim, evlat... hehe. Çok iyi hissediyorum.",
          "AudioPath": "assets/tr-TR/Pam/3.ogg"
        },
        {
          "DialogueFrom": "divorced_Penny",
          "DialogueText": "Penny eve ağlayarak geldi... Ona n'aptın?",
          "AudioPath": "assets/tr-TR/Pam/4.ogg"
        },
        {
          "DialogueFrom": "event_credit1",
          "DialogueText": "Hey, kapa çeneni. Kimse bana ne yapacağımı söyleyemez!",
          "AudioPath": "assets/tr-TR/Pam/5.ogg"
        },
        {
          "DialogueFrom": "event_credit2",
          "DialogueText": "...Değil mi? Berbat!",
          "AudioPath": "assets/tr-TR/Pam/6.ogg"
        },
        {
          "DialogueFrom": "event_credit2_1",
          "DialogueText": "...Ah.",
          "AudioPath": "assets/tr-TR/Pam/7.ogg"
        },
        {
          "DialogueFrom": "fall_Fri",
          "DialogueText": "Her gün aynı şeyler...",
          "AudioPath": "assets/tr-TR/Pam/8.ogg"
        },
        {
          "DialogueFrom": "fall_Fri_1",
          "DialogueText": "Sen de böyle bir sorun yaşıyor musun, çiftçi?",
          "AudioPath": "assets/tr-TR/Pam/9.ogg"
        },
        {
          "DialogueFrom": "fall_Sat",
          "DialogueText": "Kibarlık yapıp evimi temizleyebilir misin?",
          "AudioPath": "assets/tr-TR/Pam/10.ogg"
        },
        {
          "DialogueFrom": "fall_Sat_1",
          "DialogueText": "Şaka şaka.",
          "AudioPath": "assets/tr-TR/Pam/11.ogg"
        },
        {
          "DialogueFrom": "fall_Sun",
          "DialogueText": "İşin gücün yok mu senin?",
          "AudioPath": "assets/tr-TR/Pam/12.ogg"
        },
        {
          "DialogueFrom": "fall_Sun_1",
          "DialogueText": "Kasabada boş boş dolanmaktansa bir mesleğin olduğu için şükretmelisin.",
          "AudioPath": "assets/tr-TR/Pam/13.ogg"
        },
        {
          "DialogueFrom": "fall_Sun4",
          "DialogueText": "Eski mesleğim olan tır şöförlüğünü özledim. Eskiden vadide dolanır, milletle sohbet eder, radyo dinlerdim.",
          "AudioPath": "assets/tr-TR/Pam/14.ogg"
        },
        {
          "DialogueFrom": "fall_Sun4_1",
          "DialogueText": "Zaman akıp gidiyor...",
          "AudioPath": "assets/tr-TR/Pam/15.ogg"
        },
        {
          "DialogueFrom": "fall_Thu",
          "DialogueText": "Penny benim tek varlığım. Ona iyi davransan iyi edersin.",
          "AudioPath": "assets/tr-TR/Pam/16.ogg"
        },
        {
          "DialogueFrom": "fall_Tue",
          "DialogueText": "Eğer seni daha iyi tanısaydım sohbet edebilirdik.",
          "AudioPath": "assets/tr-TR/Pam/17.ogg"
        },
        {
          "DialogueFrom": "fall_Tue4",
          "DialogueText": "İnanması zor ama eskiden gençtim.",
          "AudioPath": "assets/tr-TR/Pam/18.ogg"
        },
        {
          "DialogueFrom": "fall_Tue4_1",
          "DialogueText": "Büyürken zaman hızlı geçiyor.",
          "AudioPath": "assets/tr-TR/Pam/19.ogg"
        },
        {
          "DialogueFrom": "fall_Wed",
          "DialogueText": "*iç çeker*... Bu akşam yemek için donmuş bir şeyler yiyeceğim sanırım.",
          "AudioPath": "assets/tr-TR/Pam/20.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Her gün bir öncekinin aynısı...",
          "AudioPath": "assets/tr-TR/Pam/21.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Keşke zengin doğmuş olsaydım...",
          "AudioPath": "assets/tr-TR/Pam/22.ogg"
        },
        {
          "DialogueFrom": "Fri2",
          "DialogueText": "Ne geldi aklıma biliyor musun? Keşke hobim olsaydı. Her akşam salonda oturmaktansa hobimle uğraşırdım.",
          "AudioPath": "assets/tr-TR/Pam/23.ogg"
        },
        {
          "DialogueFrom": "Fri2_1",
          "DialogueText": "Aklına bir şeyler geliyor mu?",
          "AudioPath": "assets/tr-TR/Pam/24.ogg"
        },
        {
          "DialogueFrom": "Fri2_2",
          "DialogueText": "...Eh, belki yine kendi kendime dama oynarım.",
          "AudioPath": "assets/tr-TR/Pam/25.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Bu bir işaret... Mahvolduk...",
          "AudioPath": "assets/tr-TR/Pam/26.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "Bu yağmurların bir güç gösterisi olduğunu düşünüyorum... Bir uyarı.",
          "AudioPath": "assets/tr-TR/Pam/27.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2_1",
          "DialogueText": "Doğadan, Yoba'dan, artık neyden dersen... Bence böyle.",
          "AudioPath": "assets/tr-TR/Pam/28.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2_2",
          "DialogueText": "Demek istediğim, fazla kibirlenmemeliyiz. Yoksa yeşil bir yağmur yerine belki kan kırmızısı bir yağmur yağar... Ne demek istediğimi anlıyor musun?",
          "AudioPath": "assets/tr-TR/Pam/29.ogg"
        },
        {
          "DialogueFrom": "GreenRainFinished",
          "DialogueText": "Sanırım kıyamet henüz gelmemiş evlat...",
          "AudioPath": "assets/tr-TR/Pam/30.ogg"
        },
        {
          "DialogueFrom": "GreenRainFinished_1",
          "DialogueText": "Neyse, eski dokuz beş işime geri dönüyorum...",
          "AudioPath": "assets/tr-TR/Pam/31.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "Selam, evlat. İsmim Pamela.",
          "AudioPath": "assets/tr-TR/Pam/32.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "Pislik yapmazsan iyi anlaşırız.",
          "AudioPath": "assets/tr-TR/Pam/33.ogg"
        },
        {
          "DialogueFrom": "married_Alex",
          "DialogueText": "Mullner ailesinin oğluyla evlenmişsin.",
          "AudioPath": "assets/tr-TR/Pam/34.ogg"
        },
        {
          "DialogueFrom": "married_Alex_1",
          "DialogueText": "O pis köpeği de alacak mısın?",
          "AudioPath": "assets/tr-TR/Pam/35.ogg"
        },
        {
          "DialogueFrom": "married_Penny",
          "DialogueText": "Onu gerçekten özleyeceğim...",
          "AudioPath": "assets/tr-TR/Pam/36.ogg"
        },
        {
          "DialogueFrom": "married_Penny_1",
          "DialogueText": "Penny'me iyi bak. Tamam mı?",
          "AudioPath": "assets/tr-TR/Pam/37.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Ah... Kafam...",
          "AudioPath": "assets/tr-TR/Pam/38.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Eğer karşılayabilseydim daha sağlıklı beslenirdim.",
          "AudioPath": "assets/tr-TR/Pam/39.ogg"
        },
        {
          "DialogueFrom": "Mon_2",
          "DialogueText": "Hey, senin çiftlikte sağlıklı bi şeyler vardır kesin. Var, değil mi?",
          "AudioPath": "assets/tr-TR/Pam/40.ogg"
        },
        {
          "DialogueFrom": "MovieInvitation",
          "DialogueText": "Kart Pam'le dışarı çıkmak mı istiyorsun? Hah!",
          "AudioPath": "assets/tr-TR/Pam/41_male.ogg"
        },
        {
          "DialogueFrom": "MovieInvitation_1",
          "DialogueText": "Kart Pam'le kadın kadına takılmak mı istiyorsun? Hah!",
          "AudioPath": "assets/tr-TR/Pam/42_female.ogg"
        },
        {
          "DialogueFrom": "MovieInvitation_2",
          "DialogueText": "Olur evlat... Orada buluşuruz.",
          "AudioPath": "assets/tr-TR/Pam/43.ogg"
        },
        {
          "DialogueFrom": "pamHouseUpgrade",
          "DialogueText": "Bana ev aldığına inanamıyorum. Bu işte bir bit yeniği mi var yoksa?",
          "AudioPath": "assets/tr-TR/Pam/44.ogg"
        },
        {
          "DialogueFrom": "pamHouseUpgrade_1",
          "DialogueText": "Üzgünüm, insanların bana bu kadar iyi davranmasına alışık değilim...",
          "AudioPath": "assets/tr-TR/Pam/45.ogg"
        },
        {
          "DialogueFrom": "pamHouseUpgrade_memory_oneweek",
          "DialogueText": "Yeni eve bayıldım. Tekrardan teşekkürler evlat!",
          "AudioPath": "assets/tr-TR/Pam/46.ogg"
        },
        {
          "DialogueFrom": "pamHouseUpgradeAnonymous",
          "DialogueText": "Biri bana ev almış! Bir pislik mi dönüyor yoksa?",
          "AudioPath": "assets/tr-TR/Pam/47.ogg"
        },
        {
          "DialogueFrom": "pamHouseUpgradeAnonymous_1",
          "DialogueText": "Üzgünüm, insanların bana bu kadar iyi davranmasın alışkın değilim...",
          "AudioPath": "assets/tr-TR/Pam/48.ogg"
        },
        {
          "DialogueFrom": "reject_867",
          "DialogueText": "Şuna bakar mısın... Gelecekten gelmiş gibi.",
          "AudioPath": "assets/tr-TR/Pam/49.ogg"
        },
        {
          "DialogueFrom": "reject_867_1",
          "DialogueText": "Hehe, üzgünüm evlat... bu işime yaramaz. Beynimde bundaki şu şeyler kadar hücre yoktur be!",
          "AudioPath": "assets/tr-TR/Pam/50.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Heh... Willy yolda her birimize bir parça kuru balık verdi. Acayip lezzetli!",
          "AudioPath": "assets/tr-TR/Pam/51.ogg"
        },
        {
          "DialogueFrom": "Resort_Bar",
          "DialogueText": "Özel bir şeye şahitlik ediyorsun, evlat... Benzersiz bir şey sipariş ettim. Sipariş iki oldu!",
          "AudioPath": "assets/tr-TR/Pam/52.ogg"
        },
        {
          "DialogueFrom": "Resort_Chair",
          "DialogueText": "Rahat dediğin budur! Ayakkabılarımı bile çıkartasım var!",
          "AudioPath": "assets/tr-TR/Pam/53.ogg"
        },
        {
          "DialogueFrom": "Resort_Chair_1",
          "DialogueText": "Yakından bakmasan daha hayırlı olur yalnız.",
          "AudioPath": "assets/tr-TR/Pam/54.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Gördüğüm kadarıyla burada eşi benzeri olmayan balıklar yüzüyor. Bir dahaki sefere oltamı da getireceğim!",
          "AudioPath": "assets/tr-TR/Pam/55.ogg"
        },
        {
          "DialogueFrom": "Saloon",
          "DialogueText": "Kemiklerimi ısıtacak bir yudum alkol gibisi yok...",
          "AudioPath": "assets/tr-TR/Pam/56.ogg"
        },
        {
          "DialogueFrom": "Saloon_Entry",
          "DialogueText": "Ah... Sonunda./İçkiye ihtiyacım var./Gus! Her zamankinden./Hop, ben geldim!/Vay anasını, ne susamışım ama!/Dilim damağıma yapıştı!",
          "AudioPath": "assets/tr-TR/Pam/57.ogg"
        },
        {
          "DialogueFrom": "Saloon_Fri",
          "DialogueText": "Hop, Gus! En sağlamından yolla!",
          "AudioPath": "assets/tr-TR/Pam/58.ogg"
        },
        {
          "DialogueFrom": "Saloon_Sat",
          "DialogueText": "yanıt vermiyor.",
          "AudioPath": "assets/tr-TR/Pam/59.ogg"
        },
        {
          "DialogueFrom": "Saloon_Sun",
          "DialogueText": "kendinde değil. Ondan uzak dursan iyi olur.",
          "AudioPath": "assets/tr-TR/Pam/60.ogg"
        },
        {
          "DialogueFrom": "Saloon_Thu",
          "DialogueText": "*yutkunur* Sabahtan beri şunu içmeyi bekliyordum. Heh heh.",
          "AudioPath": "assets/tr-TR/Pam/61.ogg"
        },
        {
          "DialogueFrom": "Saloon_Tue",
          "DialogueText": "Merhaba, Gus! Bana bir bardak daha yolla bakayım! *hıçkırır*",
          "AudioPath": "assets/tr-TR/Pam/62.ogg"
        },
        {
          "DialogueFrom": "Saloon_Wed",
          "DialogueText": "*iç çeker*... Merhaba, evlat. Böyle bir yerde sürünmek için fazla iyisin.",
          "AudioPath": "assets/tr-TR/Pam/63.ogg"
        },
        {
          "DialogueFrom": "Saloon6",
          "DialogueText": "Selam, evlat. Param olsaydı sana içki ısmarlardım!",
          "AudioPath": "assets/tr-TR/Pam/64.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Evimde girmeden önce ayakkabılarının temiz olduğundan emin ol.",
          "AudioPath": "assets/tr-TR/Pam/65.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Temizlik yapmak çok zor geliyor ki bunu çoktan anlamışsındır.",
          "AudioPath": "assets/tr-TR/Pam/66.ogg"
        },
        {
          "DialogueFrom": "Sat4",
          "DialogueText": "Keşke geceleri evime elfler gelip ortalığı toparlasa. Hahahaha.",
          "AudioPath": "assets/tr-TR/Pam/67.ogg"
        },
        {
          "DialogueFrom": "Sat4_1",
          "DialogueText": "Şu an yaban havucu olsa iyi olurdu aslında.",
          "AudioPath": "assets/tr-TR/Pam/68.ogg"
        },
        {
          "DialogueFrom": "Sat4_2",
          "DialogueText": "Bacaklarım bu kadar kötü olmasaydı ara sıra dağlarda gezer ve toplardım.",
          "AudioPath": "assets/tr-TR/Pam/69.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Fish Pond",
          "DialogueText": "Balık havuzun olduğunu duydum... Çocukken bizim de vardı.",
          "AudioPath": "assets/tr-TR/Pam/70.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Fish Pond_1",
          "DialogueText": "Havuzda neredeyse her şeyi yetiştirebilirsin... Fakat daha süslü balıklar, daha sıkıntılı olabiliyor!",
          "AudioPath": "assets/tr-TR/Pam/71.ogg"
        },
        {
          "DialogueFrom": "summer_Fri",
          "DialogueText": "...Efendim?",
          "AudioPath": "assets/tr-TR/Pam/72.ogg"
        },
        {
          "DialogueFrom": "summer_Fri_1",
          "DialogueText": "Yakışıklı adamsın, neden benim gibi yaşlı bir kadınla konuşup zaman harcıyorsun?",
          "AudioPath": "assets/tr-TR/Pam/73_male.ogg"
        },
        {
          "DialogueFrom": "summer_Fri_2",
          "DialogueText": "Güzel kızsın, neden benimle vakit harcıyorsun?",
          "AudioPath": "assets/tr-TR/Pam/74_female.ogg"
        },
        {
          "DialogueFrom": "summer_Sat",
          "DialogueText": "Birileri bahçem hakkında söyleniyor... dağınık olduğunu söylüyorlar, size ne ki bundan?",
          "AudioPath": "assets/tr-TR/Pam/75.ogg"
        },
        {
          "DialogueFrom": "summer_Sat_1",
          "DialogueText": "Hım...",
          "AudioPath": "assets/tr-TR/Pam/76.ogg"
        },
        {
          "DialogueFrom": "summer_Sat2",
          "DialogueText": "Evin oradaki ihtiyara takılma.",
          "AudioPath": "assets/tr-TR/Pam/77.ogg"
        },
        {
          "DialogueFrom": "summer_Sat2_1",
          "DialogueText": "Sana dik dik bakabilir ama barakasından yüz yıl da geçse çıkmaz.",
          "AudioPath": "assets/tr-TR/Pam/78.ogg"
        },
        {
          "DialogueFrom": "summer_Sat2_2",
          "DialogueText": "Adı mı ne? Çirkin.",
          "AudioPath": "assets/tr-TR/Pam/79.ogg"
        },
        {
          "DialogueFrom": "summer_Thu",
          "DialogueText": "Ah, merhaba yabancı.",
          "AudioPath": "assets/tr-TR/Pam/80.ogg"
        },
        {
          "DialogueFrom": "summer_Thu_1",
          "DialogueText": "Genelde yabancılarla sohbet etmem.",
          "AudioPath": "assets/tr-TR/Pam/81.ogg"
        },
        {
          "DialogueFrom": "summer_Thu4",
          "DialogueText": "Eninde sonunda salonda bitiyormuşum gibi geliyor.",
          "AudioPath": "assets/tr-TR/Pam/82.ogg"
        },
        {
          "DialogueFrom": "summer_Thu4_1",
          "DialogueText": "Evime çok yakın, hiç iyi değil.",
          "AudioPath": "assets/tr-TR/Pam/83.ogg"
        },
        {
          "DialogueFrom": "summer_Tue",
          "DialogueText": "Merhaba, yabancı.",
          "AudioPath": "assets/tr-TR/Pam/84.ogg"
        },
        {
          "DialogueFrom": "summer_Tue2",
          "DialogueText": "Ah, merhaba Bay .",
          "AudioPath": "assets/tr-TR/Pam/85_male.ogg"
        },
        {
          "DialogueFrom": "summer_Tue2_1",
          "DialogueText": "Ah, merhaba Bayan .",
          "AudioPath": "assets/tr-TR/Pam/86_female.ogg"
        },
        {
          "DialogueFrom": "summer_Tue2_2",
          "DialogueText": "Yardıma falan mı ihtiyacın var?",
          "AudioPath": "assets/tr-TR/Pam/87.ogg"
        },
        {
          "DialogueFrom": "summer_Tue6",
          "DialogueText": "Bir zamanlar senin gibi hayaller kurardım... Bir de şimdiki halime bak.",
          "AudioPath": "assets/tr-TR/Pam/88.ogg"
        },
        {
          "DialogueFrom": "summer_Tue6_1",
          "DialogueText": "Hayat hiç istediğimiz gibi gitmiyor.",
          "AudioPath": "assets/tr-TR/Pam/89.ogg"
        },
        {
          "DialogueFrom": "summer_Tue6_2",
          "DialogueText": "Her neyse, gidenin arkasından ağlamaya gerek yok.",
          "AudioPath": "assets/tr-TR/Pam/90.ogg"
        },
        {
          "DialogueFrom": "summer_Wed",
          "DialogueText": "Buralarda bir çiftlik olması çok iyi.",
          "AudioPath": "assets/tr-TR/Pam/91.ogg"
        },
        {
          "DialogueFrom": "summer_Wed4",
          "DialogueText": "Çiftlikte işlerin yolunda giderse kasaba ekonomisi de bundan faydalanır.",
          "AudioPath": "assets/tr-TR/Pam/92.ogg"
        },
        {
          "DialogueFrom": "summer_Wed4_1",
          "DialogueText": "Böylelikle herkesin bir işi olur ve mutlu mesut yaşarız, değil mi?",
          "AudioPath": "assets/tr-TR/Pam/93.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Gus ile arkadaş mısın? O iyi biridir.",
          "AudioPath": "assets/tr-TR/Pam/94.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Bana doğum günümde beleş bira verir.",
          "AudioPath": "assets/tr-TR/Pam/95.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Peh... Geçen gece salonda çok fazla karamelli bira içmişim... Kaybol.",
          "AudioPath": "assets/tr-TR/Pam/96.ogg"
        },
        {
          "DialogueFrom": "Thu_1",
          "DialogueText": "Evim çok iyi değil ama en azından nehrin kenarında bulunuyor.",
          "AudioPath": "assets/tr-TR/Pam/97.ogg"
        },
        {
          "DialogueFrom": "Thu_2",
          "DialogueText": "Selam. Penny benim bir tanem. Ya ona iyi davran, ya da uzak dur. Anladın mı?",
          "AudioPath": "assets/tr-TR/Pam/98.ogg"
        },
        {
          "DialogueFrom": "Thu4",
          "DialogueText": "Penny salonda çok fazla zaman geçirdiğimi söylüyor...",
          "AudioPath": "assets/tr-TR/Pam/99.ogg"
        },
        {
          "DialogueFrom": "Thu4_inlaw_Penny",
          "DialogueText": "Evde yapayalnız kalmak çok zor...",
          "AudioPath": "assets/tr-TR/Pam/100.ogg"
        },
        {
          "DialogueFrom": "Trailer_Entry",
          "DialogueText": "Penny! Akşam yemeğini hazırladın mı?/Penny! Ev işleri bitti mi?/Penny! Evde misin?/Hop! Ben geldim!",
          "AudioPath": "assets/tr-TR/Pam/101.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "Hey, sen. Ben yokken karavanımın oralarda dolanma. Anladın mı?",
          "AudioPath": "assets/tr-TR/Pam/102.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "Üzgünüm fakat yabancılara karşı dikkatli olmalıyım.",
          "AudioPath": "assets/tr-TR/Pam/103.ogg"
        },
        {
          "DialogueFrom": "Tue2",
          "DialogueText": "Bugün gazeteyi okuyunca moralim bozuldu. Dünyanın çivisi çıkmış evlat.",
          "AudioPath": "assets/tr-TR/Pam/104.ogg"
        },
        {
          "DialogueFrom": "Tue2_1",
          "DialogueText": "Kendini işine verirsen hayatta kalırsın...",
          "AudioPath": "assets/tr-TR/Pam/105.ogg"
        },
        {
          "DialogueFrom": "Tue2_2",
          "DialogueText": "Babam eskiden böyle derdi. Heh heh heh.",
          "AudioPath": "assets/tr-TR/Pam/106.ogg"
        },
        {
          "DialogueFrom": "Wed",
