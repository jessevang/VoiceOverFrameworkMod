{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Marnie.hu-HU",
      "VoicePackName": "Dictionary (Marnie - hu-HU)",
      "Character": "Marnie",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "AcceptGift_(O)SkillBook_0",
          "DialogueText": "Köszönöm, . Mindig van egy Almanach másolat nálam. Nagyon pontos!",
          "AudioPath": "assets/hu-HU/Marnie/1.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Váó, ezer hála, . Ezt nem fogom elfelejteni!",
          "AudioPath": "assets/hu-HU/Marnie/2.ogg"
        },
        {
          "DialogueFrom": "dating_Shane_memory_oneweek",
          "DialogueText": "Mostanában Shane sokkal pozitívabbnak és reménytelibbnek tűnik.",
          "AudioPath": "assets/hu-HU/Marnie/3.ogg"
        },
        {
          "DialogueFrom": "dating_Shane_memory_oneweek_1",
          "DialogueText": "Olyan boldog vagyok, hogy ideköltöztél és ennyire kijöttök egymással!",
          "AudioPath": "assets/hu-HU/Marnie/4.ogg"
        },
        {
          "DialogueFrom": "Fair_Judged",
          "DialogueText": "Nos, nem nyertem... de ugyanúgy nagy öröm volt megosztani mindenkivel kemény munkám eredményét!",
          "AudioPath": "assets/hu-HU/Marnie/5.ogg"
        },
        {
          "DialogueFrom": "Fair_Judged_PlayerLost_PurpleShorts",
          "DialogueText": "De furcsa kinézete van annak a lila salátának a kirakatodban!",
          "AudioPath": "assets/hu-HU/Marnie/6.ogg"
        },
        {
          "DialogueFrom": "Fair_Judged_PlayerLost_PurpleShorts_1",
          "DialogueText": "Esküszöm, hogy pont úgy néz ki, mint Lewis polgármester különleges alsón...",
          "AudioPath": "assets/hu-HU/Marnie/7.ogg"
        },
        {
          "DialogueFrom": "Fair_Judged_PlayerLost_PurpleShorts_2",
          "DialogueText": "Ó!... heh... hagyjuk!",
          "AudioPath": "assets/hu-HU/Marnie/8.ogg"
        },
        {
          "DialogueFrom": "Fair_Judging",
          "DialogueText": "Nos, mindent vagy semmit...",
          "AudioPath": "assets/hu-HU/Marnie/9.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "A legtöbb éjszakán a szalonban találhatsz engem.",
          "AudioPath": "assets/hu-HU/Marnie/10.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Az állatok jó társaságnak bizonyulnak, de néhanapján emberekkel is kell időt töltenem.",
          "AudioPath": "assets/hu-HU/Marnie/11.ogg"
        },
        {
          "DialogueFrom": "Fri6",
          "DialogueText": "Azon tűnődöm, hogy vajon Mr. Lewis nem magányos abban a nagy házban...",
          "AudioPath": "assets/hu-HU/Marnie/12.ogg"
        },
        {
          "DialogueFrom": "Fri6_1",
          "DialogueText": "Olyan üresnek tűnik ott, nem?",
          "AudioPath": "assets/hu-HU/Marnie/13.ogg"
        },
        {
          "DialogueFrom": "gotPet",
          "DialogueText": "Szerintem boldog leszel, hogy van egy kis barátod a farmon.",
          "AudioPath": "assets/hu-HU/Marnie/14.ogg"
        },
        {
          "DialogueFrom": "gotPet_memory_oneday",
          "DialogueText": "Biztos, hogy kezdetben szégyenlős lesz. Az állatoknak kis időbe telik, mire kényelmesen érzik magukat egy új helyen.",
          "AudioPath": "assets/hu-HU/Marnie/15.ogg"
        },
        {
          "DialogueFrom": "gotPet_memory_oneweek",
          "DialogueText": "A kis már berendezkedett? Egy állat számára az a terület olyan remek a felfedezésekhez.",
          "AudioPath": "assets/hu-HU/Marnie/16.ogg"
        },
        {
          "DialogueFrom": "gotPet_memory_oneyear",
          "DialogueText": "Most ugrott be... nagyjából egy éve, hogy örökbe lett fogadva általad. Fogadok, hogy most már sokkal közelebb kerültetek egymáshoz!",
          "AudioPath": "assets/hu-HU/Marnie/17.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Te nem aggódsz emiatt az eső miatt? Az számomra kissé megnyugtató...",
          "AudioPath": "assets/hu-HU/Marnie/18.ogg"
        },
        {
          "DialogueFrom": "GreenRain_1",
          "DialogueText": "Csak remélem, hogy a tehenek bebújtak a helyükre...",
          "AudioPath": "assets/hu-HU/Marnie/19.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "Úgy tűnik az állatok élvezik ezt az esőt... Azt hiszem extra tápanyaggal tölti meg a füveket.",
          "AudioPath": "assets/hu-HU/Marnie/20.ogg"
        },
        {
          "DialogueFrom": "GreenRainFinished",
          "DialogueText": "Nos, úgy tűnik a semmi miatt ijedtünk meg... Az eső teljesen ártalmatlan volt!",
          "AudioPath": "assets/hu-HU/Marnie/21.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "Ah, Lewis polgármester szólt, hogy nemrég érkeztél. Marnie vagyok!",
          "AudioPath": "assets/hu-HU/Marnie/22.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "A tanyámon haszonállatokat és állati termékeket adok el. Beugorhatnál valamikor.",
          "AudioPath": "assets/hu-HU/Marnie/23.ogg"
        },
        {
          "DialogueFrom": "married_Shane",
          "DialogueText": "Örülök, hogy Shane beköltözött a farmodra... tovább folytathatja a családi hagyományt!",
          "AudioPath": "assets/hu-HU/Marnie/24.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Szeretem az állatokat, . Ha jól bánsz a tieiddel, akkor jó barátok leszünk!",
          "AudioPath": "assets/hu-HU/Marnie/25_male.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Szeretem az állatokat, . Bánj jól a tieiddel és biztos, hogy jó barátok leszünk!",
          "AudioPath": "assets/hu-HU/Marnie/26_female.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "Az állatok olyan ártatlanok, oly édesek. És ha én nem vigyázok rájuk, akkor ki fog?",
          "AudioPath": "assets/hu-HU/Marnie/27.ogg"
        },
        {
          "DialogueFrom": "Mon4_1",
          "DialogueText": "Remélem azért a csirkék nem mérgesek, mikor elveszem a tojásaikat.",
          "AudioPath": "assets/hu-HU/Marnie/28.ogg"
        },
        {
          "DialogueFrom": "reject_789",
          "DialogueText": "Gyááá!",
          "AudioPath": "assets/hu-HU/Marnie/29.ogg"
        },
        {
          "DialogueFrom": "reject_789_1",
          "DialogueText": "...Öhm... Tessék? Heh.. Nem tudom, hogy mi az...",
          "AudioPath": "assets/hu-HU/Marnie/30.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Hehe... végre bemutathatom, hogy mi lett az aerobik klubban végzett kemény munkám eredménye.",
          "AudioPath": "assets/hu-HU/Marnie/31.ogg"
        },
        {
          "DialogueFrom": "Resort_Bar",
          "DialogueText": "Huhhh... Forróság van!",
          "AudioPath": "assets/hu-HU/Marnie/32.ogg"
        },
        {
          "DialogueFrom": "Resort_Chair",
          "DialogueText": "Fúj... vizes a szék.",
          "AudioPath": "assets/hu-HU/Marnie/33.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Melegséget érzek.",
          "AudioPath": "assets/hu-HU/Marnie/34.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Hee hee... Érdekes mindenkit a fürdőruhájában látni.",
          "AudioPath": "assets/hu-HU/Marnie/35.ogg"
        },
        {
          "DialogueFrom": "Saloon",
          "DialogueText": "Mivel itt vagyok, így végre van időm egy kicsit pihenni és szocializálódni.",
          "AudioPath": "assets/hu-HU/Marnie/36.ogg"
        },
        {
          "DialogueFrom": "Saloon_Fri",
          "DialogueText": "nem veszi észre, hogy itt vagy...",
          "AudioPath": "assets/hu-HU/Marnie/37.ogg"
        },
        {
          "DialogueFrom": "Saloon_Sat",
          "DialogueText": "Mindig látom Clintet idebent, de senkihez se szól. Csak néha szétnéz és ennyi.",
          "AudioPath": "assets/hu-HU/Marnie/38.ogg"
        },
        {
          "DialogueFrom": "Saloon_Sun",
          "DialogueText": "*sóhaj*... Ma nem voltam túlságosan eredményes.",
          "AudioPath": "assets/hu-HU/Marnie/39.ogg"
        },
        {
          "DialogueFrom": "Saloon_Thu",
          "DialogueText": "Huhhh! Jó érzés egy kemény melós nap után kirúgni a hámból.",
          "AudioPath": "assets/hu-HU/Marnie/40.ogg"
        },
        {
          "DialogueFrom": "Saloon_Tue",
          "DialogueText": "Ma nincsenek itt túl sokan...",
          "AudioPath": "assets/hu-HU/Marnie/41.ogg"
        },
        {
          "DialogueFrom": "Saloon_Wed",
          "DialogueText": "Ma egy egész vödör tejet öntöttem ki a padlóra, szörnyű volt. Annyira elpazaroltam szegény Belindám kemény munkáját.",
          "AudioPath": "assets/hu-HU/Marnie/42.ogg"
        },
        {
          "DialogueFrom": "Saloon_Wed_1",
          "DialogueText": "Örülök, hogy felbukkantál! Most már kissé nyüzsgősebb a nap.",
          "AudioPath": "assets/hu-HU/Marnie/43.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Voltál már a házamtól nyugatra lévő furcsa toronyban?",
          "AudioPath": "assets/hu-HU/Marnie/44.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Egy alkalommal szörnyű, másvilági hangot hallottam onnét kiszűrődni.",
          "AudioPath": "assets/hu-HU/Marnie/45.ogg"
        },
        {
          "DialogueFrom": "Sat_2",
          "DialogueText": "Én a helyedben elkerülném azt a helyet...",
          "AudioPath": "assets/hu-HU/Marnie/46.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Stable",
          "DialogueText": "Ó, van egy lovad? Az csodálatos! A lovak lenyűgöző állatok.",
          "AudioPath": "assets/hu-HU/Marnie/47.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Hé, jó téged itt látni!",
          "AudioPath": "assets/hu-HU/Marnie/48.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Nyugodtan nézz be hozzánk, amikor kedved tartja.",
          "AudioPath": "assets/hu-HU/Marnie/49.ogg"
        },
        {
          "DialogueFrom": "Sun8",
          "DialogueText": "Üdv !",
          "AudioPath": "assets/hu-HU/Marnie/50.ogg"
        },
        {
          "DialogueFrom": "Sun8_1",
          "DialogueText": "Örülök, hogy összebarátkoztunk. Tudom, hogy jó gondját viseled a kedves állataimnak.",
          "AudioPath": "assets/hu-HU/Marnie/51.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Üdv. Szükséged van valamire?",
          "AudioPath": "assets/hu-HU/Marnie/52.ogg"
        },
        {
          "DialogueFrom": "Thu_1",
          "DialogueText": "A jövőben lehet, hogy másfajta állatokat is be fogok szerezni.",
          "AudioPath": "assets/hu-HU/Marnie/53.ogg"
        },
        {
          "DialogueFrom": "Thu_2",
          "DialogueText": "Habár még nem tudom, hogy van-e rájuk kereslet, szóval előbb be kell bizonyítanod, hogy tudsz bánni az állatokkal.",
          "AudioPath": "assets/hu-HU/Marnie/54.ogg"
        },
        {
          "DialogueFrom": "Thu_3",
          "DialogueText": "Csak tisztelettel gondozd az állataidat, és jó dolgok fognak történni.",
          "AudioPath": "assets/hu-HU/Marnie/55.ogg"
        },
        {
          "DialogueFrom": "Thu_2",
          "DialogueText": "Egy ideje már itt vagy... hogy megy a farmod?",
          "AudioPath": "assets/hu-HU/Marnie/56.ogg"
        },
        {
          "DialogueFrom": "Thu_2_1",
          "DialogueText": "Könnyű az állatokról gondoskodni, amíg nem feledkezel meg róluk!",
          "AudioPath": "assets/hu-HU/Marnie/57.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "Most, hogy Shane elment, meg kell tanítanom Jasnak, hogy hogyan gondozhatja a csirkéket.|Az unokaöcsém, Shane nálunk lakik már pár hónapja.",
          "AudioPath": "assets/hu-HU/Marnie/58.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "Besegít nekem a csirkéknél, szóval nem panaszkodhatok.",
          "AudioPath": "assets/hu-HU/Marnie/59.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "A kaszával levághatod a füvet, hogy azzal etesd az állataidat.",
          "AudioPath": "assets/hu-HU/Marnie/60.ogg"
        },
        {
          "DialogueFrom": "Wed_1",
          "DialogueText": "Vagy akár tőlem is vásárolhatsz párat, ha gondolod.",
          "AudioPath": "assets/hu-HU/Marnie/61.ogg"
        },
        {
          "DialogueFrom": "Wed_2",
          "DialogueText": "Jól jönne egy kis pénz...",
          "AudioPath": "assets/hu-HU/Marnie/62.ogg"
        },
        {
          "DialogueFrom": "Wed_3",
          "DialogueText": "Adios.",
          "AudioPath": "assets/hu-HU/Marnie/63.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910674/f Shane 1000",
          "DialogueText": "Shane?",
          "AudioPath": "assets/hu-HU/Marnie/64.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910674/f Shane 1000_1",
          "DialogueText": "... Tudsz tenni valamit? Kiütötte magát.",
          "AudioPath": "assets/hu-HU/Marnie/65.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910674/f Shane 1000_2",
          "DialogueText": "Shane! Mi ütött beléd? Egész nap csak a szobádban bóbiskolsz és sört iszol!",
          "AudioPath": "assets/hu-HU/Marnie/66.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910674/f Shane 1000_3",
          "DialogueText": "Mi a terved? Sose gondolsz a jövődre?",
          "AudioPath": "assets/hu-HU/Marnie/67.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910674/f Shane 1000_alt",
          "DialogueText": "Ó egek...",
          "AudioPath": "assets/hu-HU/Marnie/68.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910674/f Shane 1000_alt_1",
          "DialogueText": "Aggódom...",
          "AudioPath": "assets/hu-HU/Marnie/69.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane",
          "DialogueText": "Szia Shane... ma szokatlanul boldognak tűnsz.",
          "AudioPath": "assets/hu-HU/Marnie/70.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane_1",
          "DialogueText": "Hadd találjam ki... Gus leárazta a dobozos sört?",
          "AudioPath": "assets/hu-HU/Marnie/71.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane_alt",
          "DialogueText": "Olyan élénknek tűnik!",
          "AudioPath": "assets/hu-HU/Marnie/72.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/92/e 91/i 78/t 900 1700",
          "DialogueText": "Ó! Elhoztad a barlangi répát! Nagyon köszönöm ${úr",
          "AudioPath": "assets/hu-HU/Marnie/73_male.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/92/e 91/i 78/t 900 1700_1",
          "DialogueText": "asszony}$.",
          "AudioPath": "assets/hu-HU/Marnie/74_female.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/92/e 91/i 78/t 900 1700_1",
          "DialogueText": "Ez jól néz ki... Annyira, hogy mindjárt magam eszem meg!",
          "AudioPath": "assets/hu-HU/Marnie/75.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/92/e 91/i 78/t 900 1700_1_1",
          "DialogueText": "...mindjárt.",
          "AudioPath": "assets/hu-HU/Marnie/76.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/92/e 91/i 78/t 900 1700_2",
          "DialogueText": "Remélem nem jelentett túl nagy gondot... Nagyon hálás vagyok érte!",
          "AudioPath": "assets/hu-HU/Marnie/77.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/92/e 91/i 78/t 900 1700_implied",
          "DialogueText": "Ismét köszönöm, hogy kisegítettél.",
          "AudioPath": "assets/hu-HU/Marnie/78.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/92/e 91/i 78/t 900 1700_implied_1",
          "DialogueText": "...és a kecskéimm is azt mondják, hogy 'köszönöm'.",
          "AudioPath": "assets/hu-HU/Marnie/79.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/1590166/m 1000/t 600 930/d Mon Tue Thu Sat Sun/w sunny/h cat/H",
          "DialogueText": "Látod ezt a cicát?",
          "AudioPath": "assets/hu-HU/Marnie/80.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/1590166/m 1000/t 600 930/d Mon Tue Thu Sat Sun/w sunny/h cat/H_1",
          "DialogueText": "A farmod bejáratánál üldögélt! Szerintem elkóborolt... szegény jószág.",
          "AudioPath": "assets/hu-HU/Marnie/81.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/1590166/m 1000/t 600 930/d Mon Tue Thu Sat Sun/w sunny/h cat/H_2",
          "DialogueText": "Szerintem tetszik neki ez a hely! Hé, őőő... Szerinted nem jönne jól egy macska a farmodra?",
          "AudioPath": "assets/hu-HU/Marnie/82.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/1590166/m 1000/t 600 930/d Mon Tue Thu Sat Sun/w sunny/h cat/H_3",
          "DialogueText": "Nos, kis ... Légy jó cicus... oké?",
          "AudioPath": "assets/hu-HU/Marnie/83.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/897405/m 1000/t 600 930/d Mon Tue Thu Sat Sun/w sunny/h dog/H",
          "DialogueText": "Látod ezt a kutyát?",
          "AudioPath": "assets/hu-HU/Marnie/84.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/897405/m 1000/t 600 930/d Mon Tue Thu Sat Sun/w sunny/h dog/H_2",
          "DialogueText": "Szerintem tetszik neki ez a hely! Hé, őőő... Szerinted nem jönne jól egy kutya a farmodra?",
          "AudioPath": "assets/hu-HU/Marnie/85.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/897405/m 1000/t 600 930/d Mon Tue Thu Sat Sun/w sunny/h dog/H_3",
          "DialogueText": "Nos, ... Légy jó kutyus... oké?",
          "AudioPath": "assets/hu-HU/Marnie/86.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/91/f Marnie 750/t 600 930",
          "DialogueText": "Jó reggelt, ${úr",
          "AudioPath": "assets/hu-HU/Marnie/87_male.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/91/f Marnie 750/t 600 930_1",
          "DialogueText": "asszony}$!",
          "AudioPath": "assets/hu-HU/Marnie/88_female.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/91/f Marnie 750/t 600 930_2",
          "DialogueText": "Egy szívességet jöttem kérni tőled, mivel olyan kedves szomszéd vagy.",
          "AudioPath": "assets/hu-HU/Marnie/89.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/91/f Marnie 750/t 600 930_3",
          "DialogueText": "Tudod, próbálom a kecskéimet megtanítani arra, hogy azt mondják 'helló'... de egyáltalán nem figyelnek rám, míg nincs nálam a kedvenc elemózsiájuk, a barlangi répa!",
          "AudioPath": "assets/hu-HU/Marnie/90.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/91/f Marnie 750/t 600 930_4",
          "DialogueText": "Szóval... mivel én félek a bányáktól... Arra gondoltam, hogy, nos, nem hoznál nekem egy barlangi répát, ha lesz rá lehetőséged?",
          "AudioPath": "assets/hu-HU/Marnie/91.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/91/f Marnie 750/t 600 930_5",
          "DialogueText": "Nem erőltetem a dolgot... de ha akarod, akkor nézz csak be reggel 9 és délután 5 között hozzám egy barlangi répával. Nagy örömöt okoznál egy öreglánynak. Szia.",
          "AudioPath": "assets/hu-HU/Marnie/92.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/91/f Marnie 750/t 600 930_implied",
          "DialogueText": "A kecskéim megőrülnek a barlangi répáktól... Nem értem miért.",
          "AudioPath": "assets/hu-HU/Marnie/93.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3091462/e 3911124/O Leah/w sunny/t 1130 1400/A leahPaint",
          "DialogueText": "Öhm... Leah? Meddig fog ez még tartani?",
          "AudioPath": "assets/hu-HU/Marnie/94.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3091462/e 3911124/O Leah/w sunny/t 1130 1400/A leahPaint_1",
          "DialogueText": "...Reszketnek a térdeim, a combjaim szétesnek, és minden érzés kimúlt a lábamból.",
          "AudioPath": "assets/hu-HU/Marnie/95.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3091462/e 3911124/O Leah/w sunny/t 1130 1400/A leahPaint_1",
          "DialogueText": "Ez tetszik! Ezt kirakom a szobámba...",
          "AudioPath": "assets/hu-HU/Marnie/96.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3091462/e 3911124/O Leah/w sunny/t 1130 1400/A leahPaint_2",
          "DialogueText": "Öhm... , miért nem tartod meg a sajátodat? Lehet jó volna, mint, ööö... az első festményed... értesz?",
          "AudioPath": "assets/hu-HU/Marnie/97.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3091462/e 3911124/O Leah/w sunny/t 1130 1400/A leahPaint_alt",
          "DialogueText": "Jól nézzek ki!",
          "AudioPath": "assets/hu-HU/Marnie/98.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3091462/e 3911124/O Leah/w sunny/t 1130 1400/A leahPaint_alt_1",
          "DialogueText": "Oooh!",
          "AudioPath": "assets/hu-HU/Marnie/99.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3091462/e 3911124/O Leah/w sunny/t 1130 1400/A leahPaint_alt_2",
          "DialogueText": "Hadd nézzem",
          "AudioPath": "assets/hu-HU/Marnie/100.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3091462/e 3911124/O Leah/w sunny/t 1130 1400/A leahPaint_implied",
          "DialogueText": "Öhm... Szívem? Mégis hová tervezted felakasztani azt a képet, amit készítettél?",
          "AudioPath": "assets/hu-HU/Marnie/101.ogg"
        },
        {
          "DialogueFrom": "Event:HaleyHouse/150938/n emilyStones_alt",
          "DialogueText": "Én igen!",
          "AudioPath": "assets/hu-HU/Marnie/102.ogg"
        },
        {
          "DialogueFrom": "Event:Saloon/3206194/n gusGiantOmelet",
          "DialogueText": "Mmmm... tojások.",
          "AudioPath": "assets/hu-HU/Marnie/103.ogg"
        },
        {
          "DialogueFrom": "Event:Town/3917584/f Shane 3500/O Shane/d Fri/t 800 1700",
          "DialogueText": "Jaj ne... ez nem néz ki túl jól.",
          "AudioPath": "assets/hu-HU/Marnie/104.ogg"
        },
        {
          "DialogueFrom": "Event:Town/3917584/f Shane 3500/O Shane/d Fri/t 800 1700_1",
          "DialogueText": "? Jobb lesz, ha rajta tartod a szemed... legközelebb ha látod őt kijönni a szalonból, mondanod kellene neki valamit...",
          "AudioPath": "assets/hu-HU/Marnie/105.ogg"
        },
        {
          "DialogueFrom": "Event:Town/3917584/f Shane 3500/O Shane/d Fri/t 800 1700_implied",
          "DialogueText": "Minden rendben van, ? Gondterheltnek tűnsz...",
          "AudioPath": "assets/hu-HU/Marnie/106.ogg"
        },
        {
          "DialogueFrom": "Event:Town/3917586/e 3917585/O Shane/A shaneSaloon2",
          "DialogueText": "Rendben... rajtakaptuk őt. Most szembesítsük vele. Vegyél nagy levegőt...",
          "AudioPath": "assets/hu-HU/Marnie/107.ogg"
        },
        {
          "DialogueFrom": "Event:Town/3917586/e 3917585/O Shane/A shaneSaloon2_1",
          "DialogueText": "Shane... ezt szoktad csinálni idebent?",
          "AudioPath": "assets/hu-HU/Marnie/108.ogg"
        },
        {
          "DialogueFrom": "Event:Town/3917586/e 3917585/O Shane/A shaneSaloon2_2",
          "DialogueText": "De... ezek a dobozok...",
          "AudioPath": "assets/hu-HU/Marnie/109.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184644/e 8675611/O Haley/A haleyCakewalk2/t 600 1500/w sunny/i 220",
          "DialogueText": "Öhm... Haley? Mikor állunk meg? A l... A lábaim... *liheg* *liheg*",
          "AudioPath": "assets/hu-HU/Marnie/110.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184644/e 8675611/O Haley/A haleyCakewalk2/t 600 1500/w sunny/i 220_alt",
          "DialogueText": "Igen",
          "AudioPath": "assets/hu-HU/Marnie/111.ogg"
        },
        {
          "DialogueFrom": "Event:Town/639373/f Lewis 1500/f Marnie 1500/t 1900 2300/w sunny",
          "DialogueText": "Túl sokat törődsz a munkáddal, Lewis. Tudod, van más is az életben.",
          "AudioPath": "assets/hu-HU/Marnie/112.ogg"
        },
        {
          "DialogueFrom": "Event:Town/639373/f Lewis 1500/f Marnie 1500/t 1900 2300/w sunny_1",
          "DialogueText": "...de igen, megértem. Azt hiszem, hogy... kettőnket... titokban kell tartani.",
          "AudioPath": "assets/hu-HU/Marnie/113.ogg"
        },
        {
          "DialogueFrom": "Event:Town/639373/f Lewis 1500/f Marnie 1500/t 1900 2300/w sunny_3",
          "DialogueText": "Különben is miért bujkáltál ott?",
          "AudioPath": "assets/hu-HU/Marnie/114.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "A tőlem telhető legjobb módon próbálkoztam, de az én kirakatom semmiség Pierréhez képest.Nos, talán a bírák plusz pontot adnak nekem a hatalmas sajtért.",
          "AudioPath": "assets/hu-HU/Marnie/115.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Az állataimnak sikeres évük volt! Remélem a kiváló minőségű termések le fogják nyűgözni a bírákat.Neked hogy áll a kirakatod, ? Az ad lehetőséget arra, hogy megmutasd a munkád gyümölcsét.",
          "AudioPath": "assets/hu-HU/Marnie/116.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "Szia, . Termékeny volt az ősz?Kezd eléggé hűvös lenni... a termések már nem fogják sokáig bírni.",
          "AudioPath": "assets/hu-HU/Marnie/117.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Remélem Jas jól van odabent.Jól telt az idei gazdálkodós éved, ?",
          "AudioPath": "assets/hu-HU/Marnie/118.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "Üdvözlünk a Tojásfesztiválon... hála a temérdek csirkéinknek.",
          "AudioPath": "assets/hu-HU/Marnie/119.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13_1",
          "DialogueText": "A tyúkok tojják a tojásokat, de a kakasoknak is nagyon fontos szerepük van... ők védik meg a tyúkokat és akadályozzák meg azt, hogy egymásnak essenek!Így boldogabbak a tyúkok és jobbak a tojások is...",
          "AudioPath": "assets/hu-HU/Marnie/120.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "*sóhaj*... Szerelem van a levegőben... És én még mindig szingli vagyok.",
          "AudioPath": "assets/hu-HU/Marnie/121.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "Ez a nagy nyílt tér tökéletes legelő lehet az állatok számára, nem gondolod?Psszt... Szerintem Lewis ma felkér majd táncolni engem!",
          "AudioPath": "assets/hu-HU/Marnie/122.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Ha hoztál valamit a levesbe, akkor menj fel az oldalán található lépcsőn és dobd bele.",
          "AudioPath": "assets/hu-HU/Marnie/123.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "Üdvözöllek ! Hoztál valamilyen alapanyagot a leveshez? Csak menj fel a lépcsőn és dobd bele!",
          "AudioPath": "assets/hu-HU/Marnie/124.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "Túl nagy a tömeg súlya egy ilyen ősi, fából készült kikötőnek! Én itt maradok a strand környékén.",
          "AudioPath": "assets/hu-HU/Marnie/125.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "Nem mehetek tovább a kikötőn. Reszketni tudok a recsegő fa hangjától.Mikor még kislány voltam, beszakadt alattam egy rothadó kikötő, és az mély nyomot hagyott bennem...",
          "AudioPath": "assets/hu-HU/Marnie/126.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Üdvözlünk a Téli Csillag ünnepén!",
          "AudioPath": "assets/hu-HU/Marnie/127.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "Kellemes ünnepeket, !",
          "AudioPath": "assets/hu-HU/Marnie/128.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Annyira sajnálom azokat a szegény, fázós állatokat.",
          "AudioPath": "assets/hu-HU/Marnie/129.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "Ajjj... Olyan cuki az a bebugyolált disznócska!",
          "AudioPath": "assets/hu-HU/Marnie/130.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "Ez egy hihetetlen ajándék! Köszönöm!",
          "AudioPath": "assets/hu-HU/Marnie/131.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Köszönöm!",
          "AudioPath": "assets/hu-HU/Marnie/132.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Ó. Akkor azt hiszem átveszem.",
          "AudioPath": "assets/hu-HU/Marnie/133.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "Ez értéktelen. Nem értelek.",
          "AudioPath": "assets/hu-HU/Marnie/134.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Köszönöm! Ez jól néz ki.",
          "AudioPath": "assets/hu-HU/Marnie/135.ogg"
        }
      ]
    }
  ]
}