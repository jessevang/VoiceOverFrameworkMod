      "VoicePackId": "Dictionary.Grandpa.it-IT",
      "VoicePackName": "Dictionary (Grandpa - it-IT)",
      "Character": "Grandpa",
      "Language": "it-IT",
      "Entries": [
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H",
          "DialogueText": "Mio caro ragazzo...",
          "AudioPath": "assets/it-IT/Grandpa/1_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Mia carissima nipote...",
          "AudioPath": "assets/it-IT/Grandpa/2_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "Sono passati tanti anni dall'ultima volta che abbiamo parlato. Eri solo un bambino, allora... Ricordi?",
          "AudioPath": "assets/it-IT/Grandpa/3_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_3",
          "DialogueText": "Sono passati tanti anni dall'ultima volta che abbiamo parlato. Eri solo una bambina, allora... Ricordi?",
          "AudioPath": "assets/it-IT/Grandpa/4_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_4",
          "DialogueText": "Guarda quanta strada hai fatto!",
          "AudioPath": "assets/it-IT/Grandpa/5.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_5",
          "DialogueText": "Anche se forse mi hai dimenticato, io sono sempre stato qui.",
          "AudioPath": "assets/it-IT/Grandpa/6.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_6",
          "DialogueText": "Vedi... Il mio corpo ha lasciato questo mondo, ma il mio cuore resterà sempre a Stardew Valley.",
          "AudioPath": "assets/it-IT/Grandpa/7.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Sei qui da due anni, ormai...",
          "AudioPath": "assets/it-IT/Grandpa/8.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "Il futuro della Fattoria è in mano tua, adesso.",
          "AudioPath": "assets/it-IT/Grandpa/9.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2_1",
          "DialogueText": "Addio!",
          "AudioPath": "assets/it-IT/Grandpa/10.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H",
          "DialogueText": "Sono felice che tu mi abbia evocato. Forse... sono stato troppo duro con te, l'ultima volta.",
          "AudioPath": "assets/it-IT/Grandpa/11.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "Perdonami. Stai lavorando davvero sodo, e io ne sono fiero.",
          "AudioPath": "assets/it-IT/Grandpa/12.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "Vediamo...",
          "AudioPath": "assets/it-IT/Grandpa/13.ogg"
        }
      ]
    }
  ]
}