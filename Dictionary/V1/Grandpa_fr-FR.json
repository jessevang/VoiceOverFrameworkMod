{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Grandpa.fr-FR",
      "VoicePackName": "Dictionary (Grandpa - fr-FR)",
      "Character": "Grandpa",
      "Language": "fr-FR",
      "Entries": [
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H",
          "DialogueText": "Mon cher garçon...",
          "AudioPath": "assets/fr-FR/Grandpa/1_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Ma chère petite-fille...",
          "AudioPath": "assets/fr-FR/Grandpa/2_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "Ça fait de nombreuses années que nous ne nous sommes pas parlé. Tu n’étais qu’un petit garçon à l’époque... Tu t'en souviens ?",
          "AudioPath": "assets/fr-FR/Grandpa/3_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_3",
          "DialogueText": "Ça fait de nombreuses années que nous ne sommes pas parlés. Tu n’étais qu’une petite fille à l’époque... Tu t'en souviens ?",
          "AudioPath": "assets/fr-FR/Grandpa/4_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_4",
          "DialogueText": "Regarde tout le chemin que tu as parcouru !",
          "AudioPath": "assets/fr-FR/Grandpa/5.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_5",
          "DialogueText": "Même si tu m’as oublié, je suis toujours resté près de toi.",
          "AudioPath": "assets/fr-FR/Grandpa/6.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_6",
          "DialogueText": "Mon corps a quitté ce monde, mais mon coeur restera toujours à Stardew Valley.",
          "AudioPath": "assets/fr-FR/Grandpa/7.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Tu es ici depuis deux ans maintenant...",
          "AudioPath": "assets/fr-FR/Grandpa/8.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "L’avenir de la ferme est entre tes mains, maintenant.",
          "AudioPath": "assets/fr-FR/Grandpa/9.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2_1",
          "DialogueText": "Adieu !",
          "AudioPath": "assets/fr-FR/Grandpa/10.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H",
          "DialogueText": "Je suis heureux que tu m’aies appelé. Je... J’étais peut-être un peu trop sévère avec toi la dernière fois que nous nous sommes parlé.",
          "AudioPath": "assets/fr-FR/Grandpa/11.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "Pardonne-moi. Tu as travaillé très dur et je suis très fier de toi.",
          "AudioPath": "assets/fr-FR/Grandpa/12.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "Voyons voir...",
          "AudioPath": "assets/fr-FR/Grandpa/13.ogg"
        }
      ]
    }
  ]
}