      "Entries": [
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900",
          "DialogueText": "Quell'odore... avvicinati...",
          "AudioPath": "assets/it-IT/Bear/1.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_1",
          "DialogueText": "Heh... Non mi capita tutti i giorni di parlare a un umano. A quanto pare c'è un po' di 'magia silvana' in te...",
          "AudioPath": "assets/it-IT/Bear/2.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_2",
          "DialogueText": "Ora... riguardo al mio messaggio... Hai portato la roba buona?",
          "AudioPath": "assets/it-IT/Bear/3.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_3",
          "DialogueText": "HOoOoOoooh... quell'odore... Questa roba è dolcissima!",
          "AudioPath": "assets/it-IT/Bear/4.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_3_1",
          "DialogueText": "...È il mio sciroppo preferito... mi rende davvero felice.",
          "AudioPath": "assets/it-IT/Bear/5.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_4",
          "DialogueText": "...Da oggi hai un nuovo amico, .",
          "AudioPath": "assets/it-IT/Bear/6.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_5",
          "DialogueText": "Ho una cosa speciale che voglio mostrarti...",
          "AudioPath": "assets/it-IT/Bear/7.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_alt",
          "DialogueText": "L'orso ti ha lasciato la sua conoscenza speciale...",
          "AudioPath": "assets/it-IT/Bear/8.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_alt_1",
          "DialogueText": "I Lamponi e le More ora valgono 3x di più!",
          "AudioPath": "assets/it-IT/Bear/9.ogg"
        }
      ]
    }
  ]
