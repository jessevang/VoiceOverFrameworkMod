{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Leo.hu-HU",
      "VoicePackName": "Dictionary (Leo - hu-HU)",
      "Character": "Leo",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Váó, pont olyan az íze, mint az arany dióknak! Ez nagyszerű!",
          "AudioPath": "assets/hu-HU/Leo/1.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(TR)ParrotEgg",
          "DialogueText": "Ez egy drága tojás... Egy gyermek! Köszönöm, hogy nekem adtad, majd vigyázok rá.",
          "AudioPath": "assets/hu-HU/Leo/2.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Egyszer... felmásztam este a vulkán tetejére, és fényeket láttam nagyon-nagyon messze.",
          "AudioPath": "assets/hu-HU/Leo/3.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Vajon vannak odakint más madár családok?",
          "AudioPath": "assets/hu-HU/Leo/4.ogg"
        },
        {
          "DialogueFrom": "Fri4",
          "DialogueText": "Vajon láthatom az otthonodat a vulkán tetejéről?",
          "AudioPath": "assets/hu-HU/Leo/5.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Szia... Minden rendben?",
          "AudioPath": "assets/hu-HU/Leo/6.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "Sokat tanultam a papagáj családomtól...",
          "AudioPath": "assets/hu-HU/Leo/7.ogg"
        },
        {
          "DialogueFrom": "Mon4_1",
          "DialogueText": "De biztos vagyok abban, hogy sokkal több van odakint, amiről nem tudok.",
          "AudioPath": "assets/hu-HU/Leo/8.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "...Ma szomorú vagyok.",
          "AudioPath": "assets/hu-HU/Leo/9.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Szóval menj csak... Szórakozz. Hadd szomorkodjak.",
          "AudioPath": "assets/hu-HU/Leo/10.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Mikor esik, akkor fura madarak látogatnak meg.",
          "AudioPath": "assets/hu-HU/Leo/11.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Próbálok beszélni velük... De elrepülnek.",
          "AudioPath": "assets/hu-HU/Leo/12.ogg"
        },
        {
          "DialogueFrom": "Sun_2",
          "DialogueText": "Biztos nem akarnak barátkozni...",
          "AudioPath": "assets/hu-HU/Leo/13.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Találkoztál a hozzád hasonlókkal?",
          "AudioPath": "assets/hu-HU/Leo/14.ogg"
        },
        {
          "DialogueFrom": "Thu_1",
          "DialogueText": "Én nem beszélek velük túl sokat. Biztonságban érzem magam a saját fajtámmal...",
          "AudioPath": "assets/hu-HU/Leo/15.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "Mikor Nap mérges, akkor jó érzés a dzsungelben lenni.",
          "AudioPath": "assets/hu-HU/Leo/16.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "Nap nem találhat meg minket a levelek alatt. Átverjük őt! Haha...",
          "AudioPath": "assets/hu-HU/Leo/17.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "...Elnézést... Ma szégyenlős vagyok.",
          "AudioPath": "assets/hu-HU/Leo/18.ogg"
        },
        {
          "DialogueFrom": "Wed2",
          "DialogueText": "Légy óvatos a vulkánban... a lelkek nem kedvelik a kívülállólkat.",
          "AudioPath": "assets/hu-HU/Leo/19.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved",
          "DialogueText": "Bocsánat...",
          "AudioPath": "assets/hu-HU/Leo/20.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_1",
          "DialogueText": "Nehéz visszaemlékezni, hogy milyen... normálisan beszélni...",
          "AudioPath": "assets/hu-HU/Leo/21.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_10",
          "DialogueText": "Ha nem lennének a papagájok... ki tudja, hogy mi történt volna velem.",
          "AudioPath": "assets/hu-HU/Leo/22.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_11",
          "DialogueText": "Örülök, hogy találkoztam velük. Mindig is a családomként tekintettem rájuk, akkor is, ha ez kissé furának tesz engem.",
          "AudioPath": "assets/hu-HU/Leo/23.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_12",
          "DialogueText": "Mindenesetre köszönöm, hogy beszéltél velem, .",
          "AudioPath": "assets/hu-HU/Leo/24.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_13",
          "DialogueText": "Talán egy napon te is a családunk tagja lehetsz, hehe.",
          "AudioPath": "assets/hu-HU/Leo/25.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_14",
          "DialogueText": "Tessék, hadd tanítsam meg neked azt, hogy hogyan kell papagájul azt mondani, hogy",
          "AudioPath": "assets/hu-HU/Leo/26.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_2",
          "DialogueText": "Azt gondolod, hogy fura vagyok, ugye?",
          "AudioPath": "assets/hu-HU/Leo/27.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_3",
          "DialogueText": "Néha azon tűnődöm, hogy milyen más lenne az életem, ha nem sodródtam volna erre a szigetre...",
          "AudioPath": "assets/hu-HU/Leo/28.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_4",
          "DialogueText": "Odahaza nem igazán törődtem a madarakkal. Sose gondoltam arra, hogy meghallgassam azt, amit mondhatnak.",
          "AudioPath": "assets/hu-HU/Leo/29.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_5",
          "DialogueText": "Azt hiszem ez normális... a normális gyerekeknek.",
          "AudioPath": "assets/hu-HU/Leo/30.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_6",
          "DialogueText": "Azon gondolkozom... vajon mit mondhatnak a normális gyerekek rólam...",
          "AudioPath": "assets/hu-HU/Leo/31.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_7",
          "DialogueText": "Szerinted lehetek még normális gyerek?",
          "AudioPath": "assets/hu-HU/Leo/32.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_8",
          "DialogueText": "Hmmm... Azt hiszem igazad van.",
          "AudioPath": "assets/hu-HU/Leo/33.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_9",
          "DialogueText": "Habár mivel ezen a szigeten éltem, így sok olyan tapasztalatot szereztem, amit normális gyerekek sose kapnának.",
          "AudioPath": "assets/hu-HU/Leo/34.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_implied",
          "DialogueText": "Köszönöm, hogy ezt mondtad.",
          "AudioPath": "assets/hu-HU/Leo/35.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_implied_1",
          "DialogueText": "Köszönöm, én is megszoktalak már!",
          "AudioPath": "assets/hu-HU/Leo/36.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_implied_2",
          "DialogueText": "Hehe... Kezdesz egyre jobb lenni!",
          "AudioPath": "assets/hu-HU/Leo/37.ogg"
        },
        {
          "DialogueFrom": "Event:IslandNorth/6497421/e 6497423/f Leo 1000/w sunny/t 600 1800/Hl leoMoved_implied_3",
          "DialogueText": "Nem#Talán...",
          "AudioPath": "assets/hu-HU/Leo/38.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved",
          "DialogueText": "El... elhagynám a szigetet?",
          "AudioPath": "assets/hu-HU/Leo/39.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_1",
          "DialogueText": "Má... más gyerekek?",
          "AudioPath": "assets/hu-HU/Leo/40.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_2",
          "DialogueText": "Én...",
          "AudioPath": "assets/hu-HU/Leo/41.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_2_1",
          "DialogueText": "Én megyek...",
          "AudioPath": "assets/hu-HU/Leo/42.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_3",
          "DialogueText": "De... a családom...",
          "AudioPath": "assets/hu-HU/Leo/43.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_4",
          "DialogueText": "Azt mondja...",
          "AudioPath": "assets/hu-HU/Leo/44.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_5",
          "DialogueText": "Azt mondja, hogy minden papagáj örül nekem.",
          "AudioPath": "assets/hu-HU/Leo/45.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_5_1",
          "DialogueText": "...És azt akarják, hogy új fejezetet nyissak az életemben, bárhová is vezessen az...",
          "AudioPath": "assets/hu-HU/Leo/46.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_6",
          "DialogueText": "És nem számít, hogy mi történik... ők mindig is a családom maradnak... örökké.",
          "AudioPath": "assets/hu-HU/Leo/47.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_7",
          "DialogueText": "Készen állok!",
          "AudioPath": "assets/hu-HU/Leo/48.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_alt",
          "DialogueText": "Viszlát!",
          "AudioPath": "assets/hu-HU/Leo/49.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_alt_1",
          "DialogueText": "Új ember érkezett a völgybe...",
          "AudioPath": "assets/hu-HU/Leo/50.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_alt_2",
          "DialogueText": "Úgy tűnik kis városunk gyarapodik!",
          "AudioPath": "assets/hu-HU/Leo/51.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved",
          "DialogueText": "Mit csinálsz, ?",
          "AudioPath": "assets/hu-HU/Leo/52.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_1",
          "DialogueText": "?",
          "AudioPath": "assets/hu-HU/Leo/53.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_2",
          "DialogueText": "Mesélj arról a helyről, ahonnan jöttél... az otthonodról.",
          "AudioPath": "assets/hu-HU/Leo/54.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_3",
          "DialogueText": "Van... vannak... más gyerekek?",
          "AudioPath": "assets/hu-HU/Leo/55.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_4",
          "DialogueText": "...Tudom, hogy valójában nem vagyok madár, ...",
          "AudioPath": "assets/hu-HU/Leo/56.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_4_1",
          "DialogueText": "Olyan vagyok, mint te.",
          "AudioPath": "assets/hu-HU/Leo/57.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_4_2",
          "DialogueText": "De nem szeretek erre gondolni...",
          "AudioPath": "assets/hu-HU/Leo/58.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_4_3",
          "DialogueText": "...mert olyan magányos vagyok.",
          "AudioPath": "assets/hu-HU/Leo/59.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_5",
          "DialogueText": "A madár családommal úgy érzem, hogy valahová tartozom.",
          "AudioPath": "assets/hu-HU/Leo/60.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_6",
          "DialogueText": "De... mégse tartozom.",
          "AudioPath": "assets/hu-HU/Leo/61.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_6_1",
          "DialogueText": "...sehová se tartozom...",
          "AudioPath": "assets/hu-HU/Leo/62.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied",
          "DialogueText": "Hullámok...",
          "AudioPath": "assets/hu-HU/Leo/63.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_1",
          "DialogueText": "A többi madár azt mondja, hogy Leo azért nem tud repülni... mert Leo túl sok időt töltött a hullámokban, még régen.",
          "AudioPath": "assets/hu-HU/Leo/64.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_2",
          "DialogueText": "Most a szárnyaim tele vannak vízzel.",
          "AudioPath": "assets/hu-HU/Leo/65.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_1",
          "DialogueText": "Halak? Könnyű...",
          "AudioPath": "assets/hu-HU/Leo/66.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_1_1",
          "DialogueText": "Ha 'köszönni' akarsz, csak kérdezz.",
          "AudioPath": "assets/hu-HU/Leo/67.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_10",
          "DialogueText": "Azt gondolom, hogy... okos.",
          "AudioPath": "assets/hu-HU/Leo/68.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_2",
          "DialogueText": "Hmm... Ezt még nem értem.",
          "AudioPath": "assets/hu-HU/Leo/69.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_2_1",
          "DialogueText": "Talán majd ha én is idősebb leszek.",
          "AudioPath": "assets/hu-HU/Leo/70.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_3",
          "DialogueText": "Óh... Igen. Leo is a nagy kérdéseken elmélkedik.",
          "AudioPath": "assets/hu-HU/Leo/71.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_4",
          "DialogueText": "Mint például... 'hol találhatok több diót, a strandon, vagy a vulkánnál?'",
          "AudioPath": "assets/hu-HU/Leo/72.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_5",
          "DialogueText": "*sóhaj*...az élet bonyolult...",
          "AudioPath": "assets/hu-HU/Leo/73.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_6",
          "DialogueText": "Emberek...",
          "AudioPath": "assets/hu-HU/Leo/74.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_7",
          "DialogueText": "Remekül hangzik.",
          "AudioPath": "assets/hu-HU/Leo/75.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_8",
          "DialogueText": "Hidegebb? ...Nem értem...",
          "AudioPath": "assets/hu-HU/Leo/76.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_9",
          "DialogueText": "Farm? Vagyis te termeszted a saját dióidat? Nem kell keresgélned azokat?",
          "AudioPath": "assets/hu-HU/Leo/77.ogg"
        },
        {
          "DialogueFrom": "Event:IslandWest/6497423/e 1039573/f Leo 500/w sunny/t 600 1800/Hl leoMoved_implied_9_1",
          "DialogueText": "...Váó..",
          "AudioPath": "assets/hu-HU/Leo/78.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8959199/w sunny/t 600 1900/e 6497428/f Leo 2250/F",
          "DialogueText": "Haha! ...Ez nagyszerű nap volt!",
          "AudioPath": "assets/hu-HU/Leo/79.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8959199/w sunny/t 600 1900/e 6497428/f Leo 2250/F_1",
          "DialogueText": "Tudod... Hiányzik a szigeten lévő családom...",
          "AudioPath": "assets/hu-HU/Leo/80.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8959199/w sunny/t 600 1900/e 6497428/f Leo 2250/F_2",
          "DialogueText": "De legtöbbször örülök az emlékeimnek, és nem szomorkodom.",
          "AudioPath": "assets/hu-HU/Leo/81.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8959199/w sunny/t 600 1900/e 6497428/f Leo 2250/F_3",
          "DialogueText": "...és tényleg úgy érzem, hogy ide tartozom!",
          "AudioPath": "assets/hu-HU/Leo/82.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8959199/w sunny/t 600 1900/e 6497428/f Leo 2250/F_4",
          "DialogueText": "Ez egy csodálatos világ, barátaim...",
          "AudioPath": "assets/hu-HU/Leo/83.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8959199/w sunny/t 600 1900/e 6497428/f Leo 2250/F_5",
          "DialogueText": "...és boldog vagyok, hogy részese lehetek!",
          "AudioPath": "assets/hu-HU/Leo/84.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8959199/w sunny/t 600 1900/e 6497428/f Leo 2250/F_alt",
          "DialogueText": "Elkészült!",
          "AudioPath": "assets/hu-HU/Leo/85.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue",
          "DialogueText": "Úgy tűnik, hogy életben fogsz maradni.",
          "AudioPath": "assets/hu-HU/Leo/86.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue_1",
          "DialogueText": "A madár barátaim megláttak téged, és mondták, hogy bajban vagy, ezért egész idáig húztalak.",
          "AudioPath": "assets/hu-HU/Leo/87.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue_2",
          "DialogueText": "Kérlek légy óvatos odakint. Nem akarok többé szomorú lenni...",
          "AudioPath": "assets/hu-HU/Leo/88.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Haha! Még sose láttam ehhez foghatót...",
          "AudioPath": "assets/hu-HU/Leo/89.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Összebarátkozom ezekkel a madarakkal!",
          "AudioPath": "assets/hu-HU/Leo/90.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "rezzenéstelen tekintete a képernyőre tapadt. Nem tudod elterelni a figyelmét.",
          "AudioPath": "assets/hu-HU/Leo/91.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Ez egy útvesztő az útvesztőben... Forog a fejem...",
          "AudioPath": "assets/hu-HU/Leo/92.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "A tojásvadászat gondolata kissé fura... De azt hiszem a tojások nem a családtagjaimhoz tartoznak...Na, mindegy, azt hiszem csatlakozok!",
          "AudioPath": "assets/hu-HU/Leo/93.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "Itt nagyon szépek a virágok, de nem olyan színesek, mint az otthonomban!Azon gondolkozom, hogy vajon egyszer én is odamehetek és táncolhatok majd...",
          "AudioPath": "assets/hu-HU/Leo/94.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "Egy társra van szükséged ahhoz, hogy részt vehess a táncban? Egy madár is megteszi?",
          "AudioPath": "assets/hu-HU/Leo/95.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Shhh... A tökéletes sült titkos receptjét tanulmányozom.",
          "AudioPath": "assets/hu-HU/Leo/96.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "Az otthonom arra van, az óceánon túl. Olyan közelinek érződik, mégis oly távol van.Remélem a barátaim jól vannak.",
          "AudioPath": "assets/hu-HU/Leo/97.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "A holdfény medúzák régi barátaim... a szigethez is évente eljönnek.",
          "AudioPath": "assets/hu-HU/Leo/98.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "A régi otthoni barátaim hamarosan meglátogatnak minket!",
          "AudioPath": "assets/hu-HU/Leo/99.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Linus kellemetlenül érzi magát, így nem csatlakozik, szóval majd én leszek a társasága.Majd rendezünk magunknak egy kis lakomát... zúzott téli gyökeret és fűszeres bogyós halat!",
          "AudioPath": "assets/hu-HU/Leo/100.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "Azt se tudom, hogy egyáltalán mik ezek... de jó az ízük!",
          "AudioPath": "assets/hu-HU/Leo/101.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Egy jégkunyhó kétszer olyan meleg lehet, ha a falait tollakkal borítod be!",
          "AudioPath": "assets/hu-HU/Leo/102.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "*ropog* *ropog* A hó olyan mókás hangot ad ki mikor rálépsz!",
          "AudioPath": "assets/hu-HU/Leo/103.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "Ezt szeretem. Berakom a fészkembe.",
          "AudioPath": "assets/hu-HU/Leo/104.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Biztos vagy benne? Ez olyan jó...",
          "AudioPath": "assets/hu-HU/Leo/105.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Ó... Nem tudom, hogy ezt meg tudom-e emészteni...",
          "AudioPath": "assets/hu-HU/Leo/106.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "*szimat* *szimat*... Miért adod ezt nekem? Azt akarod, hogy elássam neked?",
          "AudioPath": "assets/hu-HU/Leo/107.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Elfogadom az ajándékodat.",
          "AudioPath": "assets/hu-HU/Leo/108.ogg"
        },
        {
          "DialogueFrom": "Leo_Memory",
          "DialogueText": "Hé... Visszaemlékeztem azokra az időkre, amikor még a szigeten voltunk... Emlékszel?",
          "AudioPath": "assets/hu-HU/Leo/109.ogg"
        },
        {
          "DialogueFrom": "Leo_Memory_6497421",
          "DialogueText": "Amikor a madarakról beszélgettünk?",
          "AudioPath": "assets/hu-HU/Leo/110.ogg"
        },
        {
          "DialogueFrom": "Leo_Memory_6497423",
          "DialogueText": "Amikor az otthonodról beszélgettünk?",
          "AudioPath": "assets/hu-HU/Leo/111.ogg"
        },
        {
          "DialogueFrom": "Leo_Memory_6497428",
          "DialogueText": "Mikor úgy döntöttem, hogy költözök?",
          "AudioPath": "assets/hu-HU/Leo/112.ogg"
        },
        {
          "DialogueFrom": "Leo_Memory_Answer_No",
          "DialogueText": "Elfoglalt vagyok",
          "AudioPath": "assets/hu-HU/Leo/113.ogg"
        },
        {
          "DialogueFrom": "Leo_Memory_Answer_Yes",
          "DialogueText": "Emlékeztess",
          "AudioPath": "assets/hu-HU/Leo/114.ogg"
        }
      ]
    }
  ]
}