{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Governor.hu-HU",
      "VoicePackName": "Dictionary (Governor - hu-HU)",
      "Character": "Governor",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Természetesen! Egész évben erre vártam.",
          "AudioPath": "assets/hu-HU/Governor/1.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "...",
          "AudioPath": "assets/hu-HU/Governor/2.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_10",
          "DialogueText": "Öhm... Ez valójában kissé undorító. Azt hiszem idén kihagyom a levest.",
          "AudioPath": "assets/hu-HU/Governor/3.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_11",
          "DialogueText": "Pföjj! Ez gusztustalan! Azt hiszem rosszul leszek...",
          "AudioPath": "assets/hu-HU/Governor/4.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_12",
          "DialogueText": "Le kell ülnöm...",
          "AudioPath": "assets/hu-HU/Governor/5.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_13",
          "DialogueText": "Természetese igen! Egész délután a levessel szemezgettem.",
          "AudioPath": "assets/hu-HU/Governor/6.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_15",
          "DialogueText": "Milyen pompás alkalom... Mindig nagy öröm meglátogatni Stardew Valleyt.Beszélnem kell az asszonnyal, hogy vegyünk itt egy nyaralót.",
          "AudioPath": "assets/hu-HU/Governor/7.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_16",
          "DialogueText": "Minden évben buzgón várom a Stardew Valleybe történő látogatásomat. Érezni a kedvességet a levegőben!",
          "AudioPath": "assets/hu-HU/Governor/8.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_2",
          "DialogueText": "Hmm... Kissé fura a szaga... de az íze valójában egész jó!",
          "AudioPath": "assets/hu-HU/Governor/9.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_3",
          "DialogueText": "Várjunk csak... van valami az edényemben... ez mégis micsoda?",
          "AudioPath": "assets/hu-HU/Governor/10.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_4",
          "DialogueText": "Ez... Ez botrány!Még soha nem aláztak meg engem ennyire!",
          "AudioPath": "assets/hu-HU/Governor/11.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_5",
          "DialogueText": "Pföjjj... kezd feldagadni a nyelvem... Azt hiszem rosszul leszek.",
          "AudioPath": "assets/hu-HU/Governor/12.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_6",
          "DialogueText": "Hmmm... Ez nem is rossz, de hiányzik belőle valami...Mindenki rakott alapanyagot a levesbe? Úgy érzem mintha hiányozna valakinek az egyedisége belőle.",
          "AudioPath": "assets/hu-HU/Governor/13.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_7",
          "DialogueText": "Ó egek... ez a legjobb leves, amit valaha is kóstoltam!",
          "AudioPath": "assets/hu-HU/Governor/14.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_8",
          "DialogueText": "Ah... micsoda kellemes leves. A völgy terméseiben még nem csalódtam!",
          "AudioPath": "assets/hu-HU/Governor/15.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_9",
          "DialogueText": "Hmm... Nem tudok sokat mondani erről. Ez egy átlagos leves.",
          "AudioPath": "assets/hu-HU/Governor/16.ogg"
        }
      ]
    }
  ]
}