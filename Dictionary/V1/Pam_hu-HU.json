        },
        {
          "DialogueFrom": "Event:Town/choseToBeKnown_3",
          "DialogueText": "Bármit megteszek, amit mondasz...",
          "AudioPath": "assets/hu-HU/Pam/143.ogg"
        },
        {
          "DialogueFrom": "Event:Town/choseToBeKnown_4",
          "DialogueText": "Köszönöm, kölyök.",
          "AudioPath": "assets/hu-HU/Pam/144.ogg"
        },
        {
          "DialogueFrom": "Event:Town/choseToBeKnown_4_1",
          "DialogueText": "Mostantól kezdve úgy fogunk élni, mint egy igazi család... hallasz engem?",
