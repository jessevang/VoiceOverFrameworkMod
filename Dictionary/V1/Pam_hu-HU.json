{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Pam.hu-HU",
      "VoicePackName": "Dictionary (Pam - hu-HU)",
      "Character": "Pam",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "A teringettét... Soha nem számítottam ilyen ajándékra. Feldobtad a napomat, kölyök!",
          "AudioPath": "assets/hu-HU/Pam/1.ogg"
        },
        {
          "DialogueFrom": "cc_Bus",
          "DialogueText": "Hé, hallottad mi történt a régi busszal?",
          "AudioPath": "assets/hu-HU/Pam/2.ogg"
        },
        {
          "DialogueFrom": "cc_Bus_1",
          "DialogueText": "Bizony... Ismét nyeregben vagyok, kölyök... hehe. Jó érzés.",
          "AudioPath": "assets/hu-HU/Pam/3.ogg"
        },
        {
          "DialogueFrom": "divorced_Penny",
          "DialogueText": "Penny sírva jött haza... mit tettél vele?",
          "AudioPath": "assets/hu-HU/Pam/4.ogg"
        },
        {
          "DialogueFrom": "event_credit1",
          "DialogueText": "Hé, vigyázz, hogy beszélsz! Senki se mondhatja meg nekem, hogy mit tegyek!",
          "AudioPath": "assets/hu-HU/Pam/5.ogg"
        },
        {
          "DialogueFrom": "event_credit2",
          "DialogueText": "...nem? Ez szörnyű!",
          "AudioPath": "assets/hu-HU/Pam/6.ogg"
        },
        {
          "DialogueFrom": "event_credit2_1",
          "DialogueText": "...Oh.",
          "AudioPath": "assets/hu-HU/Pam/7.ogg"
        },
        {
          "DialogueFrom": "fall_Fri",
          "DialogueText": "Minden nap ugyanaz a régi rutin megy...",
          "AudioPath": "assets/hu-HU/Pam/8.ogg"
        },
        {
          "DialogueFrom": "fall_Fri_1",
          "DialogueText": "Mondd csak gazda, neked nincs ilyen problémád?",
          "AudioPath": "assets/hu-HU/Pam/9.ogg"
        },
        {
          "DialogueFrom": "fall_Sat",
          "DialogueText": "Lennél kedves és kitakarítanád a házamat?",
          "AudioPath": "assets/hu-HU/Pam/10.ogg"
        },
        {
          "DialogueFrom": "fall_Sat_1",
          "DialogueText": "Csak vicceltem!",
          "AudioPath": "assets/hu-HU/Pam/11.ogg"
        },
        {
          "DialogueFrom": "fall_Sun",
          "DialogueText": "Nincs mit dolgoznod?",
          "AudioPath": "assets/hu-HU/Pam/12.ogg"
        },
        {
          "DialogueFrom": "fall_Sun_1",
          "DialogueText": "Hálásnak kellene lenned, hogy van munkád, ahelyett, hogy egész nap a városban mászkálsz.",
          "AudioPath": "assets/hu-HU/Pam/13.ogg"
        },
        {
          "DialogueFrom": "fall_Sun4",
          "DialogueText": "Hiányzik a régi vezetős melóm. Régen mindig körbejártam a völgyet, találkoztam másokkal, és hallgattam a rádiót.",
          "AudioPath": "assets/hu-HU/Pam/14.ogg"
        },
        {
          "DialogueFrom": "fall_Sun4_1",
          "DialogueText": "Mennyire változnak az idők.",
          "AudioPath": "assets/hu-HU/Pam/15.ogg"
        },
        {
          "DialogueFrom": "fall_Thu",
          "DialogueText": "Penny az egyetlen hozzátartozóm, szóval jobban teszed, ha kedves vagy hozzá!",
          "AudioPath": "assets/hu-HU/Pam/16.ogg"
        },
        {
          "DialogueFrom": "fall_Tue",
          "DialogueText": "Ha jobban ismernélek, akkor lehet többet beszélgethetnénk.",
          "AudioPath": "assets/hu-HU/Pam/17.ogg"
        },
        {
          "DialogueFrom": "fall_Tue4",
          "DialogueText": "Valaha én is fiatal voltam. Nehéz elhinni, mi?",
          "AudioPath": "assets/hu-HU/Pam/18.ogg"
        },
        {
          "DialogueFrom": "fall_Tue4_1",
          "DialogueText": "Az idő tényleg gyorsan elmúlik, ha felnősz.",
          "AudioPath": "assets/hu-HU/Pam/19.ogg"
        },
        {
          "DialogueFrom": "fall_Wed",
          "DialogueText": "*sóhaj*... Azt hiszem ma este egy fagyos kaját fogok berakni a mikróba.",
          "AudioPath": "assets/hu-HU/Pam/20.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Minden nap pont olyan, mint az előtte lévő...",
          "AudioPath": "assets/hu-HU/Pam/21.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Bárcsak gazdagnak születtem volna...",
          "AudioPath": "assets/hu-HU/Pam/22.ogg"
        },
        {
          "DialogueFrom": "Fri2",
          "DialogueText": "Tudod, gondolkoztam... Bárcsak lenne egy hobbim. Valami más, nem olyan, mint most, hogy egész este a szalonban lógok.",
          "AudioPath": "assets/hu-HU/Pam/23.ogg"
        },
        {
          "DialogueFrom": "Fri2_1",
          "DialogueText": "Van ötleted?",
          "AudioPath": "assets/hu-HU/Pam/24.ogg"
        },
        {
          "DialogueFrom": "Fri2_2",
          "DialogueText": "...Ehh. Lehet dámázni fogok saját magam ellen.",
          "AudioPath": "assets/hu-HU/Pam/25.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Ez egy jel a Mindenhatótól... Végünk van...",
          "AudioPath": "assets/hu-HU/Pam/26.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "Azt hiszem ezek az esők a hatalom jelei... egy figyelmeztetés.",
          "AudioPath": "assets/hu-HU/Pam/27.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2_1",
          "DialogueText": "Mondhatod, hogy ez a természettől származik, vagy Yobatól, vagy akármitől... de én másképp gondolom.",
          "AudioPath": "assets/hu-HU/Pam/28.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2_2",
          "DialogueText": "Arra gondolok, hogy nem lehetünk beképzeltek. Különben a zöld eső helyett talán vérvörös eső fog érkezni... Érted, hogy mire célzok?",
          "AudioPath": "assets/hu-HU/Pam/29.ogg"
        },
        {
          "DialogueFrom": "GreenRainFinished",
          "DialogueText": "Nos, azt hiszem mégse jött el a végítélet, kölyök...",
          "AudioPath": "assets/hu-HU/Pam/30.ogg"
        },
        {
          "DialogueFrom": "GreenRainFinished_1",
          "DialogueText": "Áh, sebaj, vissza a mókuskerékbe...",
          "AudioPath": "assets/hu-HU/Pam/31.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "Hé kölyök. A nevem Pam.",
          "AudioPath": "assets/hu-HU/Pam/32.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "Ne szemétkedj és remekül megleszünk.",
          "AudioPath": "assets/hu-HU/Pam/33.ogg"
        },
        {
          "DialogueFrom": "married_Alex",
          "DialogueText": "Szóval hozzámentél a Mullner fiúhoz, mi?",
          "AudioPath": "assets/hu-HU/Pam/34.ogg"
        },
        {
          "DialogueFrom": "married_Alex_1",
          "DialogueText": "Akkor azt a büdös kutyát is magaddal fogod vinni, ugye?",
          "AudioPath": "assets/hu-HU/Pam/35.ogg"
        },
        {
          "DialogueFrom": "married_Penny",
          "DialogueText": "Nagyon hiányzik az a lány...",
          "AudioPath": "assets/hu-HU/Pam/36.ogg"
        },
        {
          "DialogueFrom": "married_Penny_1",
          "DialogueText": "Remélem jól bánsz az én Pennymmel, értve vagyok?",
          "AudioPath": "assets/hu-HU/Pam/37.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Uhhhhh... a fejem...",
          "AudioPath": "assets/hu-HU/Pam/38.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Tudod, ennék én egészséges kajákat, ha volna rá pénzem.",
          "AudioPath": "assets/hu-HU/Pam/39.ogg"
        },
        {
          "DialogueFrom": "Mon_2",
          "DialogueText": "Hé, neked biztos van pár ízletes kaja a farmodon, nem?",
          "AudioPath": "assets/hu-HU/Pam/40.ogg"
        },
        {
          "DialogueFrom": "MovieInvitation",
          "DialogueText": "Randiest az öreg Pammel, mi? Hah!",
          "AudioPath": "assets/hu-HU/Pam/41_male.ogg"
        },
        {
          "DialogueFrom": "MovieInvitation_1",
          "DialogueText": "Csajos est az öreg Pammel, mi? Hah!",
          "AudioPath": "assets/hu-HU/Pam/42_female.ogg"
        },
        {
          "DialogueFrom": "MovieInvitation_2",
          "DialogueText": "Hogyne kölyök... Majd ott találkozunk.",
          "AudioPath": "assets/hu-HU/Pam/43.ogg"
        },
        {
          "DialogueFrom": "pamHouseUpgrade",
          "DialogueText": "El se hiszem, hogy vettél nekem egy új házat! Van valami trükk a dologban?",
          "AudioPath": "assets/hu-HU/Pam/44.ogg"
        },
        {
          "DialogueFrom": "pamHouseUpgrade_1",
          "DialogueText": "Sajnálom, nem vagyok ahhoz szokva, hogy ilyen kedvesek az emberek...",
          "AudioPath": "assets/hu-HU/Pam/45.ogg"
        },
        {
          "DialogueFrom": "pamHouseUpgrade_memory_oneweek",
          "DialogueText": "Szeretem az új házat. Köszönöm, kölyök!",
          "AudioPath": "assets/hu-HU/Pam/46.ogg"
        },
        {
          "DialogueFrom": "pamHouseUpgradeAnonymous",
          "DialogueText": "El se hiszem, hogy valaki vett nekem egy új házat! Vajon ez talán valamilyen átverés?",
          "AudioPath": "assets/hu-HU/Pam/47.ogg"
        },
        {
          "DialogueFrom": "reject_867",
          "DialogueText": "Váó, ezt nézd... Olyan, mint valami, ami a jövőből jött.",
          "AudioPath": "assets/hu-HU/Pam/48.ogg"
        },
        {
          "DialogueFrom": "reject_867_1",
          "DialogueText": "Hehe, bocsi, kölyök... Azt nem használhatom. Több gomb van rajta, mint ahány agysejtem van!",
          "AudioPath": "assets/hu-HU/Pam/49.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Heh... Willy mindenkinek adott egy kis halszeletet. Nagyon ízletes!",
          "AudioPath": "assets/hu-HU/Pam/50.ogg"
        },
        {
          "DialogueFrom": "Resort_Bar",
          "DialogueText": "Ez egy különleges alkalom, kölyök... Valami egyedit rendelek. Legyen kettő!",
          "AudioPath": "assets/hu-HU/Pam/51.ogg"
        },
        {
          "DialogueFrom": "Resort_Chair",
          "DialogueText": "Ez ám a kényelem! Talán a végén még a cipőmet is leveszem!",
          "AudioPath": "assets/hu-HU/Pam/52.ogg"
        },
        {
          "DialogueFrom": "Resort_Chair_1",
          "DialogueText": "Jobb, ha nem nézegeted a lábaimat, kölyök.",
          "AudioPath": "assets/hu-HU/Pam/53.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Úgy tűnik, hogy pár egyedi hal úszkál errefelé. Legközelebb el kell hoznom a horgászbotomat!",
          "AudioPath": "assets/hu-HU/Pam/54.ogg"
        },
        {
          "DialogueFrom": "Saloon",
          "DialogueText": "Nincs is jobb a régi kis itókánál, ami felmelegíti ezeket a vén csontokat...",
          "AudioPath": "assets/hu-HU/Pam/55.ogg"
        },
        {
          "DialogueFrom": "Saloon_Entry",
          "DialogueText": "Ah... Végre./Innom kell./Gus! Adj a szokásosból!/Hé, visszatértem!/Óóh öregem, de megszomjaztam!/Kiszáradtam!",
          "AudioPath": "assets/hu-HU/Pam/56.ogg"
        },
        {
          "DialogueFrom": "Saloon_Fri",
          "DialogueText": "Hékás, Gus! Adj még egy korsót a legerősebb italodból!",
          "AudioPath": "assets/hu-HU/Pam/57.ogg"
        },
        {
          "DialogueFrom": "Saloon_Sat",
          "DialogueText": "nem reagál.",
          "AudioPath": "assets/hu-HU/Pam/58.ogg"
        },
        {
          "DialogueFrom": "Saloon_Sun",
          "DialogueText": "tűnik Pam nincs magánál. Jobb, ha magára hagyod őt.",
          "AudioPath": "assets/hu-HU/Pam/59.ogg"
        },
        {
          "DialogueFrom": "Saloon_Thu",
          "DialogueText": "*szürcs* ... Egész délután csak erre az italra vártam. Heh heh.",
          "AudioPath": "assets/hu-HU/Pam/60.ogg"
        },
        {
          "DialogueFrom": "Saloon_Tue",
          "DialogueText": "Hé Gus! Adj még egy kört! *hukk*",
          "AudioPath": "assets/hu-HU/Pam/61.ogg"
        },
        {
          "DialogueFrom": "Saloon_Wed",
          "DialogueText": "*Sóhaj*... Hé kölyök. Túl fiatal vagy még ahhoz, hogy ilyen helyen mászkálj.",
          "AudioPath": "assets/hu-HU/Pam/62.ogg"
        },
        {
          "DialogueFrom": "Saloon6",
          "DialogueText": "Hé kölyök. Vennék neked egy italt, ha lenne rá pénzem!",
          "AudioPath": "assets/hu-HU/Pam/63.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Legyen tiszta a cipőd, mielőtt a házamban mászkálnál.",
          "AudioPath": "assets/hu-HU/Pam/64.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Nagyon idegesítő a mocsok feltakarítása. És ezt neked is tudnod kellene.",
          "AudioPath": "assets/hu-HU/Pam/65.ogg"
        },
        {
          "DialogueFrom": "Sat4",
          "DialogueText": "Bárcsak egy csapat manó eljönne az este, és kitakarítanák a házamat. Hahahaha.",
          "AudioPath": "assets/hu-HU/Pam/66.ogg"
        },
        {
          "DialogueFrom": "Sat4_1",
          "DialogueText": "Jól jönne most egy kis paszternák.",
          "AudioPath": "assets/hu-HU/Pam/67.ogg"
        },
        {
          "DialogueFrom": "Sat4_2",
          "DialogueText": "Ha a lábaim nem lennének ennyire merevek, akkor néha-néha felnéznék a hegyekbe.",
          "AudioPath": "assets/hu-HU/Pam/68.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Fish Pond",
          "DialogueText": "Hallottam, hogy van halastavad... Heh, én egy olyannal együtt nőttem fel.",
          "AudioPath": "assets/hu-HU/Pam/69.ogg"
        },
        {
          "DialogueFrom": "structureBuilt_Fish Pond_1",
          "DialogueText": "Abban szinte bármit tenyészthetsz... De azok a díszes halak elég nyűgösek tudnak lenni!",
          "AudioPath": "assets/hu-HU/Pam/70.ogg"
        },
        {
          "DialogueFrom": "summer_Fri",
          "DialogueText": "...Igen?",
          "AudioPath": "assets/hu-HU/Pam/71.ogg"
        },
        {
          "DialogueFrom": "summer_Fri_1",
          "DialogueText": "Elég szép fiatal ember vagy. Miért vesztegeted az idődet azzal, hogy egy magamfajta öreglánnyal beszélsz?",
          "AudioPath": "assets/hu-HU/Pam/72_male.ogg"
        },
        {
          "DialogueFrom": "summer_Fri_2",
          "DialogueText": "Elég szép fiatal lány vagy. Miért vesztegeted az idődet azzal, hogy egy magamfajtával beszélgetsz?",
          "AudioPath": "assets/hu-HU/Pam/73_female.ogg"
        },
        {
          "DialogueFrom": "summer_Sat",
          "DialogueText": "Valaki panaszkodott a kertemről... hogy csupa mocsok. Te voltál az?",
          "AudioPath": "assets/hu-HU/Pam/74.ogg"
        },
        {
          "DialogueFrom": "summer_Sat_1",
          "DialogueText": "Hmmm...",
          "AudioPath": "assets/hu-HU/Pam/75.ogg"
        },
        {
          "DialogueFrom": "summer_Sat2",
          "DialogueText": "Ne is figyelj arra a vén ebre, ami a ház mellett van.",
          "AudioPath": "assets/hu-HU/Pam/76.ogg"
        },
        {
          "DialogueFrom": "summer_Sat2_1",
          "DialogueText": "Lehet, hogy csúnyán néz rád, de sose tenné ki a lábát abból a kutyaólból.",
          "AudioPath": "assets/hu-HU/Pam/77.ogg"
        },
        {
          "DialogueFrom": "summer_Sat2_2",
          "DialogueText": "Hogy mi az öregnek a neve? Dusty.",
          "AudioPath": "assets/hu-HU/Pam/78.ogg"
        },
        {
          "DialogueFrom": "summer_Thu",
          "DialogueText": "Ó, üdv, gazda.",
          "AudioPath": "assets/hu-HU/Pam/79.ogg"
        },
        {
          "DialogueFrom": "summer_Thu_1",
          "DialogueText": "Nem beszélgetek csak úgy az idegenekkel.",
          "AudioPath": "assets/hu-HU/Pam/80.ogg"
        },
        {
          "DialogueFrom": "summer_Thu4",
          "DialogueText": "Valamiért idővel a szalonban lyukadok ki.",
          "AudioPath": "assets/hu-HU/Pam/81.ogg"
        },
        {
          "DialogueFrom": "summer_Thu4_1",
          "DialogueText": "Túl közel van a házamhoz. Ez nem egészséges.",
          "AudioPath": "assets/hu-HU/Pam/82.ogg"
        },
        {
          "DialogueFrom": "summer_Tue",
          "DialogueText": "Üdv, idegen.",
          "AudioPath": "assets/hu-HU/Pam/83.ogg"
        },
        {
          "DialogueFrom": "summer_Tue2",
          "DialogueText": "Ó, ez úr.",
          "AudioPath": "assets/hu-HU/Pam/84_male.ogg"
        },
        {
          "DialogueFrom": "summer_Tue2_1",
          "DialogueText": "Ó, ez asszony.",
          "AudioPath": "assets/hu-HU/Pam/85_female.ogg"
        },
        {
          "DialogueFrom": "summer_Tue2_2",
          "DialogueText": "Segítségre van szükséged, vagy mi az?",
          "AudioPath": "assets/hu-HU/Pam/86.ogg"
        },
        {
          "DialogueFrom": "summer_Tue6",
          "DialogueText": "Nekem is voltak olyan álmaim és reményeim, mint neked... De nézz csak rám!",
          "AudioPath": "assets/hu-HU/Pam/87.ogg"
        },
        {
          "DialogueFrom": "summer_Tue6_1",
          "DialogueText": "Az élet nem mindig alakul úgy, ahogyan szeretnéd.",
          "AudioPath": "assets/hu-HU/Pam/88.ogg"
        },
        {
          "DialogueFrom": "summer_Tue6_2",
          "DialogueText": "No mindegy. Nincs értelme azon agyalnod, hogy mid nincs.",
          "AudioPath": "assets/hu-HU/Pam/89.ogg"
        },
        {
          "DialogueFrom": "summer_Wed",
          "DialogueText": "Jót tesz a városunknak, hogy van egy helyi farmunk.",
          "AudioPath": "assets/hu-HU/Pam/90.ogg"
        },
        {
          "DialogueFrom": "summer_Wed4",
          "DialogueText": "Talán ha elég jól dolgozol a farmodon, az javíthatja a helyi gazdaságot is.",
          "AudioPath": "assets/hu-HU/Pam/91.ogg"
        },
        {
          "DialogueFrom": "summer_Wed4_1",
          "DialogueText": "Aztán mindenkinek lesz munkája, és mindannyian boldogok leszünk, ugye?",
          "AudioPath": "assets/hu-HU/Pam/92.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Te Gus barátja vagy? Ő egy jó fickó.",
          "AudioPath": "assets/hu-HU/Pam/93.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "A születésnapomon még ingyen sört is ad nekem.",
          "AudioPath": "assets/hu-HU/Pam/94.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Blahh... Egy kicsivel több karamellás portert ittam tegnap este a szalonban... Menj innét.",
          "AudioPath": "assets/hu-HU/Pam/95.ogg"
        },
        {
          "DialogueFrom": "Thu_1",
          "DialogueText": "Az otthonom nem szép, de legalább a folyó mellett van.",
          "AudioPath": "assets/hu-HU/Pam/96.ogg"
        },
        {
          "DialogueFrom": "Thu_2",
          "DialogueText": "Hékás. Penny az én egyetlen kicsi lányom. Légy kedves hozzá, vagy hagyd őt békén, értve vagyok?",
          "AudioPath": "assets/hu-HU/Pam/97.ogg"
        },
        {
          "DialogueFrom": "Thu4",
          "DialogueText": "Penny szerint túl sok időt töltök a szalonban...",
          "AudioPath": "assets/hu-HU/Pam/98.ogg"
        },
        {
          "DialogueFrom": "Thu4_inlaw_Penny",
          "DialogueText": "Roppant magányos itthon így egymagamban...",
          "AudioPath": "assets/hu-HU/Pam/99.ogg"
        },
        {
          "DialogueFrom": "Trailer_Entry",
          "DialogueText": "Penny! Csináltál vacsorát?/Penny! Kész vagy a feladataiddal?/Penny! Itthon vagy?/Hé! Megjöttem!",
          "AudioPath": "assets/hu-HU/Pam/100.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "Hé te! Ne mászkálj a lakókocsimban, ha nem vagyok otthon. Értve vagyok?",
          "AudioPath": "assets/hu-HU/Pam/101.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "Elnézést, de óvatosan kell bánnom az idegenekkel.",
          "AudioPath": "assets/hu-HU/Pam/102.ogg"
        },
        {
          "DialogueFrom": "Tue2",
          "DialogueText": "Ma reggel újságot olvastam, de eléggé letaglózott. Ez egy rothadt világ, kölyök.",
          "AudioPath": "assets/hu-HU/Pam/103.ogg"
        },
        {
          "DialogueFrom": "Tue2_1",
          "DialogueText": "Tartsd a fejed a nyakadon, és mindent átvészelhetsz...",
          "AudioPath": "assets/hu-HU/Pam/104.ogg"
        },
        {
          "DialogueFrom": "Tue2_2",
