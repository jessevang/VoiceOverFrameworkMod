        },
        {
          "DialogueFrom": "Event:Trailer_Big/positive_alt",
          "DialogueText": "Köszönöm, hogy meghallgattál, kölyök.",
          "AudioPath": "assets/hu-HU/Pam/180.ogg"
        },
