{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.MrQi.en",
      "VoicePackName": "Dictionary (MrQi - en)",
      "Character": "MrQi",
      "Language": "en",
      "Entries": [
        {
          "DialogueFrom": "Event:QiNutRoom/10040609",
          "DialogueText": "Well, well, well... Look who's made it to my secret walnut room.",
          "AudioPath": "assets/en/MrQi/1.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1",
          "DialogueText": "I'm truly impressed.",
          "AudioPath": "assets/en/MrQi/2.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_1",
          "DialogueText": "Even though you only needed a fraction of the walnuts to gain full access to the island, you decided to push yourself further.",
          "AudioPath": "assets/en/MrQi/3.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_2",
          "DialogueText": "Your desire to enter this secret door was so strong, that you went above and beyond to get inside.",
          "AudioPath": "assets/en/MrQi/4.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_3",
          "DialogueText": "That drive is what sets you apart, kid. You've got a very special energy.",
          "AudioPath": "assets/en/MrQi/5.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2",
          "DialogueText": "That being said...",
          "AudioPath": "assets/en/MrQi/6.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2_1",
          "DialogueText": "You weren't under the impression that the challenge ended here, were you?",
          "AudioPath": "assets/en/MrQi/7.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2_2",
          "DialogueText": "No... Hah. Hah. Hah... In fact, the challenge has just begun.",
          "AudioPath": "assets/en/MrQi/8.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_3",
          "DialogueText": "On your left, you'll find a board with some very interesting opportunities.",
          "AudioPath": "assets/en/MrQi/9.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_3_1",
          "DialogueText": "These challenges, which I've designed just for you, will push you to your very limit.",
          "AudioPath": "assets/en/MrQi/10.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_4",
          "DialogueText": "However, if you can successfully conquer my challenges, you'll earn an exclusive currency... I call them 'Qi Gems'",
          "AudioPath": "assets/en/MrQi/11.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_5",
          "DialogueText": "Just don't put 'em in your mouth, kid... They're highly radioactive. Heh. heh. heh...",
          "AudioPath": "assets/en/MrQi/12.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_6",
          "DialogueText": "You can use the machine on your right to trade these Qi Gems for rare and powerful rewards.",
          "AudioPath": "assets/en/MrQi/13.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_7",
          "DialogueText": "Sounds interesting, doesn't it?",
          "AudioPath": "assets/en/MrQi/14.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_7_1",
          "DialogueText": "Yes... I have a feeling someone like you will enjoy this, very much...",
          "AudioPath": "assets/en/MrQi/15.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_8",
          "DialogueText": "Now, if you'll excuse me... I have important business to attend to.",
          "AudioPath": "assets/en/MrQi/16.ogg"
        }
      ]
    }
  ]
}