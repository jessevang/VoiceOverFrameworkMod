      "Entries": [
        {
          "DialogueFrom": "AcceptBirthdayGift_Negative",
          "DialogueText": "Un regalo di compleanno? Oh... non è molto divertente.",
          "AudioPath": "assets/it-IT/Vincent/1.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive",
          "DialogueText": "È un regalo di compleanno? Posso aprirlo subito?",
          "AudioPath": "assets/it-IT/Vincent/2.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive_1",
          "DialogueText": "Oh, wow! Proprio quello che volevo! Yayayaya!",
          "AudioPath": "assets/it-IT/Vincent/3.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Wow! Com'è dolce! Mmmmm....",
          "AudioPath": "assets/it-IT/Vincent/4.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(TR)FrogEgg",
          "DialogueText": "*squish* *squish* ... Bleeeaah! Me lo dai? Me lo dai? Evvaaaiii!",
          "AudioPath": "assets/it-IT/Vincent/5.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Negative_alcohol_item",
          "DialogueText": "*annusa*... che roba è? Puzza tantissimo... Mammaaaaa!!!",
          "AudioPath": "assets/it-IT/Vincent/6.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Bleah, stasera ci sono di nuovo le barbabietole bollite...",
          "AudioPath": "assets/it-IT/Vincent/7.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Oh no... mamma stasera fa la zuppa di lenticchie.",
          "AudioPath": "assets/it-IT/Vincent/8.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Mamma! Voglio uscire a giocare!",
          "AudioPath": "assets/it-IT/Vincent/9.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "è coperto di muschio.",
          "AudioPath": "assets/it-IT/Vincent/10.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "Oh, tu non sei di qui! Mi chiamo Vincent.",
          "AudioPath": "assets/it-IT/Vincent/11.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "Mamma dice di non parlare con gli estranei... Ma tu sembri a posto.",
          "AudioPath": "assets/it-IT/Vincent/12.ogg"
        },
        {
          "DialogueFrom": "married_Sam",
          "DialogueText": "Non portarmi via il mio Sammy!",
          "AudioPath": "assets/it-IT/Vincent/13.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Da grande voglio essere proprio come il mio fratellone!",
          "AudioPath": "assets/it-IT/Vincent/14.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Ok! Ciao!",
          "AudioPath": "assets/it-IT/Vincent/15.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "kent#Sono contento che papà sia tornato... ma è diverso da come lo ricordavo...|Papà sta sempre in giro... Mi manca giocare con lui.",
          "AudioPath": "assets/it-IT/Vincent/16.ogg"
        },
        {
          "DialogueFrom": "reject_869",
          "DialogueText": "Oooh! Che forte!",
          "AudioPath": "assets/it-IT/Vincent/17.ogg"
        },
        {
          "DialogueFrom": "reject_869_1",
          "DialogueText": "Mamma non me lo lascerebbe tenere, però...",
          "AudioPath": "assets/it-IT/Vincent/18.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Se guardi bene, si vedono degli insettini che saltano fuori dalla sabbia.",
          "AudioPath": "assets/it-IT/Vincent/19.ogg"
        },
        {
          "DialogueFrom": "Resort_Entering",
          "DialogueText": "È stato divertente! Da grande farò il capitano di una barca!",
          "AudioPath": "assets/it-IT/Vincent/20.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Voglio ritornare presto!",
          "AudioPath": "assets/it-IT/Vincent/21.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Voglio vedere uno squalo!",
          "AudioPath": "assets/it-IT/Vincent/22.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "La signorina Penny mi fa leggere un libro nuovo alla settimana.",
          "AudioPath": "assets/it-IT/Vincent/23.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Almeno li lascia scegliere a me, certe volte!",
          "AudioPath": "assets/it-IT/Vincent/24.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Presto tornerà papà!",
          "AudioPath": "assets/it-IT/Vincent/25.ogg"
        },
        {
          "DialogueFrom": "Sat_1_1",
          "DialogueText": "Spero che mi porti dei giocattoli.",
          "AudioPath": "assets/it-IT/Vincent/26.ogg"
        },
        {
          "DialogueFrom": "Sat8",
          "DialogueText": "kent#Papà sembra strano da quando è tornato. Cosa gli sarà successo?",
          "AudioPath": "assets/it-IT/Vincent/27.ogg"
        },
        {
          "DialogueFrom": "Sat8_1",
          "DialogueText": "Adesso non gioca più con me.|Chissà se papà tornerà mai a casa...",
          "AudioPath": "assets/it-IT/Vincent/28.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Ehi ciao, signore!",
          "AudioPath": "assets/it-IT/Vincent/29_male.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Ehi ciao, signorina.",
          "AudioPath": "assets/it-IT/Vincent/30_female.ogg"
        },
        {
          "DialogueFrom": "Sun10",
          "DialogueText": "Ciao, zio !",
          "AudioPath": "assets/it-IT/Vincent/31_male.ogg"
        },
        {
          "DialogueFrom": "Sun10_1",
          "DialogueText": "Ciao, zia !",
          "AudioPath": "assets/it-IT/Vincent/32_female.ogg"
        },
        {
          "DialogueFrom": "Sun10_2",
          "DialogueText": "Non dirlo a mamma... ma di tutti i grandi, preferisco te.",
          "AudioPath": "assets/it-IT/Vincent/33.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Ho fame... Dov'è mamma?",
          "AudioPath": "assets/it-IT/Vincent/34.ogg"
        },
        {
          "DialogueFrom": "Thu2",
          "DialogueText": "Non sei una noia come gli altri grandi!",
          "AudioPath": "assets/it-IT/Vincent/35.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "*sospira*... Mamma mi ha proibito di mangiare altre caramelle gommose, oggi.",
          "AudioPath": "assets/it-IT/Vincent/36.ogg"
        },
        {
          "DialogueFrom": "Tue6",
          "DialogueText": "Lo sai tenere un segreto, signore?",
          "AudioPath": "assets/it-IT/Vincent/37_male.ogg"
        },
        {
          "DialogueFrom": "Tue6_1",
          "DialogueText": "Lo sai tenere un segreto, signorina?",
          "AudioPath": "assets/it-IT/Vincent/38_female.ogg"
        },
        {
          "DialogueFrom": "Tue6_2",
          "DialogueText": "Da grande voglio sposare la signorina Penny.",
          "AudioPath": "assets/it-IT/Vincent/39.ogg"
        },
        {
          "DialogueFrom": "Tue6_3",
          "DialogueText": "Però non dirlo a lei, va bene?",
          "AudioPath": "assets/it-IT/Vincent/40.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "Voglio andare a caccia d'insetti, ma mamma s'arrabbia quando mi sporco tutto.",
          "AudioPath": "assets/it-IT/Vincent/41.ogg"
        },
        {
          "DialogueFrom": "Wed_1",
          "DialogueText": "Che scelta dura.",
          "AudioPath": "assets/it-IT/Vincent/42.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1",
          "DialogueText": "Sam...?",
          "AudioPath": "assets/it-IT/Vincent/43.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_1",
          "DialogueText": "Papà tornerà mai a casa?",
          "AudioPath": "assets/it-IT/Vincent/44.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_2",
          "DialogueText": "Ma ho sentito alcuni grandi che dicevano... che stanno facendo a pezzi tutte le nostre truppe.",
          "AudioPath": "assets/it-IT/Vincent/45.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_3",
          "DialogueText": "Va bene!",
          "AudioPath": "assets/it-IT/Vincent/46.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L",
          "DialogueText": "Non sto più nella pelle, Sam!",
          "AudioPath": "assets/it-IT/Vincent/47.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L_alt",
          "DialogueText": "OK!",
          "AudioPath": "assets/it-IT/Vincent/48.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L_alt_1",
          "DialogueText": "Pure a me!",
          "AudioPath": "assets/it-IT/Vincent/49.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19",
          "DialogueText": "Signor , tu sposerai la signorina Penny?",
          "AudioPath": "assets/it-IT/Vincent/50_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19_1",
          "DialogueText": "Signorina ... Ehm, tu ce l'hai un fidanzato?",
          "AudioPath": "assets/it-IT/Vincent/51_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat",
          "DialogueText": "...",
          "AudioPath": "assets/it-IT/Vincent/52.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Perché è chiusa? Voglio esplorare le fogne!",
          "AudioPath": "assets/it-IT/Vincent/53.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Il professor Gunther? Il tipo che ha il museo? Perché dici che ce l'ha lui?",
          "AudioPath": "assets/it-IT/Vincent/54.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny",
          "DialogueText": "Ehi, signor !",
          "AudioPath": "assets/it-IT/Vincent/55_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "Ehi, signorina !",
          "AudioPath": "assets/it-IT/Vincent/56_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "Cipolle pr..privameriglie. Ogni tanto mamma mi manda a raccoglierle...",
          "AudioPath": "assets/it-IT/Vincent/57.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_2",
          "DialogueText": "Sei proprio gentile per essere un grande... quindi ti mostro un segreto.",
          "AudioPath": "assets/it-IT/Vincent/58.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3",
          "DialogueText": "Se la vedi così, sembra a posto, no?",
          "AudioPath": "assets/it-IT/Vincent/59.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_4",
          "DialogueText": "Ma se la sbucci un pochetto...",
          "AudioPath": "assets/it-IT/Vincent/60.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_5",
          "DialogueText": "...È piena d'insetti!",
          "AudioPath": "assets/it-IT/Vincent/61.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_6",
          "DialogueText": "Forte, eh?",
          "AudioPath": "assets/it-IT/Vincent/62.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_7",
          "DialogueText": "Di solito li spiaccico!",
          "AudioPath": "assets/it-IT/Vincent/63.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseAnimals",
          "DialogueText": "Fattorista ? Posso mettere una sella su una tua mucca e cavalcarla fino in paese? Voglio essere un vero cowboy!",
          "AudioPath": "assets/it-IT/Vincent/64.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseFarming",
          "DialogueText": "Ehm, Fattorista ? Se mangio le tue verdure, poi crescerò forte come il mio fratellone Sam?",
          "AudioPath": "assets/it-IT/Vincent/65.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseFarming_1",
          "DialogueText": "È fortissimo, lui. Ieri sera l'ho visto sollevare tutta la signorina Penny da terra, mentre si arrampicavano su un albero.",
          "AudioPath": "assets/it-IT/Vincent/66.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseMinerals",
          "DialogueText": "Fattorista ? È vero che in fondo alle caverne ci sono i goblin... e che gli piace rapire le bambine per mangiarsele?",
          "AudioPath": "assets/it-IT/Vincent/67.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny",
          "DialogueText": "Significa che non devo più fare i compiti?",
          "AudioPath": "assets/it-IT/Vincent/68.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny_implied",
          "DialogueText": "Ultimamente penso parecchio alle torte...",
          "AudioPath": "assets/it-IT/Vincent/69.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Muuuuuu!",
          "AudioPath": "assets/it-IT/Vincent/70.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Posso rifare il gioco della fionda, papà!? Per favore?",
          "AudioPath": "assets/it-IT/Vincent/71.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "Mamma non mi lascia entrare nel labirinto. *singhiozza*",
          "AudioPath": "assets/it-IT/Vincent/72.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Abbiamo finito il labirinto? Dov'è il premio?",
          "AudioPath": "assets/it-IT/Vincent/73.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "Quando comincia la caccia all'uovo?! Sono così eccitato!!",
          "AudioPath": "assets/it-IT/Vincent/74.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13_1",
          "DialogueText": "Voglio vivere dentro un uovo gigante!",
          "AudioPath": "assets/it-IT/Vincent/75.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "Non è giusto... Perché non posso essere io la regina dei fiori?",
          "AudioPath": "assets/it-IT/Vincent/76.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "Se c'è una regina dei fiori... dev'esserci pure un re dei fiori!",
          "AudioPath": "assets/it-IT/Vincent/77.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Mi annoio...",
          "AudioPath": "assets/it-IT/Vincent/78.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "Perché non posso andare a giocare vicino alle pozze di marea?",
          "AudioPath": "assets/it-IT/Vincent/79.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "Voglio vedere la rara medusa verde!",
          "AudioPath": "assets/it-IT/Vincent/80.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "Una volta ho toccato una medusa e mi ha punto una mano.",
          "AudioPath": "assets/it-IT/Vincent/81.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Possiamo aprire i regali adesso?!",
          "AudioPath": "assets/it-IT/Vincent/82.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "Ho avuto il dinosauro giocattolo che volevo! Guarda!?Si chiama ehm... ste-...stego-...stegosonauro! ...mi pare.",
          "AudioPath": "assets/it-IT/Vincent/83.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Ho visto un pescione laggiù!",
          "AudioPath": "assets/it-IT/Vincent/84.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "Il mio fratellone mi aiuta a fare un pupazzo di neve!",
          "AudioPath": "assets/it-IT/Vincent/85.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "Wow, grazie davvero! È fortissimo!",
          "AudioPath": "assets/it-IT/Vincent/86.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Per me? Wow, grazie!",
          "AudioPath": "assets/it-IT/Vincent/87.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Non mi sembra un granché...",
          "AudioPath": "assets/it-IT/Vincent/88.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "Puah, ma che roba è?",
          "AudioPath": "assets/it-IT/Vincent/89.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Grazie!",
          "AudioPath": "assets/it-IT/Vincent/90.ogg"
        }
      ]
    }
  ]
