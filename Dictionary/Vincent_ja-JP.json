{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Vincent.ja-JP",
      "VoicePackName": "Dictionary (Vincent - ja-JP)",
      "Character": "Vincent",
      "Language": "ja-JP",
      "Entries": [
        {
          "DialogueFrom": "AcceptBirthdayGift_Negative",
          "DialogueText": "誕生日プレゼント？なーんだ… あまりうれしくないや",
          "AudioPath": "assets/ja-JP/Vincent/1.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive",
          "DialogueText": "誕生日プレゼント？ぼくに？",
          "AudioPath": "assets/ja-JP/Vincent/2.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive_1",
          "DialogueText": "うわぁ！すっごく欲しかったんだ！やったー！",
          "AudioPath": "assets/ja-JP/Vincent/3.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "わぁ！とっても甘いね！うん… おいしいよ！",
          "AudioPath": "assets/ja-JP/Vincent/4.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(TR)FrogEgg",
          "DialogueText": "（つるん つるん）…えーっ！これをくれるの？もらっていいの？やったー！",
          "AudioPath": "assets/ja-JP/Vincent/5.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Negative_alcohol_item",
          "DialogueText": "（クンクン）… これはなに？変なにおい…ねえ、お母さーん！",
          "AudioPath": "assets/ja-JP/Vincent/6.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "うわっ、 また テンサイの煮込み…",
          "AudioPath": "assets/ja-JP/Vincent/7.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "サイアクだよ…今夜は おかあさんが レンズマメの スープを 作るんだって。",
          "AudioPath": "assets/ja-JP/Vincent/8.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "お母さん！外に出て遊びたいよ！",
          "AudioPath": "assets/ja-JP/Vincent/9.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "%ヴィンセントはコケにおおわれている。",
          "AudioPath": "assets/ja-JP/Vincent/10.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "はじめまして！ボクはヴィンセント。",
          "AudioPath": "assets/ja-JP/Vincent/11.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "おかあさんが 知らない人と 話しちゃ ダメだって 言ってるけど…あなたなら 大丈夫そう。",
          "AudioPath": "assets/ja-JP/Vincent/12.ogg"
        },
        {
          "DialogueFrom": "married_Sam",
          "DialogueText": "ボクのお兄ちゃんをつれていかないで！",
          "AudioPath": "assets/ja-JP/Vincent/13.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "大きくなったら、 お兄ちゃん みたいに なりたいな！",
          "AudioPath": "assets/ja-JP/Vincent/14.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "うん！じゃあね！",
          "AudioPath": "assets/ja-JP/Vincent/15.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "kent#おとうさんが戻ってうれしいけどさ、前とは様子が違うんだ。|おとうさんとはあんまり会えないんだ…一緒に遊びたいよ。",
          "AudioPath": "assets/ja-JP/Vincent/16.ogg"
        },
        {
          "DialogueFrom": "reject_869",
          "DialogueText": "おおー！よく見つけたね！",
          "AudioPath": "assets/ja-JP/Vincent/17.ogg"
        },
        {
          "DialogueFrom": "reject_869_1",
          "DialogueText": "お母さんは飼っちゃダメって言ってるけど…",
          "AudioPath": "assets/ja-JP/Vincent/18.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "近くで見ると、小さな虫が砂の中から飛び出しているのが見えるんだ。",
          "AudioPath": "assets/ja-JP/Vincent/19.ogg"
        },
        {
          "DialogueFrom": "Resort_Entering",
          "DialogueText": "とても楽しかったよ！いつかボートの船長になりたいな！",
          "AudioPath": "assets/ja-JP/Vincent/20.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "すぐに戻って来たいよ！",
          "AudioPath": "assets/ja-JP/Vincent/21.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "サメを見てみたいな！",
          "AudioPath": "assets/ja-JP/Vincent/22.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "ペニーさんは 毎週 新しい 本を 読ませてくれるんだ。",
          "AudioPath": "assets/ja-JP/Vincent/23.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "時々、 本を 選ばせてくれることも あるんだよ！",
          "AudioPath": "assets/ja-JP/Vincent/24.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "もうすぐ おとうさんが 帰ってくるんだって！",
          "AudioPath": "assets/ja-JP/Vincent/25.ogg"
        },
        {
          "DialogueFrom": "Sat_1_1",
          "DialogueText": "お土産の オモチャは ないかな。",
          "AudioPath": "assets/ja-JP/Vincent/26.ogg"
        },
        {
          "DialogueFrom": "Sat8",
          "DialogueText": "kent#おとうさんが帰ってきたけど、何か変。いったいどうしたんだろう？",
          "AudioPath": "assets/ja-JP/Vincent/27.ogg"
        },
        {
          "DialogueFrom": "Sat8_1",
          "DialogueText": "ボクと一緒に遊んでくれないんだ。|ちゃんと帰ってくるかなぁって思ってしまう。",
          "AudioPath": "assets/ja-JP/Vincent/28.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "こんにちは、 お兄さん！",
          "AudioPath": "assets/ja-JP/Vincent/29_male.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "こんにちは、 お姉さん！",
          "AudioPath": "assets/ja-JP/Vincent/30_female.ogg"
        },
        {
          "DialogueFrom": "Sun10",
          "DialogueText": "こんにちは、おじさん！",
          "AudioPath": "assets/ja-JP/Vincent/31_male.ogg"
        },
        {
          "DialogueFrom": "Sun10_1",
          "DialogueText": "こんにちは、おばさん！",
          "AudioPath": "assets/ja-JP/Vincent/32_female.ogg"
        },
        {
          "DialogueFrom": "Sun10_2",
          "DialogueText": "大人では あなたが 好きだって… おかあさんには 言わないでね。",
          "AudioPath": "assets/ja-JP/Vincent/33.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "おなかすいた… おかあさんは どこ？",
          "AudioPath": "assets/ja-JP/Vincent/34.ogg"
        },
        {
          "DialogueFrom": "Thu2",
          "DialogueText": "あなたって、 他の 大人と 違って 面白いんだね！",
          "AudioPath": "assets/ja-JP/Vincent/35.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "はぁ… 今日は これ以上 おかあさんから グミが もらえないんだ。",
          "AudioPath": "assets/ja-JP/Vincent/36.ogg"
        },
        {
          "DialogueFrom": "Tue6",
          "DialogueText": "お兄さん、 秘密だよ？",
          "AudioPath": "assets/ja-JP/Vincent/37_male.ogg"
        },
        {
          "DialogueFrom": "Tue6_1",
          "DialogueText": "お姉さん、 秘密だよ？",
          "AudioPath": "assets/ja-JP/Vincent/38_female.ogg"
        },
        {
          "DialogueFrom": "Tue6_2",
          "DialogueText": "大きくなったら、 ペニーさんと 結婚したいんだ。",
          "AudioPath": "assets/ja-JP/Vincent/39.ogg"
        },
        {
          "DialogueFrom": "Tue6_3",
          "DialogueText": "ペニーさんには 言わないでね。",
          "AudioPath": "assets/ja-JP/Vincent/40.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "虫を 探したいけど、 泥まみれになると おかあさんに しかられるんだ。",
          "AudioPath": "assets/ja-JP/Vincent/41.ogg"
        },
        {
          "DialogueFrom": "Wed_1",
          "DialogueText": "難しいよね。",
          "AudioPath": "assets/ja-JP/Vincent/42.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1",
          "DialogueText": "サムにいちゃん…",
          "AudioPath": "assets/ja-JP/Vincent/43.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_1",
          "DialogueText": "おとうさん、いつかおうちに帰ってくるのかな？",
          "AudioPath": "assets/ja-JP/Vincent/44.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_2",
          "DialogueText": "でも、大人の人がいってるのをきいたんだ…ボクたちのぐんたいはめちゃくちゃにされちゃったんだって。",
          "AudioPath": "assets/ja-JP/Vincent/45.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_3",
          "DialogueText": "うん、わかった！",
          "AudioPath": "assets/ja-JP/Vincent/46.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L",
          "DialogueText": "サム、ボクは楽しみだよ！",
          "AudioPath": "assets/ja-JP/Vincent/47.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L_alt",
          "DialogueText": "オッケー！",
          "AudioPath": "assets/ja-JP/Vincent/48.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L_alt_1",
          "DialogueText": "ボクも同感だよ！",
          "AudioPath": "assets/ja-JP/Vincent/49.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19",
          "DialogueText": "さんは、ペニー先生と結婚するんですか？",
          "AudioPath": "assets/ja-JP/Vincent/50_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19_1",
          "DialogueText": "さんは…えっと、ボーイフレンドはいるんですか？",
          "AudioPath": "assets/ja-JP/Vincent/51_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat",
          "DialogueText": "…",
          "AudioPath": "assets/ja-JP/Vincent/52.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "なんでカギがかかってるんだろ？ボク、げすいどうを探検したいのに！",
          "AudioPath": "assets/ja-JP/Vincent/53.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "ギュンターきょうじゅ？あの博物館のおじさん？なんでそうおもうのさ？",
          "AudioPath": "assets/ja-JP/Vincent/54.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny",
          "DialogueText": "こんにちは、さん！",
          "AudioPath": "assets/ja-JP/Vincent/55.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "葉…葉タマネギ。ママに とってきてって 言われて…",
          "AudioPath": "assets/ja-JP/Vincent/56.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_2",
          "DialogueText": "あなたって 大人なのに やさしいね…だから ボクの ひみつを 教えちゃうね。",
          "AudioPath": "assets/ja-JP/Vincent/57.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3",
          "DialogueText": "こんな感じの。ぱっと見 大丈夫そうだよね？",
          "AudioPath": "assets/ja-JP/Vincent/58.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_4",
          "DialogueText": "でもちょっと 皮を むいてみると…",
          "AudioPath": "assets/ja-JP/Vincent/59.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_5",
          "DialogueText": "…虫だらけ！",
          "AudioPath": "assets/ja-JP/Vincent/60.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_6",
          "DialogueText": "すごいでしょ？",
          "AudioPath": "assets/ja-JP/Vincent/61.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_7",
          "DialogueText": "だいたい つぶしちゃうかな！",
          "AudioPath": "assets/ja-JP/Vincent/62.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseAnimals",
          "DialogueText": "さん、あなたの牛の背中にのって、町をまわってもいいですか？ボク、ほんもののカウボーイになりたいんだ！",
          "AudioPath": "assets/ja-JP/Vincent/63.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseFarming",
          "DialogueText": "えっと、さん、あなたがつくったお野菜をたべたら、ボクもサムにいちゃんみたいにつよくなれますか？",
          "AudioPath": "assets/ja-JP/Vincent/64.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseFarming_1",
          "DialogueText": "サムにいちゃんは、ペニー先生をもちあげられるくらい強いんだよ。ボクきのう、2人が木のぼりしてるところ見たもん。",
          "AudioPath": "assets/ja-JP/Vincent/65.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseMinerals",
          "DialogueText": "さん、どうくつのおく深くにはゴブリンがすんでいて…女の子をさらってばんごはんにしちゃうってほんとうですか？",
          "AudioPath": "assets/ja-JP/Vincent/66.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny",
          "DialogueText": "私が宿題をする必要がないことを意味してる？",
          "AudioPath": "assets/ja-JP/Vincent/67.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny_implied",
          "DialogueText": "私は最近ケーキについてよく考えてるの...",
          "AudioPath": "assets/ja-JP/Vincent/68.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "モオー！モオー！",
          "AudioPath": "assets/ja-JP/Vincent/69.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "お父さん！もう一回的あてゲームをやってもいい？お願い！",
          "AudioPath": "assets/ja-JP/Vincent/70.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "おかあさんが、ボクは迷路に行っちゃダメって。ぐすっ…",
          "AudioPath": "assets/ja-JP/Vincent/71.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "もう迷路はおしまい？賞品は？",
          "AudioPath": "assets/ja-JP/Vincent/72.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "エッグハントはいつはじまるの？！すっごく楽しみ！！",
          "AudioPath": "assets/ja-JP/Vincent/73.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13_1",
          "DialogueText": "大きなタマゴの中に住んでみたいな！",
          "AudioPath": "assets/ja-JP/Vincent/74.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "不公平だよ…なんでボクはフラワークイーンになれないの？",
          "AudioPath": "assets/ja-JP/Vincent/75.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "フラワークイーンがいるなら… フラワーキングもいるはずだよね！",
          "AudioPath": "assets/ja-JP/Vincent/76.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "あきちゃった…",
          "AudioPath": "assets/ja-JP/Vincent/77.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "どうして潮だまりであそんじゃいけないの？",
          "AudioPath": "assets/ja-JP/Vincent/78.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "グリーンはレアなんだよ！見たいなあ！",
          "AudioPath": "assets/ja-JP/Vincent/79.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "一度だけクラゲをさわったことがあるけど、さされちゃったんだ。",
          "AudioPath": "assets/ja-JP/Vincent/80.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "ねえ、いまプレゼント開けちゃダメ？！",
          "AudioPath": "assets/ja-JP/Vincent/81.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "ずっとほしかった恐竜のおもちゃだ！ほら！えっと… これはス… ステゴ… ステゴサウルスっていうんだよ！ …きっと。",
          "AudioPath": "assets/ja-JP/Vincent/82.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "氷の下にでっかいのがいたよ！",
          "AudioPath": "assets/ja-JP/Vincent/83.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "お兄ちゃんといっしょに雪だるまをつくってるんだ！",
          "AudioPath": "assets/ja-JP/Vincent/84.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "うわぁ、ありがとう！これ、めちゃくちゃすごいや！",
          "AudioPath": "assets/ja-JP/Vincent/85.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "ぼくにくれるの？わあ、ありがとう！",
          "AudioPath": "assets/ja-JP/Vincent/86.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "つまんないの…",
          "AudioPath": "assets/ja-JP/Vincent/87.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "うえーっ、なにこれ？",
          "AudioPath": "assets/ja-JP/Vincent/88.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "ありがとう！",
          "AudioPath": "assets/ja-JP/Vincent/89.ogg"
        }
      ]
    }
  ]
}