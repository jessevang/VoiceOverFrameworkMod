{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.MrQi.tr-TR",
      "VoicePackName": "Dictionary (MrQi - tr-TR)",
      "Character": "MrQi",
      "Language": "tr-TR",
      "Entries": [
        {
          "DialogueFrom": "Event:QiNutRoom/10040609",
          "DialogueText": "Vay vay vay... Bakın kim gizli ceviz odamı keşfetmiş.",
          "AudioPath": "assets/tr-TR/MrQi/1.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1",
          "DialogueText": "Çok ama çok etkilendim.",
          "AudioPath": "assets/tr-TR/MrQi/2.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_1",
          "DialogueText": "Adadaki her yere erişmek için alt tarafı bir avuç cevize ihtiyacın vardı ama sen kendini aştın.",
          "AudioPath": "assets/tr-TR/MrQi/3.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_2",
          "DialogueText": "Bu gizemli kapıdan içeri girme arzun o kadar güçlüydü ki girmek için ne gerekiyorsa yaptın.",
          "AudioPath": "assets/tr-TR/MrQi/4.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_3",
          "DialogueText": "Bu arzu seni diğerlerinden ayırıyor, evlat. İçinde çok özel bir enerji var.",
          "AudioPath": "assets/tr-TR/MrQi/5.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2",
          "DialogueText": "Bununla beraber...",
          "AudioPath": "assets/tr-TR/MrQi/6.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2_1",
          "DialogueText": "Asıl mücadelenin burada bittiğini sanmıyorsun, değil mi?",
          "AudioPath": "assets/tr-TR/MrQi/7.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2_2",
          "DialogueText": "Hayır... Hah. Hah. Hah... Aslında her şey daha yeni başlıyor.",
          "AudioPath": "assets/tr-TR/MrQi/8.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_3",
          "DialogueText": "Sol tarafında gayet ilginç fırsatların olduğu bir pano göreceksin.",
          "AudioPath": "assets/tr-TR/MrQi/9.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_3_1",
          "DialogueText": "Bunlar sana özel olarak hazırladığım ve seni zorlayacak mücadeleler.",
          "AudioPath": "assets/tr-TR/MrQi/10.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_4",
          "DialogueText": "Eğer mücadelelerimi başarıyla tamamlarsan özel şeyler kazanacaksın... Ben bunlara 'Qi Taşı' diyorum.",
          "AudioPath": "assets/tr-TR/MrQi/11.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_5",
          "DialogueText": "Bunları ağzına sokma ama... zira fazlasıyla radyoaktifler. Heh. heh. heh...",
          "AudioPath": "assets/tr-TR/MrQi/12.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_6",
          "DialogueText": "Sağında bulunan makine aracılığıyla bu taşları nadir ve güçlü ödüllerle takas edebilirsin.",
          "AudioPath": "assets/tr-TR/MrQi/13.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_7",
          "DialogueText": "İlgini çekti, değil mi?",
          "AudioPath": "assets/tr-TR/MrQi/14.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_7_1",
          "DialogueText": "Kesinlikle... İçimden bir ses bu mücadeleleri tamamlamaktan zevk alacağını söylüyor...",
          "AudioPath": "assets/tr-TR/MrQi/15.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_8",
          "DialogueText": "Şimdi izninle... Halletmem gereken önemli işler var.",
          "AudioPath": "assets/tr-TR/MrQi/16.ogg"
        }
      ]
    }
  ]
}