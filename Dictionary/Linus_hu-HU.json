{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Linus.hu-HU",
      "VoicePackName": "Dictionary (Linus - hu-HU)",
      "Character": "Linus",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "AcceptGift_(O)166",
          "DialogueText": "Nem, köszönöm. Nem vágyom pénzre... Sőt, szerintem átkozott.",
          "AudioPath": "assets/hu-HU/Linus/1.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)774",
          "DialogueText": "Hadd nézzem csak... tökéletesnek tűnik. Jó tanítvány vagy, .",
          "AudioPath": "assets/hu-HU/Linus/2.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)Book_Trash",
          "DialogueText": "Általában nem olvasok könyveket... Csak amiket a 'sarki boltban' találok... Köszönöm!",
          "AudioPath": "assets/hu-HU/Linus/3.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Ez sokat jelent nekem, . A sátramban fogom őrizni egy csendes napig. Köszönöm!",
          "AudioPath": "assets/hu-HU/Linus/4.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Positive_category_fish",
          "DialogueText": "Áh, ez frissnek tűnik. Ma este jól fogok lakni!",
          "AudioPath": "assets/hu-HU/Linus/5.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Positive_category_greens",
          "DialogueText": "Az remek zsákmány! Mindig boldog vagyok, ha vadon lévő ételeket ehetek. Köszönöm.",
          "AudioPath": "assets/hu-HU/Linus/6.ogg"
        },
        {
          "DialogueFrom": "cc_Boulder",
          "DialogueText": "Régi jó barátom, a ragyogó szikla, továbbállt. Lehet ez nem tűnik fontos eseménynek, de számomra ez nagy változás.",
          "AudioPath": "assets/hu-HU/Linus/7.ogg"
        },
        {
          "DialogueFrom": "cc_Boulder_1",
          "DialogueText": "De boldog vagyok, hogy a vén kő végre többet láthat a világból.",
          "AudioPath": "assets/hu-HU/Linus/8.ogg"
        },
        {
          "DialogueFrom": "DumpsterDiveComment",
          "DialogueText": "Találtál valami jót?",
          "AudioPath": "assets/hu-HU/Linus/9.ogg"
        },
        {
          "DialogueFrom": "eventSeen_8357109",
          "DialogueText": "Az itteni víz a magasból, a hegyi gleccserekből származik.",
          "AudioPath": "assets/hu-HU/Linus/10.ogg"
        },
        {
          "DialogueFrom": "eventSeen_8357109_1",
          "DialogueText": "Egyszer elutaztam a forráshoz, csak azért, hogy lerójam tiszteletemet a víznek.",
          "AudioPath": "assets/hu-HU/Linus/11.ogg"
        },
        {
          "DialogueFrom": "fall_Mon",
          "DialogueText": "Bejártam a barlangok legmélyét.",
          "AudioPath": "assets/hu-HU/Linus/12.ogg"
        },
        {
          "DialogueFrom": "fall_Mon_1",
          "DialogueText": "Sok rejtett titok lapul bennük.",
          "AudioPath": "assets/hu-HU/Linus/13.ogg"
        },
        {
          "DialogueFrom": "fall_Mon_2",
          "DialogueText": "Csak légy óvatos, ha bemennél egybe.",
          "AudioPath": "assets/hu-HU/Linus/14.ogg"
        },
        {
          "DialogueFrom": "fall_Sun",
          "DialogueText": "Nem szeretek sokáig egy helyen maradni.",
          "AudioPath": "assets/hu-HU/Linus/15.ogg"
        },
        {
          "DialogueFrom": "fall_Sun6",
          "DialogueText": "Nincs is jobb az őszi vad gombákból való belakmározásnál.",
          "AudioPath": "assets/hu-HU/Linus/16.ogg"
        },
        {
          "DialogueFrom": "fall_Tue4",
          "DialogueText": "Valami vicces kedvű alak este befújta festékkel az otthonomat... Reggel több órámba telt, mire sikerült lesikálnom.",
          "AudioPath": "assets/hu-HU/Linus/17.ogg"
        },
        {
          "DialogueFrom": "fall_Tue4_1",
          "DialogueText": "Fát kell gyűjtenem a télre.",
          "AudioPath": "assets/hu-HU/Linus/18.ogg"
        },
        {
          "DialogueFrom": "fall_Tue4_2",
          "DialogueText": "Van elég fád, hogy melegen tartson téged télen?",
          "AudioPath": "assets/hu-HU/Linus/19.ogg"
        },
        {
          "DialogueFrom": "fall_Wed4",
          "DialogueText": "Nincs túl sok felelősségem, szóval az időmet gondolkodással töltöm.",
          "AudioPath": "assets/hu-HU/Linus/20.ogg"
        },
        {
          "DialogueFrom": "fall_Wed4_1",
          "DialogueText": "Meg kell értened a gondolataidat, mielőtt irányítanád őket.",
          "AudioPath": "assets/hu-HU/Linus/21.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Kérlek ne rombold le a sátramat.",
          "AudioPath": "assets/hu-HU/Linus/22.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Korábban már megtörtént.",
          "AudioPath": "assets/hu-HU/Linus/23.ogg"
        },
        {
          "DialogueFrom": "Fri4",
          "DialogueText": "Sokat tanulhatsz a fáktól.",
          "AudioPath": "assets/hu-HU/Linus/24.ogg"
        },
        {
          "DialogueFrom": "Fri4_1",
          "DialogueText": "Ha sok időt töltesz velük, megosztják veled a titkaikat.",
          "AudioPath": "assets/hu-HU/Linus/25.ogg"
        },
        {
          "DialogueFrom": "Fri4_2",
          "DialogueText": "Menj békével, fiatal gyermek.",
          "AudioPath": "assets/hu-HU/Linus/26.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Ez a sok fura fa holnapra eltűnik... ez a természet egyik rejtélye.",
          "AudioPath": "assets/hu-HU/Linus/27.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "Én már egész évre elegendő mohát gyűjtöttem. És te hogy állsz?",
          "AudioPath": "assets/hu-HU/Linus/28.ogg"
        },
        {
          "DialogueFrom": "GreenRainFinished",
          "DialogueText": "Hogy sikerült a mohagyűjtés? Hehe... Az ágyam már sokkal puhább.",
          "AudioPath": "assets/hu-HU/Linus/29.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "Egy idegen?... Üdv.",
          "AudioPath": "assets/hu-HU/Linus/30.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "Ne is törődj velem. Egyedül éldegélek idekint.",
          "AudioPath": "assets/hu-HU/Linus/31.ogg"
        },
        {
          "DialogueFrom": "married",
          "DialogueText": "Egyszer gondoltam rá, hogy megházasodom. De most boldog harmóniában élek a levelek között suhogó széllel és az ezüst hold alatt brekegő békákkal.",
          "AudioPath": "assets/hu-HU/Linus/32.ogg"
        },
        {
          "DialogueFrom": "mineArea_80",
          "DialogueText": "Nagyon kevés ember jutott le olyan mélyre a bányákba, mint te.",
          "AudioPath": "assets/hu-HU/Linus/33.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Engem kizárólag az üde, friss levegő illata érdekel.",
          "AudioPath": "assets/hu-HU/Linus/34.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Saját döntésből élek itt.",
          "AudioPath": "assets/hu-HU/Linus/35.ogg"
        },
        {
          "DialogueFrom": "MovieInvitation",
          "DialogueText": "Óh... velem? Általában nem mennék, de... rendben van. Kedves tőled, hogy gondoltál rám.",
          "AudioPath": "assets/hu-HU/Linus/36.ogg"
        },
        {
          "DialogueFrom": "MovieInvitation_1",
          "DialogueText": "Nemsokára odamegyek.",
          "AudioPath": "assets/hu-HU/Linus/37.ogg"
        },
        {
          "DialogueFrom": "movieTheater",
          "DialogueText": "Mozi? Hmm... számomra a természet lassú körforgása az egyetlen dráma amire szükségem van.",
          "AudioPath": "assets/hu-HU/Linus/38.ogg"
        },
        {
          "DialogueFrom": "reject_869",
          "DialogueText": "Mi az ott nálad? Abból talán jó csalit lehetne készíteni.",
          "AudioPath": "assets/hu-HU/Linus/39.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Elvagyok én magamban is.",
          "AudioPath": "assets/hu-HU/Linus/40.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Nincs szükségem új barátokra.",
          "AudioPath": "assets/hu-HU/Linus/41.ogg"
        },
        {
          "DialogueFrom": "Sat4",
          "DialogueText": "Mindenem megvan, ami a túléléshez kell, sőt. A természet gyönyörű dallamot hallat, ha megtanulod, hogyan lehet meghallgatni.",
          "AudioPath": "assets/hu-HU/Linus/42.ogg"
        },
        {
          "DialogueFrom": "summer_Fri4",
          "DialogueText": "Amikor kifogsz egy halat, egy pillanatra állj meg, és köszönd meg az áldozatát.",
          "AudioPath": "assets/hu-HU/Linus/43.ogg"
        },
        {
          "DialogueFrom": "summer_Fri4_1",
          "DialogueText": "Még ha nem is hiszel a hal lelkében, ez egy remek mentális gyakorlat. Tisztában kell lennünk a környezetre mért hatásunkkal.",
          "AudioPath": "assets/hu-HU/Linus/44.ogg"
        },
        {
          "DialogueFrom": "summer_Mon",
          "DialogueText": "Egy kócsagot láttam kecsesen gázolni a reggeli ködben. Ilyenek a csendes élet kincsei.",
          "AudioPath": "assets/hu-HU/Linus/45.ogg"
        },
        {
          "DialogueFrom": "summer_Mon_1",
          "DialogueText": "Olyan könnyű belegabalyodni a modern élet zajába. A legjobb éveid pazarlódnak el egy nagy homályos katyvaszban. Ezért kell megtanulnod, hogy hogyan lassíts le.",
          "AudioPath": "assets/hu-HU/Linus/46.ogg"
        },
        {
          "DialogueFrom": "summer_Mon4",
          "DialogueText": "Oly sok év alatt rengeteg titkot fedeztem fel az élettel kapcsolatban..",
          "AudioPath": "assets/hu-HU/Linus/47.ogg"
        },
        {
          "DialogueFrom": "summer_Mon4_1",
          "DialogueText": "Ezekre mindre magadtól kell rájönnöd.",
          "AudioPath": "assets/hu-HU/Linus/48.ogg"
        },
        {
          "DialogueFrom": "summer_Sat4",
          "DialogueText": "A folyóban most szivárványos pisztrángot találhatsz.",
          "AudioPath": "assets/hu-HU/Linus/49.ogg"
        },
        {
          "DialogueFrom": "summer_Sun",
          "DialogueText": "Ez számomra az év könnyebbik időszaka. Nem kell aggódnom, hogy esténként fázni fogok.",
          "AudioPath": "assets/hu-HU/Linus/50.ogg"
        },
        {
          "DialogueFrom": "summer_Sun_1",
          "DialogueText": "Plusz a vadon gyümölcsei mindenütt megteremnek.",
          "AudioPath": "assets/hu-HU/Linus/51.ogg"
        },
        {
          "DialogueFrom": "summer_Thu4",
          "DialogueText": "Megvan a saját okom arra, hogy ilyen magányosan éljek.",
          "AudioPath": "assets/hu-HU/Linus/52.ogg"
        },
        {
          "DialogueFrom": "summer_Thu4_1",
          "DialogueText": "Vannak dolgok, amiket nem szabad kimondani...",
          "AudioPath": "assets/hu-HU/Linus/53.ogg"
        },
        {
          "DialogueFrom": "summer_Thu4_2",
          "DialogueText": "Gyűjtögetnem kéne.",
          "AudioPath": "assets/hu-HU/Linus/54.ogg"
        },
        {
          "DialogueFrom": "summer_Tue4",
          "DialogueText": "Hogy vagy, fiatal barátom?",
          "AudioPath": "assets/hu-HU/Linus/55.ogg"
        },
        {
          "DialogueFrom": "summer_Wed6",
          "DialogueText": "Ah... nyár. A meleg Nap felhevíti a konzervjeimet.",
          "AudioPath": "assets/hu-HU/Linus/56.ogg"
        },
        {
          "DialogueFrom": "summer_Wed6_1",
          "DialogueText": "Aztán este meleg étel vár rám.",
          "AudioPath": "assets/hu-HU/Linus/57.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Tegnap este valaki kövekkel dobálta meg a sátramat... Csak ki kellett várnom.",
          "AudioPath": "assets/hu-HU/Linus/58.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Nem szeretek sokáig egy helyen lenni. Túl sok dolgot lehet tapasztalni a világban.",
          "AudioPath": "assets/hu-HU/Linus/59.ogg"
        },
        {
          "DialogueFrom": "Sun4",
          "DialogueText": "Jó lenne, ha a városiak elfogadnának olyannak, amilyen vagyok.",
          "AudioPath": "assets/hu-HU/Linus/60.ogg"
        },
        {
          "DialogueFrom": "Sun4_1",
          "DialogueText": "Én csak szeretek a friss levegőn élni. És ők ezt nem értik meg.",
          "AudioPath": "assets/hu-HU/Linus/61.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "...Hmm? Szükséged van valamire?",
          "AudioPath": "assets/hu-HU/Linus/62.ogg"
        },
        {
          "DialogueFrom": "Thu4",
          "DialogueText": "Az itteni emberek kedvesnek tűnnek, de kerülnek engem.",
          "AudioPath": "assets/hu-HU/Linus/63.ogg"
        },
        {
          "DialogueFrom": "Thu4_1",
          "DialogueText": "Az emberek félnek az ismeretlentől.",
          "AudioPath": "assets/hu-HU/Linus/64.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "...Azért jöttél, hogy gúnyolódj rajtam?",
          "AudioPath": "assets/hu-HU/Linus/65.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "Csak teszem a napi dolgaimat.",
          "AudioPath": "assets/hu-HU/Linus/66.ogg"
        },
        {
          "DialogueFrom": "Tue4",
          "DialogueText": "Megtanulhatod, hogy hogyan élheted túl a vadonban. Mint én.",
          "AudioPath": "assets/hu-HU/Linus/67.ogg"
        },
        {
          "DialogueFrom": "Tue4_1",
          "DialogueText": "Úgy hiszem titokban mindannyian vissza akarunk térni a természetbe. Csak kissé ijesztő megtenni az első lépéseket.",
          "AudioPath": "assets/hu-HU/Linus/68.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "Nem ismerlek eléggé, hogy megbízzak benned. Sajnálom.",
          "AudioPath": "assets/hu-HU/Linus/69.ogg"
        },
        {
          "DialogueFrom": "Wed2",
          "DialogueText": "Óvakodnom kell az idegenektől. A legtöbben nem szeretik a 'vadembereket'.",
          "AudioPath": "assets/hu-HU/Linus/70.ogg"
        },
        {
          "DialogueFrom": "Wed6",
          "DialogueText": "Sok időt töltök gondolkodással.",
          "AudioPath": "assets/hu-HU/Linus/71.ogg"
        },
        {
          "DialogueFrom": "Wed6_1",
          "DialogueText": "Ha teljesen megértetted a gondolataid mögött rejlő indokokat, akkor a lét egy új szintjére emelkedtél.",
          "AudioPath": "assets/hu-HU/Linus/72.ogg"
        },
        {
          "DialogueFrom": "winter_Fri4",
          "DialogueText": "A fák aludni térnek a télen. Így egy kissé magányos vagyok.",
          "AudioPath": "assets/hu-HU/Linus/73.ogg"
        },
        {
          "DialogueFrom": "winter_Fri4_1",
          "DialogueText": "De nem szeretném felzavarni őket szép álmukból.",
          "AudioPath": "assets/hu-HU/Linus/74.ogg"
        },
        {
          "DialogueFrom": "winter_Mon",
          "DialogueText": "Ha egy sátorban laksz, akkor nagyon hideg lehet az idő.",
          "AudioPath": "assets/hu-HU/Linus/75.ogg"
        },
        {
          "DialogueFrom": "winter_Mon4",
          "DialogueText": "Miután teljesen elszakadtál a modern élettől, akkor kezded igazán élvezni az apróbb dolgokat... mint például a Napot, az érett gyümölcsöket, vagy azt, hogy pont olyan alakú botot találsz amit kerestél.",
          "AudioPath": "assets/hu-HU/Linus/76.ogg"
        },
        {
          "DialogueFrom": "winter_Sat4",
          "DialogueText": "Köszönöm, hogy benéztél. Már reggel óta kezdtem kissé magányos lenni.",
          "AudioPath": "assets/hu-HU/Linus/77.ogg"
        },
        {
          "DialogueFrom": "winter_Sat4_1",
          "DialogueText": "Szóval találtál valami érdekeset a bányákban?",
          "AudioPath": "assets/hu-HU/Linus/78.ogg"
        },
        {
          "DialogueFrom": "winter_Sun",
          "DialogueText": "A földön alvás jót tesz a hátamnak.",
          "AudioPath": "assets/hu-HU/Linus/79.ogg"
        },
        {
          "DialogueFrom": "winter_Sun_1",
          "DialogueText": "Mindig a dolgok pozitív oldalát kell figyelembe venni.",
          "AudioPath": "assets/hu-HU/Linus/80.ogg"
        },
        {
          "DialogueFrom": "winter_Thu4",
          "DialogueText": "A tél kemény, de attól még lehet horgászni és gyűjtögetni.",
          "AudioPath": "assets/hu-HU/Linus/81.ogg"
        },
        {
          "DialogueFrom": "winter_Thu4_1",
          "DialogueText": "Ha vannak állataid, jobb, ha bent tartod őket.",
          "AudioPath": "assets/hu-HU/Linus/82.ogg"
        },
        {
          "DialogueFrom": "winter_Tue4",
          "DialogueText": "Köszönöm, hogy nem menekülsz előlem, . Nagy bölcsesség kell ahhoz, hogy felülírd az alap ösztöneidet.",
          "AudioPath": "assets/hu-HU/Linus/83.ogg"
        },
        {
          "DialogueFrom": "winter_Tue4_1",
          "DialogueText": "Én is ember vagyok, akárcsak bárki más. Csak más az életstílusom.",
          "AudioPath": "assets/hu-HU/Linus/84.ogg"
        },
        {
          "DialogueFrom": "winter_Wed4",
          "DialogueText": "Bizonyára megérted, miért óvakodok az idegenektől.",
          "AudioPath": "assets/hu-HU/Linus/85.ogg"
        },
        {
          "DialogueFrom": "winter_Wed4_1",
          "DialogueText": "Azt hiszem te és én már barátok vagyunk.",
          "AudioPath": "assets/hu-HU/Linus/86.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved",
          "DialogueText": "Helló, Leo... Az én nevem Linus.",
          "AudioPath": "assets/hu-HU/Linus/87.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_1",
          "DialogueText": "Sokat hallottam rólad, és a papagáj családodról.",
          "AudioPath": "assets/hu-HU/Linus/88.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_2",
          "DialogueText": "Igazán különleges dolog!",
          "AudioPath": "assets/hu-HU/Linus/89.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_1",
          "DialogueText": "Leo...",
          "AudioPath": "assets/hu-HU/Linus/90.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_1_1",
          "DialogueText": "Szeretném, ha visszajönnél hozzánk, Stardew Valleybe.",
          "AudioPath": "assets/hu-HU/Linus/91.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_1_2",
          "DialogueText": "Az egy gyönyörű hely... ott nem olyan meleg, mint itt, de tele van élettel... és én pont a közepén élek!",
          "AudioPath": "assets/hu-HU/Linus/92.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_1_3",
          "DialogueText": "Ismerem a földet, és sokat tudok a vizekről, a fákról, az állatokról, és másról is...",
          "AudioPath": "assets/hu-HU/Linus/93.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_1_4",
          "DialogueText": "Tudod... én is a természet gyermeke vagyok. Úgy is mondhatjuk, hogy 'madarat tolláról'.",
          "AudioPath": "assets/hu-HU/Linus/94.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_1_5",
          "DialogueText": "De kezdek öreg lenni... És szeretném átadni minden tudásomat valakinek, mielőtt továbbmennék...",
          "AudioPath": "assets/hu-HU/Linus/95.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_1_6",
          "DialogueText": "Leo... visszajönnél velünk?",
          "AudioPath": "assets/hu-HU/Linus/96.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_alt",
          "DialogueText": "Várj!",
          "AudioPath": "assets/hu-HU/Linus/97.ogg"
        },
        {
          "DialogueFrom": "Event:IslandSouth/6497428/e 6497423/f Leo 1500/w sunny/t 600 1800/Hl leoMoved_alt_1",
          "DialogueText": "Nagyszerű!",
          "AudioPath": "assets/hu-HU/Linus/98.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400",
          "DialogueText": "! Gyere ide a tűz mellé. Jó érzés.",
          "AudioPath": "assets/hu-HU/Linus/99.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400_1",
          "DialogueText": "Reménykedtem, hogy egyszer majd eljössz.",
          "AudioPath": "assets/hu-HU/Linus/100.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400_2",
          "DialogueText": "Bocsánatot szeretnék kérni, amiért kezdetben nem bíztam benned.",
          "AudioPath": "assets/hu-HU/Linus/101.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400_2_1",
          "DialogueText": "A legtöbb ember nem bánik velem jól, szóval megtanultam óvatos lenni.",
          "AudioPath": "assets/hu-HU/Linus/102.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400_3",
          "DialogueText": "De te szokatlanul kedves voltál hozzám. Te különleges személy vagy.",
          "AudioPath": "assets/hu-HU/Linus/103.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400_3_1",
          "DialogueText": "...És jó barátomnak tekintelek.",
          "AudioPath": "assets/hu-HU/Linus/104.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400_4",
          "DialogueText": "Hé, mutatni szeretnék neked valamit. Kerülj beljebb!",
          "AudioPath": "assets/hu-HU/Linus/105.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400_5",
          "DialogueText": "Áh... itt is van. Látod? Ez egy különös csali, amit én készítettem.",
          "AudioPath": "assets/hu-HU/Linus/106.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400_5_1",
          "DialogueText": "Nagyon jó minőségű cucc... Még magam is megenném!",
          "AudioPath": "assets/hu-HU/Linus/107.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400_5_2",
          "DialogueText": "Tessék, átadom neked a receptjét.",
          "AudioPath": "assets/hu-HU/Linus/108.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/26/f Linus 1000/w sunny/t 2000 2400_alt",
          "DialogueText": "Megtanultál 'Vad csalit' készíteni",
          "AudioPath": "assets/hu-HU/Linus/109.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26",
          "DialogueText": "Ez roppant kedves tőled, de köszönöm, nem kell! Ma jó sok dolgot tudtam begyűjteni.",
          "AudioPath": "assets/hu-HU/Linus/110.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_1",
          "DialogueText": "Öhm...",
          "AudioPath": "assets/hu-HU/Linus/111.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_1_1",
          "DialogueText": "Izé... Köszönöm, nem.",
          "AudioPath": "assets/hu-HU/Linus/112.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_2",
          "DialogueText": "Értékelem a kedvességet... De tényleg...",
          "AudioPath": "assets/hu-HU/Linus/113.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_2_1",
          "DialogueText": "De... Mint korábban már említettem... Én döntöttem úgy, hogy így szeretnék élni...",
          "AudioPath": "assets/hu-HU/Linus/114.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_3",
          "DialogueText": "Legtöbbször szeretek magányos lenni... Szeretem a csendes hangokat... egyszerre mozdulni a természet ritmusával...",
          "AudioPath": "assets/hu-HU/Linus/115.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_3_1",
          "DialogueText": "Ez egy olyan életmód számomra, ami kényelmes. Ezen soha nem akarok változtatni.",
          "AudioPath": "assets/hu-HU/Linus/116.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_4",
          "DialogueText": "... Én nagyra értékelem a barátságunkat. És tudom, hogy te is így vagy ezzel.",
          "AudioPath": "assets/hu-HU/Linus/117.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_4_1",
          "DialogueText": "De... Nem kell próbálnod 'segíteni' nekem... Én tudom a legjobban, hogyan szeretném élni az életemet... rendben?",
          "AudioPath": "assets/hu-HU/Linus/118.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/371652/f Linus 2000/w sunny/t 600 1700/a 12 26_5",
          "DialogueText": "...érzed ezt az illatot? Ez az érett bogyók édes aromája...",
          "AudioPath": "assets/hu-HU/Linus/119.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8357109/w sunny/t 600 1900/n linusTrashCleanup",
          "DialogueText": "Áhhh... ez jó érzés.",
          "AudioPath": "assets/hu-HU/Linus/120.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8357109/w sunny/t 600 1900/n linusTrashCleanup_1",
          "DialogueText": "A víz sose volt ilyen tiszta.",
          "AudioPath": "assets/hu-HU/Linus/121.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8357109/w sunny/t 600 1900/n linusTrashCleanup_2",
          "DialogueText": "...És ez rám is vonatkozik! Hehehe.",
          "AudioPath": "assets/hu-HU/Linus/122.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/8959199/w sunny/t 600 1900/e 6497428/f Leo 2250/F_alt",
          "DialogueText": "Még nem...",
          "AudioPath": "assets/hu-HU/Linus/123.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/linusWell",
          "DialogueText": "Köszönöm, . Egy pillanatra már aggódni kezdtem... Azt hittem majd arra kérsz meg, hogy költözzek be hozzád a farmra! *kacsint*",
          "AudioPath": "assets/hu-HU/Linus/124.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/linusWell_1",
          "DialogueText": "Tudod, az egész völgyben téged tartalak a legközelebbi barátomnak... soha nem próbáltál 'megjavítani' engem... tiszteletben tartod az életmódomat, még akkor is, ha nem is érted teljesen.",
          "AudioPath": "assets/hu-HU/Linus/125.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/linusWell_2",
          "DialogueText": "Ezt nagyra értékelem.",
          "AudioPath": "assets/hu-HU/Linus/126.ogg"
        },
        {
          "DialogueFrom": "Event:Mountain/linusWell_1",
          "DialogueText": "Ah... érzed ezt az illatot? Ez az érett bogyók édes aromája...",
          "AudioPath": "assets/hu-HU/Linus/127.ogg"
        },
        {
          "DialogueFrom": "Event:Town/502969/w sunny/f Linus 50/j 7/t 2000 2400",
          "DialogueText": "Én voltam az... Sajnálom.",
          "AudioPath": "assets/hu-HU/Linus/128.ogg"
        },
        {
          "DialogueFrom": "Event:Town/502969/w sunny/f Linus 50/j 7/t 2000 2400_1",
          "DialogueText": "Rengeteg forró, friss ételt találok ezekben a kukákban... olyan dolgokat, amik megromlanak, ha nem viszem el őket.",
          "AudioPath": "assets/hu-HU/Linus/129.ogg"
        },
        {
          "DialogueFrom": "Event:Town/502969/w sunny/f Linus 50/j 7/t 2000 2400_2",
          "DialogueText": "'Szerinted van abban valami rossz, amit művelek?_Igen, ez undorító._Neked talán undorító. De számomra ez egy életmód. És még nem betegedtem meg. Csak olyanokat eszek meg, amik frissnek tűnnek._Nem. Szégyen, ha az étel kárba vész._Köszönöm, . Tudtam, hogy te nyitott elméjű ember vagy. Jót érzek azzal kapcsolatban, amit teszek. Senkit se bántok ezzel._Igen, ez illegális. Ez George magánterülete._Én nem hiszek a 'magánterületben'. Mellesleg mit akarsz, mit tegyek? Szükségem van erre az ételre a túléléshez. Legyen szíved._Nem, de munkát kellene szerezned, és nem másokon élősködnöd._**sóhaj**... Nem mindenkinek való ez a világ, . Te nem tudod, hogy milyen olyannak lenni, mint amilyen én vagyok. Senkit se bántok, csak a saját módomon akarom élni az életemet. Ez ennyire rossz volna?'",
          "AudioPath": "assets/hu-HU/Linus/130.ogg"
        },
        {
          "DialogueFrom": "Event:Town/502969/w sunny/f Linus 50/j 7/t 2000 2400_3",
          "DialogueText": "Hazamehetsz. Többé nem fogom George szemetesét kifosztani.",
          "AudioPath": "assets/hu-HU/Linus/131.ogg"
        },
        {
          "DialogueFrom": "Event:Town/502969/w sunny/f Linus 50/j 7/t 2000 2400_3_1",
          "DialogueText": "Közölheted vele, hogy jó alaposan elijesztetted a mosómedvéket.",
          "AudioPath": "assets/hu-HU/Linus/132.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue",
          "DialogueText": "...Eszméletlenül találtam rád a bányában. Szerencséd van, hogy épp arrafelé jártam!",
          "AudioPath": "assets/hu-HU/Linus/133.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue_1",
          "DialogueText": "Legközelebb légy egy kicsit óvatosabb, rendben?",
          "AudioPath": "assets/hu-HU/Linus/134.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Ezek az állatok sose ítélik meg az embereket a kinézetük alapján. Ezt persze az emberekről nem lehet elmondani.",
          "AudioPath": "assets/hu-HU/Linus/135.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Az állatok olyanok, mint a kicsi emberek... nekik is vannak érzéseik és érzelmeik.",
          "AudioPath": "assets/hu-HU/Linus/136.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "Szép munka, öreg barátom.",
          "AudioPath": "assets/hu-HU/Linus/137.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Haha! Egy újabb remek készítmény, öreg barátom.",
          "AudioPath": "assets/hu-HU/Linus/138.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "Senki se beszél hozzám igazán... Én csak a fűszeres tojásokért jöttem.",
          "AudioPath": "assets/hu-HU/Linus/139.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13_1",
          "DialogueText": "Később beosonok egy kicsit, és eszek valamit. Éppenséggel azzal a pompás pitével szemezgetek!",
          "AudioPath": "assets/hu-HU/Linus/140.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "Ó, üdv néked. Kedves tőled, hogy beszélsz velem.Hamarosan véget ér a tavasz... milyen kár.",
          "AudioPath": "assets/hu-HU/Linus/141.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "Sikeres díszszeder szüreted volt?Én eleget szedtem ahhoz, hogy édes és savanykás zselét készítsek! Ma mindenkinek hoztam egy kicsit kóstolóként.",
          "AudioPath": "assets/hu-HU/Linus/142.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "A lassú, folytonos forgatás a tökéletes sütés elérésének kulcsa.",
          "AudioPath": "assets/hu-HU/Linus/143.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "Áh, érzed ezt az illatot?Ez azt jelenti, hogy hamarosan készen van a sült!",
          "AudioPath": "assets/hu-HU/Linus/144.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "Majd idelopódzom, ha megérkeznek a medúzák... Senkit se akarok megzavarni.",
          "AudioPath": "assets/hu-HU/Linus/145.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "Észrevettem, hogy senki se használja ezt a kikötőt, szóval halkan ideosontam.Én is szeretem a medúzákat bámulni.",
          "AudioPath": "assets/hu-HU/Linus/146.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Csatlakoznék... de nem hiszem, hogy szívesen látnának.",
          "AudioPath": "assets/hu-HU/Linus/147.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "A polgármester személyesen hívott az asztalához. Ezt nem utasíthatom el!",
          "AudioPath": "assets/hu-HU/Linus/148.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "A jégkunyhó építés olyan művészet, amit a fagyos tengeren túli tundra lakóktól tanultam.Ez több évvel ezelőtt volt.Egy jégkunyhó remek otthonnak bizonyul, de könnyebb egész évben a sátramban lenni.",
          "AudioPath": "assets/hu-HU/Linus/149.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "Áhh... Az erdő megkezdte hosszú téli álmát...",
          "AudioPath": "assets/hu-HU/Linus/150.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "Ez csodálatos! Feldobtad a napomat.",
          "AudioPath": "assets/hu-HU/Linus/151.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Ez egy nagyszerű ajándék. Köszönöm!",
          "AudioPath": "assets/hu-HU/Linus/152.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Hmm... Ez nem igazán jön be nekem.",
          "AudioPath": "assets/hu-HU/Linus/153.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "Ezt miért adnád nekem? Azt hiszed szeretem a szemetet, mert egy sátorban élek? Ez borzasztó.",
          "AudioPath": "assets/hu-HU/Linus/154.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Egy ajándék? Milyen kedves.",
          "AudioPath": "assets/hu-HU/Linus/155.ogg"
        }
      ]
    }
  ]
}