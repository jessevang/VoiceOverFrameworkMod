{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Grandpa.hu-HU",
      "VoicePackName": "Dictionary (Grandpa - hu-HU)",
      "Character": "Grandpa",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H",
          "DialogueText": "Drága fiam...",
          "AudioPath": "assets/hu-HU/Grandpa/1_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Drága lányom...",
          "AudioPath": "assets/hu-HU/Grandpa/2_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "Több év telt el legutóbbi beszélgetésünk óta. Akkor még csak kisfiú voltál... Emlékszel?",
          "AudioPath": "assets/hu-HU/Grandpa/3_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_3",
          "DialogueText": "Több év telt el legutóbbi beszélgetésünk óta. Akkor még csak kislány voltál... Emlékszel?",
          "AudioPath": "assets/hu-HU/Grandpa/4_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_4",
          "DialogueText": "Nézd, hogy mi lett belőled!",
          "AudioPath": "assets/hu-HU/Grandpa/5.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_5",
          "DialogueText": "Habár lehet, hogy már réges-régen megfeledkeztél rólam, de én mindig is itt voltam.",
          "AudioPath": "assets/hu-HU/Grandpa/6.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_6",
          "DialogueText": "Tudod... A testem elhagyta ezt a világot, de a szívem mindig is Stardew Valleyben marad.",
          "AudioPath": "assets/hu-HU/Grandpa/7.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Már két éve vagy itt...",
          "AudioPath": "assets/hu-HU/Grandpa/8.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "farm jövője már a te kezedben van.",
          "AudioPath": "assets/hu-HU/Grandpa/9.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2_1",
          "DialogueText": "Viszlát!",
          "AudioPath": "assets/hu-HU/Grandpa/10.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H",
          "DialogueText": "Örülök, hogy megidéztél engem. Talán... túlságosan nyers voltam veled legutóbb.",
          "AudioPath": "assets/hu-HU/Grandpa/11.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "Bocsáss meg nekem! Keményen dolgoztál, és büszke vagyok rád!",
          "AudioPath": "assets/hu-HU/Grandpa/12.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "Nézzük csak...",
          "AudioPath": "assets/hu-HU/Grandpa/13.ogg"
        }
      ]
    }
  ]
}