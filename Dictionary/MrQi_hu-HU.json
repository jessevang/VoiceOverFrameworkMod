{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.MrQi.hu-HU",
      "VoicePackName": "Dictionary (MrQi - hu-HU)",
      "Character": "MrQi",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "Event:QiNutRoom/10040609",
          "DialogueText": "Nocsak, nocsak, nocsak... Nézzenek oda, hogy ki jutott be a titkos diós szobámba.",
          "AudioPath": "assets/hu-HU/MrQi/1.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1",
          "DialogueText": "Le vagyok nyűgözve.",
          "AudioPath": "assets/hu-HU/MrQi/2.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_1",
          "DialogueText": "Habár csak a diók egy részére volt szükséged, hogy teljes hozzáférést nyerj a szigetre, de úgy döntöttél, hogy tovább küzdesz.",
          "AudioPath": "assets/hu-HU/MrQi/3.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_2",
          "DialogueText": "Olyan erős akarattal akartad kinyitni ezt a titkos ajtót, hogy mindent megtettél, hogy bejuss.",
          "AudioPath": "assets/hu-HU/MrQi/4.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_3",
          "DialogueText": "Ez a hajtóerő az, ami különlegessé tesz téged, kölyök. Nagyon különleges energiával rendelkezel.",
          "AudioPath": "assets/hu-HU/MrQi/5.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2",
          "DialogueText": "Ezek után...",
          "AudioPath": "assets/hu-HU/MrQi/6.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2_1",
          "DialogueText": "Ugye nem gondoltad, hogy itt véget ért a kihívás, hmmm?",
          "AudioPath": "assets/hu-HU/MrQi/7.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2_2",
          "DialogueText": "Nem... Hah. Hah. Hah... Sőt, még csak most kezdődik!",
          "AudioPath": "assets/hu-HU/MrQi/8.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_3",
          "DialogueText": "Tőled balra látható egy tábla, rajta nagyon érdekes lehetőségekkel.",
          "AudioPath": "assets/hu-HU/MrQi/9.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_3_1",
          "DialogueText": "Ezek a kihívások, amiket csak neked készítettem, tesztelni fogják a korlátaidat.",
          "AudioPath": "assets/hu-HU/MrQi/10.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_4",
          "DialogueText": "Ha képes vagy sikeresen teljesíteni a kihívásaimat, akkor kapsz egy exkluzív fizetőeszközt... Én 'Qi drágaköveknek' nevezem őket.",
          "AudioPath": "assets/hu-HU/MrQi/11.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_5",
          "DialogueText": "Csak ne vedd őket a szádba, kölyök... Nagyon radioaktívak. Heh, heh, heh...",
          "AudioPath": "assets/hu-HU/MrQi/12.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_6",
          "DialogueText": "A tőled jobbra lévő eszközzel ezeket a Qi drágaköveket ritka és erős jutalmakra cserélheted.",
          "AudioPath": "assets/hu-HU/MrQi/13.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_7",
          "DialogueText": "Érdekesnek hangzik, nemde?",
          "AudioPath": "assets/hu-HU/MrQi/14.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_7_1",
          "DialogueText": "Igen... gondoltam, hogy egy hozzád hasonlónak nagyon tetszeni fog mindez...",
          "AudioPath": "assets/hu-HU/MrQi/15.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_8",
          "DialogueText": "Nos, ha megbocsátasz... Fontos üzletelnivalóm van.",
          "AudioPath": "assets/hu-HU/MrQi/16.ogg"
        }
      ]
    }
  ]
}