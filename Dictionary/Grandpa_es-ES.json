{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Grandpa.es-ES",
      "VoicePackName": "Dictionary (Grandpa - es-ES)",
      "Character": "Grandpa",
      "Language": "es-ES",
      "Entries": [
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H",
          "DialogueText": "Mi querido muchacho...",
          "AudioPath": "assets/es-ES/Grandpa/1_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Mi querida nieta...",
          "AudioPath": "assets/es-ES/Grandpa/2_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "Han pasado muchos años desde que hablamos por última vez. Eras solo un niño... ¿Lo recuerdas?",
          "AudioPath": "assets/es-ES/Grandpa/3_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_3",
          "DialogueText": "Han pasado muchos años desde que hablamos por última vez. Eras solo una niña... ¿Lo recuerdas?",
          "AudioPath": "assets/es-ES/Grandpa/4_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_4",
          "DialogueText": "¡Mira lo lejos que has llegado!",
          "AudioPath": "assets/es-ES/Grandpa/5.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_5",
          "DialogueText": "Aunque a lo mejor te has olvidado de mí, siempre he estado aquí.",
          "AudioPath": "assets/es-ES/Grandpa/6.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_6",
          "DialogueText": "¿Sabes...? Mi cuerpo ha abandonado este mundo, pero mi corazón se quedará para siempre en Stardew Valley.",
          "AudioPath": "assets/es-ES/Grandpa/7.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Ya llevas aquí dos años...",
          "AudioPath": "assets/es-ES/Grandpa/8.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "El futuro de la granja está en tus manos ahora.",
          "AudioPath": "assets/es-ES/Grandpa/9.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2_1",
          "DialogueText": "¡Hasta siempre!",
          "AudioPath": "assets/es-ES/Grandpa/10.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H",
          "DialogueText": "Me alegro de que me hayas invocado. A lo mejor... fui demasiado duro contigo la última vez.",
          "AudioPath": "assets/es-ES/Grandpa/11.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "Perdóname. Has trabajado muy duro y estoy muy orgulloso.",
          "AudioPath": "assets/es-ES/Grandpa/12.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "Veamos...",
          "AudioPath": "assets/es-ES/Grandpa/13.ogg"
        }
      ]
    }
  ]
}