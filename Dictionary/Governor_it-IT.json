{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Governor.it-IT",
      "VoicePackName": "Dictionary (Governor - it-IT)",
      "Character": "Governor",
      "Language": "it-IT",
      "Entries": [
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Certamente! È tutto l'anno che aspetto questo momento.",
          "AudioPath": "assets/it-IT/Governor/1.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "...",
          "AudioPath": "assets/it-IT/Governor/2.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_10",
          "DialogueText": "Ehm... in realtà è piuttosto disgustosa. Credo che salterò, per quest'anno.",
          "AudioPath": "assets/it-IT/Governor/3.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_11",
          "DialogueText": "Bleah! Che porcheria! Mi sta venendo la nausea...",
          "AudioPath": "assets/it-IT/Governor/4.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_12",
          "DialogueText": "Ho bisogno di sdraiarmi...",
          "AudioPath": "assets/it-IT/Governor/5.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_13",
          "DialogueText": "Con grande piacere! È tutto il pomeriggio che tengo d'occhio la zuppa.",
          "AudioPath": "assets/it-IT/Governor/6.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_15",
          "DialogueText": "Che evento incantevole... È sempre una gioia visitare Stardew Valley.Devo discutere con mia moglie se comprare una casa delle vacanze qui.",
          "AudioPath": "assets/it-IT/Governor/7.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_16",
          "DialogueText": "Attendo con ansia la mia visita annuale a Stardew Valley. C'è un fascino palpabile nell'aria!",
          "AudioPath": "assets/it-IT/Governor/8.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_2",
          "DialogueText": "Hmm... Sa un po' di asprigno... ma a parte questo ha un ottimo sapore!",
          "AudioPath": "assets/it-IT/Governor/9.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_3",
          "DialogueText": "Un momento... c'è qualcosa nel mio piatto... Cos'è?",
          "AudioPath": "assets/it-IT/Governor/10.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_4",
          "DialogueText": "Questo... Questo è un oltraggio!Non avevo mai ricevuto un insulto simile!",
          "AudioPath": "assets/it-IT/Governor/11.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_5",
          "DialogueText": "Bleah... mi si sta gonfiando la lingua... Sto per sentirmi male.",
          "AudioPath": "assets/it-IT/Governor/12.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_6",
          "DialogueText": "Hmmm... Beh, non c'è male, ma ci manca qualcosa...Hanno messo tutti quanti un ingrediente nella zuppa? Mi sembra che manchi la voce unica di qualcuno.",
          "AudioPath": "assets/it-IT/Governor/13.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_7",
          "DialogueText": "Oh cielo... è la zuppa migliore che abbia mai assaggiato!",
          "AudioPath": "assets/it-IT/Governor/14.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_8",
          "DialogueText": "Ah... Una zuppa molto gradevole. I prodotti di questa valle non deludono mai!",
          "AudioPath": "assets/it-IT/Governor/15.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_9",
          "DialogueText": "Hmm... non ho molto da dire. È una zuppa nella media.",
          "AudioPath": "assets/it-IT/Governor/16.ogg"
        }
      ]
    }
  ]
}