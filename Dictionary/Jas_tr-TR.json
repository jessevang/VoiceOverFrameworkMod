        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_2_1",
          "DialogueText": "Bunun gibi tüyler ürpertici bir lağım kapısının anahtarı, profesördeki gibi büyük, paslı ve eski olmak zorunda.",
          "AudioPath": "assets/tr-TR/Jas/65.ogg"
        },
