        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "...Selam.",
          "AudioPath": "assets/tr-TR/Jas/48.ogg"
        },
        {
          "DialogueFrom": "Tue6",
          "DialogueText": "İstersen oyuncak bebeklerimle oynayabilirsin. Yalnızca işin bittiğinde saçlarını taramayı unutma.",
          "AudioPath": "assets/tr-TR/Jas/49.ogg"
        },
        {
