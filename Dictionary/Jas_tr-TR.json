        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Ayakkabıma kum girdi...",
          "AudioPath": "assets/tr-TR/Jas/30.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
