        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Yabancılar ile konuşmamam gerekiyor.",
          "AudioPath": "assets/tr-TR/Jas/39.ogg"
        },
        {
          "DialogueFrom": "Sun10",
          "DialogueText": "Shane'in vaftiz babam olduğunu biliyor muydun? Ebeveynlerimin bir arkadaşıydı.",
          "AudioPath": "assets/tr-TR/Jas/40.ogg"
        },
        {
          "DialogueFrom": "Sun10_1",
          "DialogueText": "...vefat ettiler. *burnunu çeker*",
          "AudioPath": "assets/tr-TR/Jas/41.ogg"
        },
        {
          "DialogueFrom": "Sun2",
          "DialogueText": "Shane'in kafası hep uçuk ve Marnie teyze hep meşgul... Bu yüzden kendi oyunlarımı icat etmem gerekiyor.",
          "AudioPath": "assets/tr-TR/Jas/42.ogg"
        },
        {
          "DialogueFrom": "Sun2_1",
          "DialogueText": "Oyuncaklarıma minnettarım.",
          "AudioPath": "assets/tr-TR/Jas/43.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Yabancılar ile konuşmamam gerekiyor...",
          "AudioPath": "assets/tr-TR/Jas/44.ogg"
        },
        {
          "DialogueFrom": "Thu2",
          "DialogueText": "Bayan Penny bana el yazısı ile yazmayı öğretiyor.",
