      "Entries": [
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900",
          "DialogueText": "Ez az illat... jöjj közelebb...",
          "AudioPath": "assets/hu-HU/Bear/1.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_1",
          "DialogueText": "Heh... Nem minden nap adatik meg, hogy beszélhetek egy emberrel. Úgy látom valamilyen 'erdei mágia' lakozik benned...",
          "AudioPath": "assets/hu-HU/Bear/2.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_2",
          "DialogueText": "Nos... a jegyzetemmel kapcsolatban... Elhoztad nekem amit kértem?",
          "AudioPath": "assets/hu-HU/Bear/3.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_3",
          "DialogueText": "HOoOoOoooh... ez az illat... ez nagyon édes dolog ám!",
          "AudioPath": "assets/hu-HU/Bear/4.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_3_1",
          "DialogueText": "...ez a kedvenc szószom... ettől nagyon boldog leszek.",
          "AudioPath": "assets/hu-HU/Bear/5.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_4",
          "DialogueText": "...Ma jó barátra leltél, .",
          "AudioPath": "assets/hu-HU/Bear/6.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_5",
          "DialogueText": "Van nálam egy különlegesség, amit meg kell mutatnom neked...",
          "AudioPath": "assets/hu-HU/Bear/7.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_alt",
          "DialogueText": "A medve átadta neked a speciális tudását...",
          "AudioPath": "assets/hu-HU/Bear/8.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_alt_1",
          "DialogueText": "A díszszeder és a szeder mostantól 3x annyi aranyat ér!",
          "AudioPath": "assets/hu-HU/Bear/9.ogg"
        }
      ]
    }
  ]
