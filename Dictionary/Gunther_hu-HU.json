{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Gunther.hu-HU",
      "VoicePackName": "Dictionary (Gunther - hu-HU)",
      "Character": "Gunther",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated",
          "DialogueText": "Mélységes...",
          "AudioPath": "assets/hu-HU/Gunther/1.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_1",
          "DialogueText": "Egy darab sincs az egész kollekcióból.",
          "AudioPath": "assets/hu-HU/Gunther/2.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_1",
          "DialogueText": "Mi az? Találtál valamit? Hadd nézzem!",
          "AudioPath": "assets/hu-HU/Gunther/3.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_2",
          "DialogueText": "Rendkívüli!",
          "AudioPath": "assets/hu-HU/Gunther/4.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_2_1",
          "DialogueText": "Ez nagyon régi.",
          "AudioPath": "assets/hu-HU/Gunther/5.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_2_2",
          "DialogueText": "Boldogan áttanulmányoznám teljes egészében... De ez a tiéd.",
          "AudioPath": "assets/hu-HU/Gunther/6.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_3",
          "DialogueText": "Hmmm...",
          "AudioPath": "assets/hu-HU/Gunther/7.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_3_1",
          "DialogueText": "Lenne hozzád egy kérésem.",
          "AudioPath": "assets/hu-HU/Gunther/8.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_3_2",
          "DialogueText": "Nem szeretnél eladományozni pár új ereklyét vagy ércet, amit találsz?",
          "AudioPath": "assets/hu-HU/Gunther/9.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_3_3",
          "DialogueText": "Együtt új felfedezéseket tehetnénk!",
          "AudioPath": "assets/hu-HU/Gunther/10.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_3_4",
          "DialogueText": "Ó, és ki tudja... Ha folyton adományoznál, talán találhatnék pár érdekes dolgot, amit cserébe odaadhatnék neked.",
          "AudioPath": "assets/hu-HU/Gunther/11.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_4",
          "DialogueText": "Gondolkozz el rajta, rendben?",
          "AudioPath": "assets/hu-HU/Gunther/12.ogg"
        },
        {
          "DialogueFrom": "Event:ArchaeologyHouse/0/n somethingToDonate/l somethingWasDonated_4_1",
          "DialogueText": "Ha adományozni szeretnél valamit, csak hozd az első asztalhoz.",
          "AudioPath": "assets/hu-HU/Gunther/13.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/66/e 295672/t 600 700/H",
          "DialogueText": "Ó, jó reggelt, ${úr",
          "AudioPath": "assets/hu-HU/Gunther/14_male.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/66/e 295672/t 600 700/H_1",
          "DialogueText": "asszony}$!",
          "AudioPath": "assets/hu-HU/Gunther/15_female.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/66/e 295672/t 600 700/H_2",
          "DialogueText": "Remélem nincs túl korán...",
          "AudioPath": "assets/hu-HU/Gunther/16.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/66/e 295672/t 600 700/H_3",
          "DialogueText": "Nos, csak be szerettem volna nézni és személyesen megköszönni azokat a csodálatos ereklyéket és ásványokat, amiket felfedeztél... sok munkát végeztél így egyedül!",
          "AudioPath": "assets/hu-HU/Gunther/17.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/66/e 295672/t 600 700/H_4",
          "DialogueText": "Tulajdonképpen pont most kaptam egy levelet az ereklyék regionális titkárságának irodájától... Megjutalmaznak minket az 'arany ásó' díjjal, amiért olyan sokat értünk el ezen a terepen!",
          "AudioPath": "assets/hu-HU/Gunther/18.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/66/e 295672/t 600 700/H_5",
          "DialogueText": "És ez mind neked köszönhető!",
          "AudioPath": "assets/hu-HU/Gunther/19.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/66/e 295672/t 600 700/H_6",
          "DialogueText": "Nos, azt hiszem hagynom kell téged dolgozni...",
          "AudioPath": "assets/hu-HU/Gunther/20.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/66/e 295672/t 600 700/H_1",
          "DialogueText": "Ó! Majd elfelejtettem!",
          "AudioPath": "assets/hu-HU/Gunther/21.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/66/e 295672/t 600 700/H_1_1",
          "DialogueText": "Van számodra egy ajándékom... Ez egy régi kulcs, ami a múzeumban volt elzárva legalább 100 éven át. Kissé rozsdás, de mégis gyönyörű.",
          "AudioPath": "assets/hu-HU/Gunther/22.ogg"
        },
        {
          "DialogueFrom": "Event:Farm/66/e 295672/t 600 700/H_alt",
          "DialogueText": "Megkaptad a 'Rozsdás kulcsot'! Bekerült a pénztárcádba",
          "AudioPath": "assets/hu-HU/Gunther/23.ogg"
        },
        {
          "DialogueFrom": "Event:Town/191393/Hn ccFishTank/Hn ccBulletin/Hn ccPantry/Hn ccVault/Hn ccBoilerRoom/Hn ccCraftsRoom/Hl jojaFishTank/Hl jojaPantry/Hl jojaVault/Hl jojaBoilerRoom/Hl jojaCraftsRoom/Hl JojaMember/w sunny/H_alt",
          "DialogueText": "Itt volt már az ideje!",
          "AudioPath": "assets/hu-HU/Gunther/24.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue",
          "DialogueText": "A múzeum fantasztikusan néz ki! Ismét köszönöm a segítségedet.",
          "AudioPath": "assets/hu-HU/Gunther/25.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue_1",
          "DialogueText": "Nem úgy tűnik, mintha lenne nálad a múzeumnak adományozható tárgy. Jobb lesz, ha kimész egy kicsit kincsekre vadászni, nem gondolod?",
          "AudioPath": "assets/hu-HU/Gunther/26.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue_2",
          "DialogueText": "Üdv néked! Üdvözöllek a Stardew Valley archeológiai irodában.",
          "AudioPath": "assets/hu-HU/Gunther/27.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue_2_1",
          "DialogueText": "Öhm... Ez kissé kínos, de az előző múzeumvezetőnk a teljes kollekciót magával vitte. Jelenleg egy ereklyénk sincs, ami ki lenne állítva.",
          "AudioPath": "assets/hu-HU/Gunther/28.ogg"
        },
        {
          "DialogueFrom": "ExtraDialogue_2_2",
          "DialogueText": "De reménykedem, hogy ezt hamarosan orvosolni tudom! Hé, ha netán találnál pár ereklyét vagy ásványt odakint... szólnál nekem?",
          "AudioPath": "assets/hu-HU/Gunther/29.ogg"
        }
      ]
    }
  ]
}