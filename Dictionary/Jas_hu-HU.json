      "Character": "Jas",
      "Language": "hu-HU",
      "Entries": [
        {
          "DialogueFrom": "AcceptBirthdayGift_Negative",
          "DialogueText": "Egy szülinapi ajándék? Ó... Ez nem tetszik.",
          "AudioPath": "assets/hu-HU/Jas/1.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive",
          "DialogueText": "Váó, egy szülinapi ajándék! Imádom!",
          "AudioPath": "assets/hu-HU/Jas/2.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)103",
          "DialogueText": "Óh, egy játékbaba! Kissé piszkos, de majd megfürdetem. Köszönöm!",
          "AudioPath": "assets/hu-HU/Jas/3.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Biztos vagy benne? Olyan jó az illata! Naponta csak egy gyűszűnyit fogok belőle inni.",
          "AudioPath": "assets/hu-HU/Jas/4.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(TR)FairyBox",
          "DialogueText": "Váó, ez annyira szép! És egy tündérke van benne? *kacag*... Hallom ahogy énekel. Ő lesz a titkos barátom.",
          "AudioPath": "assets/hu-HU/Jas/5.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Negative_alcohol_item",
          "DialogueText": "Öhm... Ezt nem szabadna meginnom. Ez nem csak felnőtteknek való?",
          "AudioPath": "assets/hu-HU/Jas/6.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Negative_coffee_item",
          "DialogueText": "Óh... Marnie néni még nem engedi nekem, hogy kávét igyak...",
          "AudioPath": "assets/hu-HU/Jas/7.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Positive_doll_item",
          "DialogueText": "Óh, őt kedvelem! Jól be fog illeszkedni a többi barátom közé.",
          "AudioPath": "assets/hu-HU/Jas/8.ogg"
        },
        {
          "DialogueFrom": "cropMatured_595",
          "DialogueText": "'Öhm... Találkoztál már tündérrel?_Igen_Váó! Én is látni akarok egyet._Nem_...Még én sem.'",
          "AudioPath": "assets/hu-HU/Jas/9.ogg"
        },
        {
          "DialogueFrom": "eventSeen_3910974",
          "DialogueText": "*kacag* ...nézd, igazi nyuszókás papucsom van!",
          "AudioPath": "assets/hu-HU/Jas/10.ogg"
        },
        {
          "DialogueFrom": "eventSeen_3910975",
          "DialogueText": "...Shane jól lesz?",
          "AudioPath": "assets/hu-HU/Jas/11.ogg"
        },
        {
          "DialogueFrom": "eventSeen_3910975_memory_oneweek",
          "DialogueText": "Egyszer olyan rémálmom volt, hogy ég a házunk...",
          "AudioPath": "assets/hu-HU/Jas/12.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "... Nem is ismerlek.",
          "AudioPath": "assets/hu-HU/Jas/13.ogg"
        },
        {
          "DialogueFrom": "Fri2",
          "DialogueText": "Tetszik a masnim?",
          "AudioPath": "assets/hu-HU/Jas/14.ogg"
        },
        {
          "DialogueFrom": "Fri4",
          "DialogueText": "Szeretek a farmon élni... A kedvencem a friss fagylalt a bocijaink tejéből.",
          "AudioPath": "assets/hu-HU/Jas/15.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Félek...",
          "AudioPath": "assets/hu-HU/Jas/16.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "Remélem az eső nem fogja bezöldíteni a ruhámat...",
          "AudioPath": "assets/hu-HU/Jas/17.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "...",
          "AudioPath": "assets/hu-HU/Jas/18.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "Szia...",
          "AudioPath": "assets/hu-HU/Jas/19.ogg"
        },
        {
          "DialogueFrom": "married",
          "DialogueText": "*sóhaj*... Remélem egy napon én is meg fogok házasodni.",
          "AudioPath": "assets/hu-HU/Jas/20.ogg"
        },
        {
          "DialogueFrom": "married_Shane",
          "DialogueText": "Remélem Shane továbbra is eljön meglátogatni engem...",
          "AudioPath": "assets/hu-HU/Jas/21.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Ó...Marnie nénit keresed?",
          "AudioPath": "assets/hu-HU/Jas/22.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "Helló, úr.",
          "AudioPath": "assets/hu-HU/Jas/23_male.ogg"
        },
        {
          "DialogueFrom": "Mon4_1",
          "DialogueText": "Helló, néni.",
          "AudioPath": "assets/hu-HU/Jas/24_female.ogg"
        },
        {
          "DialogueFrom": "Mon4_2",
          "DialogueText": "Jó móka farmon élni.",
          "AudioPath": "assets/hu-HU/Jas/25.ogg"
        },
        {
          "DialogueFrom": "movieTheater",
          "DialogueText": "Marnie néni azt mondta, hogy még nem vagyok elég idős ahhoz, hogy egyedül moziba menjek...",
          "AudioPath": "assets/hu-HU/Jas/26.ogg"
        },
        {
          "DialogueFrom": "reject_869",
          "DialogueText": "Fúj, undi!",
          "AudioPath": "assets/hu-HU/Jas/27.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Marnie néni mondta, hogy ne égjek le... De mikor gyorsan sétálok, akkor az jó érzés, szóval szerintem jól leszek.",
          "AudioPath": "assets/hu-HU/Jas/28.ogg"
        },
        {
          "DialogueFrom": "Resort_Entering",
          "DialogueText": "Látok egy rákot!",
          "AudioPath": "assets/hu-HU/Jas/29.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Homok került a cipőmbe...",
          "AudioPath": "assets/hu-HU/Jas/30.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Kagylókat gyűjtök... Csinálok egy nyakláncot!",
          "AudioPath": "assets/hu-HU/Jas/31.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Fűillatod van.",
          "AudioPath": "assets/hu-HU/Jas/32.ogg"
        },
        {
          "DialogueFrom": "Sat2",
          "DialogueText": "Néha folyami rákokra vadászok a folyóban. Ne mondd el Marnie néninek... de egyszer megetettem egyet egy bocival.",
          "AudioPath": "assets/hu-HU/Jas/33.ogg"
        },
        {
          "DialogueFrom": "Sat2_1",
          "DialogueText": "Nagyon ízlett neki.",
          "AudioPath": "assets/hu-HU/Jas/34.ogg"
        },
        {
          "DialogueFrom": "Sat8",
          "DialogueText": "Mindig olyan kedves vagy hozzám, ${bácsi",
          "AudioPath": "assets/hu-HU/Jas/35_male.ogg"
        },
        {
          "DialogueFrom": "Sat8_1",
          "DialogueText": "néni}$.",
          "AudioPath": "assets/hu-HU/Jas/36_female.ogg"
        },
        {
          "DialogueFrom": "Sat8_2",
          "DialogueText": "A felnőttek általában gonoszak és unalmasak, de te más vagy.",
          "AudioPath": "assets/hu-HU/Jas/37.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Őőő... sze... szeretnél valamit?",
          "AudioPath": "assets/hu-HU/Jas/38.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Nem szabadna idegenekkel beszélnem.",
          "AudioPath": "assets/hu-HU/Jas/39.ogg"
        },
        {
          "DialogueFrom": "Sun10",
          "DialogueText": "Tudtad, hogy Shane a keresztapám? A szüleim barátja volt.",
          "AudioPath": "assets/hu-HU/Jas/40.ogg"
        },
        {
          "DialogueFrom": "Sun10_1",
          "DialogueText": "...ők meghaltak. *szipp*",
          "AudioPath": "assets/hu-HU/Jas/41.ogg"
        },
        {
          "DialogueFrom": "Sun2",
          "DialogueText": "Shane keveset van itthon, Marnie néni folyton elfoglalt... szóval magamnak kell kitalálnom a játékokat.",
          "AudioPath": "assets/hu-HU/Jas/42.ogg"
        },
        {
          "DialogueFrom": "Sun2_1",
          "DialogueText": "Hálás vagyok az összes játékomért.",
          "AudioPath": "assets/hu-HU/Jas/43.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Nem szabadna idegenekkel beszélnem...",
          "AudioPath": "assets/hu-HU/Jas/44.ogg"
        },
        {
          "DialogueFrom": "Thu2",
          "DialogueText": "Penny néni folyóírásra tanít.",
          "AudioPath": "assets/hu-HU/Jas/45.ogg"
        },
        {
          "DialogueFrom": "Thu2_1",
          "DialogueText": "Olyan szép kézírása van.",
          "AudioPath": "assets/hu-HU/Jas/46.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "Hogy? *kuncog*",
          "AudioPath": "assets/hu-HU/Jas/47.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "...szia.",
          "AudioPath": "assets/hu-HU/Jas/48.ogg"
        },
        {
          "DialogueFrom": "Tue6",
          "DialogueText": "Játszhatsz a babáimmal, ha szeretnél. Csak mindig fésüld meg a hajukat, ha végeztél.",
          "AudioPath": "assets/hu-HU/Jas/49.ogg"
        },
        {
          "DialogueFrom": "Wed4",
          "DialogueText": "Marnie néni nem enged ki hat óra után. Ez nem igazság!",
          "AudioPath": "assets/hu-HU/Jas/50.ogg"
        },
        {
          "DialogueFrom": "Wed4_1",
          "DialogueText": "Már nem félek a sötéttől!",
          "AudioPath": "assets/hu-HU/Jas/51.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3900074/f Shane 2000/e 2118991/p Shane",
          "DialogueText": "Shane bácsi az istállóban van. Azt mondta... Izé... Azt mondta, hogy akar mutatni neked valamit.",
          "AudioPath": "assets/hu-HU/Jas/52.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3900074/f Shane 2000/e 2118991/p Shane_implied",
          "DialogueText": "White Chicken",
          "AudioPath": "assets/hu-HU/Jas/53.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3900074/f Shane 2000/e 2118991/p Shane_implied_1",
          "DialogueText": "Blue Chicken",
          "AudioPath": "assets/hu-HU/Jas/54.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910674/f Shane 1000_alt",
          "DialogueText": "...*sír*...",
          "AudioPath": "assets/hu-HU/Jas/55.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane",
          "DialogueText": "Ez... Ezek igazi nyuszi-ékszeres papucsok!",
          "AudioPath": "assets/hu-HU/Jas/56.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane_1",
          "DialogueText": "D... De ezek nagyon drágák!",
          "AudioPath": "assets/hu-HU/Jas/57.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane_1_1",
          "DialogueText": "Hogy volt erre pénzed?",
          "AudioPath": "assets/hu-HU/Jas/58.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L",
          "DialogueText": "Ez tetszik!",
          "AudioPath": "assets/hu-HU/Jas/59.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19",
          "DialogueText": "A természetes erőforrás olyan gazdag anyagi forrás, ami természetes közegben fordul elő, és nagy gazdasági értékkel rendelkezik.",
          "AudioPath": "assets/hu-HU/Jas/60.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19_1",
          "DialogueText": "Igeeen!",
          "AudioPath": "assets/hu-HU/Jas/61.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat",
          "DialogueText": "Fúj... ez büdös!",
          "AudioPath": "assets/hu-HU/Jas/62.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Azt hiszem Gunthernél van a kulcs.",
          "AudioPath": "assets/hu-HU/Jas/63.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_2",
          "DialogueText": "Egyszer láttam, hogy egy nagy, régi, rozsdás kulcs esett ki a zsebéből.",
          "AudioPath": "assets/hu-HU/Jas/64.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_2_1",
          "DialogueText": "Egy ilyen ijesztő csatornaajtóhoz pont ilyen nagy, régi és rozsdás kulcs illik.",
          "AudioPath": "assets/hu-HU/Jas/65.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_3",
          "DialogueText": "Valami mozgolódik odabent!",
          "AudioPath": "assets/hu-HU/Jas/66.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny",
          "DialogueText": "Számomra túl fűszeresek!",
          "AudioPath": "assets/hu-HU/Jas/67.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "Öhm... Te mit csinálsz a rovarokkal, Vincent?",
          "AudioPath": "assets/hu-HU/Jas/68.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_2",
          "DialogueText": "Az szörnyű! Ne, ne, ne, ne, ne...",
          "AudioPath": "assets/hu-HU/Jas/69.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3",
          "DialogueText": "bácsi? Kérlek... rakd vissza a rovarokat a fűbe, hogy békében élhessenek.",
          "AudioPath": "assets/hu-HU/Jas/70_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3_1",
          "DialogueText": "néni? Kérlek... rakd vissza a rovarokat a fűbe, hogy békében élhessenek.",
          "AudioPath": "assets/hu-HU/Jas/71_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_alt",
          "DialogueText": "*meghökken*!",
          "AudioPath": "assets/hu-HU/Jas/72.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_alt_1",
          "DialogueText": "Megtanultál újhagymát tisztítani... a rovarok bántása nélkül.",
          "AudioPath": "assets/hu-HU/Jas/73.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_alt_2",
          "DialogueText": "Mostantól az újhagymák 5x annyit érnek!",
          "AudioPath": "assets/hu-HU/Jas/74.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny",
          "DialogueText": "Penny néni? Leesett a matek könyvem borítója...",
          "AudioPath": "assets/hu-HU/Jas/75.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Aú! Egy csirke megcsípte a kisujjamat!",
          "AudioPath": "assets/hu-HU/Jas/76.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Múúú! Múúúúúú!Próbálok a bébi tehenekkel beszélni.",
          "AudioPath": "assets/hu-HU/Jas/77.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "De Marnie néni! Be akarok menni az útvesztőbe! Én nem félek!",
          "AudioPath": "assets/hu-HU/Jas/78.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Menjünk erre!",
          "AudioPath": "assets/hu-HU/Jas/79.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "Remélem találni fogok pár tojást, mielőtt Vincent mindent felkapkodja...",
          "AudioPath": "assets/hu-HU/Jas/80.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13_1",
          "DialogueText": "Mindent megteszek, hogy idén én legyek a győztes!",
          "AudioPath": "assets/hu-HU/Jas/81.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "Egy napon én leszek majd a virágkirálynő!",
          "AudioPath": "assets/hu-HU/Jas/82.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "*kacag* Jól csinálom?",
          "AudioPath": "assets/hu-HU/Jas/83.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Vincent nem táncol túl jól.",
          "AudioPath": "assets/hu-HU/Jas/84.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "D...De... Ezt meg is kell ennem?",
          "AudioPath": "assets/hu-HU/Jas/85.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "Remélem idén lesznek kicsinyek!",
          "AudioPath": "assets/hu-HU/Jas/86.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "Honnan tudják a medúzák, hogy merre ússzanak, ha nincs szemük?",
          "AudioPath": "assets/hu-HU/Jas/87.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Ohh! Ajándékok!",
          "AudioPath": "assets/hu-HU/Jas/88.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "Ki lehet már bontani az ajándékokat?",
          "AudioPath": "assets/hu-HU/Jas/89.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Egy répaorrt akarok neki adni!",
          "AudioPath": "assets/hu-HU/Jas/90.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "A halak nem fáznak odalent?",
          "AudioPath": "assets/hu-HU/Jas/91.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "*meghökken*...Váó! Köszönöm szépen!",
          "AudioPath": "assets/hu-HU/Jas/92.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Szeretem az ajándékokat! Köszönöm szépen!",
          "AudioPath": "assets/hu-HU/Jas/93.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Ez egy ajándék volna? Ó...",
          "AudioPath": "assets/hu-HU/Jas/94.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "Fúj. Ez undi.",
          "AudioPath": "assets/hu-HU/Jas/95.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Köszönöm!",
          "AudioPath": "assets/hu-HU/Jas/96.ogg"
        }
      ]
    }
  ]
}