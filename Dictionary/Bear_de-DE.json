      "VoicePackId": "Dictionary.Bear.de-DE",
      "VoicePackName": "Dictionary (Bear - de-DE)",
      "Character": "Bear",
      "Language": "de-DE",
      "Entries": [
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900",
          "DialogueText": "Dieser Geruch ... komm näher ...",
          "AudioPath": "assets/de-DE/Bear/1.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_1",
          "DialogueText": "Hehe ... Ich habe nicht jeden Tag die Gelegenheit, mit einem Menschen zu sprechen. Sieht so aus, als hättest du etwas 'Waldmagie' in dir ...",
          "AudioPath": "assets/de-DE/Bear/2.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_2",
          "DialogueText": "Also dann ... zu meiner Notiz ... Hast du's mir mitgebracht?",
          "AudioPath": "assets/de-DE/Bear/3.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_3",
          "DialogueText": "HOoOoOoooh... dieser Duft ... das riecht so süß!",
          "AudioPath": "assets/de-DE/Bear/4.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_3_1",
          "DialogueText": "... Es ist meine Lieblingssoße ... Die macht mich sehr glücklich.",
          "AudioPath": "assets/de-DE/Bear/5.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_4",
          "DialogueText": "... Du hast heute einen neuen Freund gefunden, .",
          "AudioPath": "assets/de-DE/Bear/6.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_5",
          "DialogueText": "Ich habe etwas Besonderes, das ich dir zeigen wollte ...",
          "AudioPath": "assets/de-DE/Bear/7.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_alt",
          "DialogueText": "Der Bär hat dir besonderes Wissen vermittelt ...",
          "AudioPath": "assets/de-DE/Bear/8.ogg"
        },
        {
          "DialogueFrom": "Event:Woods/2120303/S 23/i 724/t 600 1900_alt_1",
          "DialogueText": "Lachsbeeren und Bromberen sind jetzt 3x so viel Gold wert!",
          "AudioPath": "assets/de-DE/Bear/9.ogg"
        }
      ]
    }
  ]
