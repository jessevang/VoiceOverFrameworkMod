{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.MrQi.it-IT",
      "VoicePackName": "Dictionary (MrQi - it-IT)",
      "Character": "MrQi",
      "Language": "it-IT",
      "Entries": [
        {
          "DialogueFrom": "Event:QiNutRoom/10040609",
          "DialogueText": "Bene, bene, bene... Guarda chi ha raggiunto la mia stanza delle noci segreta.",
          "AudioPath": "assets/it-IT/MrQi/1.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1",
          "DialogueText": "Sono davvero colpito.",
          "AudioPath": "assets/it-IT/MrQi/2.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_1",
          "DialogueText": "Nonostante ti servisse solo una piccola parte delle noci per ottenere libero accesso a tutta l'isola, hai deciso di spingerti oltre.",
          "AudioPath": "assets/it-IT/MrQi/3.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_2",
          "DialogueText": "Il tuo desiderio di aprire questa porta segreta era talmente intenso, che hai impiegato tutte le tue forze per entrare.",
          "AudioPath": "assets/it-IT/MrQi/4.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_1_3",
          "DialogueText": "Questa grinta è ciò che ti distingue, giovane. Hai un'energia davvero speciale.",
          "AudioPath": "assets/it-IT/MrQi/5.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2",
          "DialogueText": "Detto questo...",
          "AudioPath": "assets/it-IT/MrQi/6.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2_1",
          "DialogueText": "Non penserai mica che la sfida finisca qui, vero?",
          "AudioPath": "assets/it-IT/MrQi/7.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_2_2",
          "DialogueText": "No... Hah. Hah. Hah... Al contrario. La sfida è appena iniziata.",
          "AudioPath": "assets/it-IT/MrQi/8.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_3",
          "DialogueText": "Alla tua sinistra, troverai una bacheca con delle opportunità molto interessanti.",
          "AudioPath": "assets/it-IT/MrQi/9.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_3_1",
          "DialogueText": "Queste sfide, che ho progettato specificamente per te, ti spingeranno fino al tuo limite estremo.",
          "AudioPath": "assets/it-IT/MrQi/10.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_4",
          "DialogueText": "Tuttavia, se supererai le mie sfide con successo, ti guadagnerai una valuta esclusiva... Io le chiamo 'Gemme Qi'",
          "AudioPath": "assets/it-IT/MrQi/11.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_5",
          "DialogueText": "Ma vedi di non mettertele in bocca, giovane... Sono terribilmente radioattive. Heh. heh. heh...",
          "AudioPath": "assets/it-IT/MrQi/12.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_6",
          "DialogueText": "Puoi usare la macchina alla tua destra per scambiare le Gemme Qi con ricompense rare e potenti.",
          "AudioPath": "assets/it-IT/MrQi/13.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_7",
          "DialogueText": "Stuzzicante, non trovi?",
          "AudioPath": "assets/it-IT/MrQi/14.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_7_1",
          "DialogueText": "Sì... Ho la sensazione che una persona come te si divertirà parecchio...",
          "AudioPath": "assets/it-IT/MrQi/15.ogg"
        },
        {
          "DialogueFrom": "Event:QiNutRoom/10040609_8",
          "DialogueText": "Ora, se vuoi scusarmi... Ho degli affari importanti di cui occuparmi.",
          "AudioPath": "assets/it-IT/MrQi/16.ogg"
        }
      ]
    }
  ]
}