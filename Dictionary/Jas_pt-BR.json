      "VoicePackId": "Dictionary.Jas.pt-BR",
      "VoicePackName": "Dictionary (Jas - pt-BR)",
      "Character": "Jas",
      "Language": "pt-BR",
      "Entries": [
        {
          "DialogueFrom": "AcceptBirthdayGift_Negative",
          "DialogueText": "Um presente de aniversário? Oh... eu não gostei disso.",
          "AudioPath": "assets/pt-BR/Jas/1.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive",
          "DialogueText": "Uau, um presente de aniversário! Eu amei!",
          "AudioPath": "assets/pt-BR/Jas/2.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)103",
          "DialogueText": "Oh, uma boneca! Ela tá um pouco suja, mas eu vou dar um banho nela. Obrigada!",
          "AudioPath": "assets/pt-BR/Jas/3.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Você tem certeza? O cheiro é tão bom! Eu vou beber só um pouquinho por dia.",
          "AudioPath": "assets/pt-BR/Jas/4.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(TR)FairyBox",
          "DialogueText": "Uau, é tão linda! E tem uma fada dentro? *risada*... Eu consigo ouvir ela cantando. Ela vai ser a minha amiga secreta.",
          "AudioPath": "assets/pt-BR/Jas/5.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Negative_alcohol_item",
          "DialogueText": "Hmm... Eu não posso beber. Isso não é para adultos?",
          "AudioPath": "assets/pt-BR/Jas/6.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Negative_coffee_item",
          "DialogueText": "Oh... A tia Marnie não me deixa tomar café ainda...",
          "AudioPath": "assets/pt-BR/Jas/7.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Positive_doll_item",
          "DialogueText": "Oh, eu gostei! Vai ficar perfeito com os meus outros amiguinhos.",
          "AudioPath": "assets/pt-BR/Jas/8.ogg"
        },
        {
          "DialogueFrom": "cropMatured_595",
          "DialogueText": "'Hm... Você já viu uma fada?_Sim_Uau! Eu também quero ver uma._Não_...Eu também não.'",
          "AudioPath": "assets/pt-BR/Jas/9.ogg"
        },
        {
          "DialogueFrom": "eventSeen_3910974",
          "DialogueText": "*risada* ...olha, eu tenho sandálias de joia de coelho!",
          "AudioPath": "assets/pt-BR/Jas/10.ogg"
        },
        {
          "DialogueFrom": "eventSeen_3910975",
          "DialogueText": "...O Shane vai ficar bem?",
          "AudioPath": "assets/pt-BR/Jas/11.ogg"
        },
        {
          "DialogueFrom": "eventSeen_3910975_memory_oneweek",
          "DialogueText": "Eu tive um pesadelo em que a casa estava pegando fogo...",
          "AudioPath": "assets/pt-BR/Jas/12.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "... Não conheço você.",
          "AudioPath": "assets/pt-BR/Jas/13.ogg"
        },
        {
          "DialogueFrom": "Fri2",
          "DialogueText": "Gostou do meu laço?",
          "AudioPath": "assets/pt-BR/Jas/14.ogg"
        },
        {
          "DialogueFrom": "Fri4",
          "DialogueText": "Gosto de viver na fazenda... minha comida favorita é sorvete feito com o leite de nossas vacas.",
          "AudioPath": "assets/pt-BR/Jas/15.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Estou com medo...",
          "AudioPath": "assets/pt-BR/Jas/16.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "Espero que o meu vestido não fique verde por causa da chuva...",
          "AudioPath": "assets/pt-BR/Jas/17.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "...",
          "AudioPath": "assets/pt-BR/Jas/18.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "Oi...",
          "AudioPath": "assets/pt-BR/Jas/19.ogg"
        },
        {
          "DialogueFrom": "married",
          "DialogueText": "*suspira*... Eu espero que eu consiga casar algum dia.",
          "AudioPath": "assets/pt-BR/Jas/20.ogg"
        },
        {
          "DialogueFrom": "married_Shane",
          "DialogueText": "Eu espero que o Shane ainda venha me visitar...",
          "AudioPath": "assets/pt-BR/Jas/21.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Ah... você está procurando pela tia Marnie?",
          "AudioPath": "assets/pt-BR/Jas/22.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "Oi, sr. .",
          "AudioPath": "assets/pt-BR/Jas/23_male.ogg"
        },
        {
          "DialogueFrom": "Mon4_1",
          "DialogueText": "Oi, srª.",
          "AudioPath": "assets/pt-BR/Jas/24_female.ogg"
        },
        {
          "DialogueFrom": "Mon4_2",
          "DialogueText": "É divertido morar em uma fazenda.",
          "AudioPath": "assets/pt-BR/Jas/25.ogg"
        },
        {
          "DialogueFrom": "movieTheater",
          "DialogueText": "A tia Marnie falou que não tenho idade para ir sozinha no cinema...",
          "AudioPath": "assets/pt-BR/Jas/26.ogg"
        },
        {
          "DialogueFrom": "reject_869",
          "DialogueText": "Ew, nojento!",
          "AudioPath": "assets/pt-BR/Jas/27.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "A Tia Marnie falou pra eu não me queimar... Mas quando eu ando rápido, fica fresco, então acho que vou ficar bem.",
          "AudioPath": "assets/pt-BR/Jas/28.ogg"
        },
        {
          "DialogueFrom": "Resort_Entering",
          "DialogueText": "Eu vejo um caranguejo!",
          "AudioPath": "assets/pt-BR/Jas/29.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Tem areia no meu sapato...",
          "AudioPath": "assets/pt-BR/Jas/30.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Estou coletando conchas... eu vou fazer um colar!",
          "AudioPath": "assets/pt-BR/Jas/31.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Você tem cheiro de grama.",
          "AudioPath": "assets/pt-BR/Jas/32.ogg"
        },
        {
          "DialogueFrom": "Sat2",
          "DialogueText": "Às vezes, eu procuro por lagostins no rio. Não conte para a Tia Marnie... mas já alimentei uma vaca uma vez.",
          "AudioPath": "assets/pt-BR/Jas/33.ogg"
        },
        {
          "DialogueFrom": "Sat2_1",
          "DialogueText": "Ela amou.",
          "AudioPath": "assets/pt-BR/Jas/34.ogg"
        },
        {
          "DialogueFrom": "Sat8",
          "DialogueText": "Você é sempre tão gentil comigo, ${seu",
          "AudioPath": "assets/pt-BR/Jas/35_male.ogg"
        },
        {
          "DialogueFrom": "Sat8_1",
          "DialogueText": "dona}$ .",
          "AudioPath": "assets/pt-BR/Jas/36_female.ogg"
        },
        {
          "DialogueFrom": "Sat8_2",
          "DialogueText": "Os adultos geralmente são cruéis e chatos, mas você é diferente.",
          "AudioPath": "assets/pt-BR/Jas/37.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Hã... v-você quer alguma coisa?",
          "AudioPath": "assets/pt-BR/Jas/38.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Não posso falar com estranhos.",
          "AudioPath": "assets/pt-BR/Jas/39.ogg"
        },
        {
          "DialogueFrom": "Sun10",
          "DialogueText": "Você sabia que o Shane é o meu padrinho? Ele era um amigo dos meus pais.",
          "AudioPath": "assets/pt-BR/Jas/40.ogg"
        },
        {
          "DialogueFrom": "Sun10_1",
          "DialogueText": "... eles morreram. Snif...",
          "AudioPath": "assets/pt-BR/Jas/41.ogg"
        },
        {
          "DialogueFrom": "Sun2",
          "DialogueText": "O Shane sai muito, tia Marnie está ocupada o tempo todo... então tenho que inventar minhas próprias brincadeiras.",
          "AudioPath": "assets/pt-BR/Jas/42.ogg"
        },
        {
          "DialogueFrom": "Sun2_1",
          "DialogueText": "Fico grata por todos os meus brinquedos.",
          "AudioPath": "assets/pt-BR/Jas/43.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Não posso falar com estranhos...",
          "AudioPath": "assets/pt-BR/Jas/44.ogg"
        },
        {
          "DialogueFrom": "Thu2",
          "DialogueText": "Tia Penny está me ensinando a escrever com letra de mão.",
          "AudioPath": "assets/pt-BR/Jas/45.ogg"
        },
        {
          "DialogueFrom": "Thu2_1",
          "DialogueText": "A letra dela é tão bonita!",
          "AudioPath": "assets/pt-BR/Jas/46.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "Quê? Hi, hi",
          "AudioPath": "assets/pt-BR/Jas/47.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "... oi.",
          "AudioPath": "assets/pt-BR/Jas/48.ogg"
        },
        {
          "DialogueFrom": "Tue6",
          "DialogueText": "Você pode brincar com minhas bonecas, se quiser. Só escove o cabelo delas quando acabar.",
          "AudioPath": "assets/pt-BR/Jas/49.ogg"
        },
        {
          "DialogueFrom": "Wed4",
          "DialogueText": "A tia Marnie não me deixa sair depois das 6. Não é justo!",
          "AudioPath": "assets/pt-BR/Jas/50.ogg"
        },
        {
          "DialogueFrom": "Wed4_1",
          "DialogueText": "Não tenho mais medo do escuro!",
          "AudioPath": "assets/pt-BR/Jas/51.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3900074/f Shane 2000/e 2118991/p Shane",
          "DialogueText": "O tio Shane está no estábulo. Ele disse... hã... ele disse que quer contar algo a você.",
          "AudioPath": "assets/pt-BR/Jas/52.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3900074/f Shane 2000/e 2118991/p Shane_implied",
          "DialogueText": "White Chicken",
          "AudioPath": "assets/pt-BR/Jas/53.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3900074/f Shane 2000/e 2118991/p Shane_implied_1",
          "DialogueText": "Blue Chicken",
          "AudioPath": "assets/pt-BR/Jas/54.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910674/f Shane 1000_alt",
          "DialogueText": "*Chuif*...",
          "AudioPath": "assets/pt-BR/Jas/55.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane",
          "DialogueText": "Mas... São aquelas sandálias de joia de coelho!!",
          "AudioPath": "assets/pt-BR/Jas/56.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane_1",
          "DialogueText": "Ma... Mas são tão caras!",
          "AudioPath": "assets/pt-BR/Jas/57.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane_1_1",
          "DialogueText": "Como você conseguiu comprar?",
          "AudioPath": "assets/pt-BR/Jas/58.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L",
          "DialogueText": "Eu adorei!",
          "AudioPath": "assets/pt-BR/Jas/59.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19",
          "DialogueText": "Um 'recurso natural' é uma fonte de riqueza que ocorre em estado natural e possui valor econômico.",
          "AudioPath": "assets/pt-BR/Jas/60.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19_1",
          "DialogueText": "Iiiuurru!",
          "AudioPath": "assets/pt-BR/Jas/61.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat",
          "DialogueText": "Eca! Que cheiro horrível.",
          "AudioPath": "assets/pt-BR/Jas/62.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Acho que Gunther tem a chave.",
          "AudioPath": "assets/pt-BR/Jas/63.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_2",
          "DialogueText": "Eu vi uma grande chave enferrujada cair do bolso dele uma vez.",
          "AudioPath": "assets/pt-BR/Jas/64.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_2_1",
          "DialogueText": "Uma porta assustadora assim, no esgoto, é a combinação perfeita para uma grande chave enferrujada.",
          "AudioPath": "assets/pt-BR/Jas/65.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_3",
          "DialogueText": "Tem algo se movendo ali!",
          "AudioPath": "assets/pt-BR/Jas/66.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny",
          "DialogueText": "São apimentadas demais para mim!",
          "AudioPath": "assets/pt-BR/Jas/67.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "Mmm... O que você faz com os insetos, Vincent?",
          "AudioPath": "assets/pt-BR/Jas/68.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_2",
          "DialogueText": "Isso é horrível! Não, não, não, não, não...",
          "AudioPath": "assets/pt-BR/Jas/69.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3",
          "DialogueText": "Sr. ? Por favor... coloque os insetos de volta na grama para que eles possam viver em paz.",
          "AudioPath": "assets/pt-BR/Jas/70_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3_1",
          "DialogueText": "Senhorita ? Por favor... coloque os insetos de volta na grama para que eles possam viver em paz.",
          "AudioPath": "assets/pt-BR/Jas/71_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_alt",
          "DialogueText": "*arfada*!",
          "AudioPath": "assets/pt-BR/Jas/72.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_alt_1",
          "DialogueText": "Você aprendeu a remover a cebolinha... sem machucar os insetos.",
          "AudioPath": "assets/pt-BR/Jas/73.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_alt_2",
          "DialogueText": "Cebolinhas agora valem 5x seu valor em ouro!",
          "AudioPath": "assets/pt-BR/Jas/74.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny",
          "DialogueText": "Srta. Penny? A capa do meu caderno de matemática caiu...",
          "AudioPath": "assets/pt-BR/Jas/75.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Au! Uma galinha bicou o meu dedinho!",
          "AudioPath": "assets/pt-BR/Jas/76.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Mooo! Moooooo!Estou tentando falar com os bezerros.",
          "AudioPath": "assets/pt-BR/Jas/77.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "Aaah, tia Marnie! Eu quero ir no labirinto! Eu não tô com medo!",
          "AudioPath": "assets/pt-BR/Jas/78.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Vamos por aqui!",
          "AudioPath": "assets/pt-BR/Jas/79.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "Espero encontrar alguns ovos antes que o Vincent ache todos...",
          "AudioPath": "assets/pt-BR/Jas/80.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13_1",
          "DialogueText": "Eu vou dar o meu melhor para ganhar esse ano!",
          "AudioPath": "assets/pt-BR/Jas/81.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "Algum dia, vou ser a rainha das flores!",
          "AudioPath": "assets/pt-BR/Jas/82.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "*risada* Estou fazendo isso certo?",
          "AudioPath": "assets/pt-BR/Jas/83.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "O Vincent não dança muito bem.",
          "AudioPath": "assets/pt-BR/Jas/84.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "M... Mas... Eu tenho que comer isso?",
          "AudioPath": "assets/pt-BR/Jas/85.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "Espero que nasçam bebês este ano!",
          "AudioPath": "assets/pt-BR/Jas/86.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "Como as medusas sabem para onde nadar se elas não tem olhos?",
          "AudioPath": "assets/pt-BR/Jas/87.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Aaah! Presentes!",
          "AudioPath": "assets/pt-BR/Jas/88.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "Já é a hora de abrir de presentes?",
          "AudioPath": "assets/pt-BR/Jas/89.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Quero dar a ele um nariz de cenoura!",
          "AudioPath": "assets/pt-BR/Jas/90.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "Os peixes não ficam com frio ali embaixo?",
          "AudioPath": "assets/pt-BR/Jas/91.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "*Suspira*... uau! Obrigada!",
          "AudioPath": "assets/pt-BR/Jas/92.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Eu amo presentes! Obrigada!",
          "AudioPath": "assets/pt-BR/Jas/93.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Um presente? Ah...",
          "AudioPath": "assets/pt-BR/Jas/94.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "Eca. Que nojo.",
          "AudioPath": "assets/pt-BR/Jas/95.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Obrigada!",
          "AudioPath": "assets/pt-BR/Jas/96.ogg"
        }
      ]
    }
  ]
}