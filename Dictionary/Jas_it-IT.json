      "Entries": [
        {
          "DialogueFrom": "AcceptBirthdayGift_Negative",
          "DialogueText": "Un regalo di compleanno? Oh... non mi piace.",
          "AudioPath": "assets/it-IT/Jas/1.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive",
          "DialogueText": "Wow, un regalo di compleanno! Mi piace tantissimo!",
          "AudioPath": "assets/it-IT/Jas/2.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)103",
          "DialogueText": "Oh, una bambolina! È un po' sporca, ma le farò il bagnetto. Grazie!",
          "AudioPath": "assets/it-IT/Jas/3.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Davvero me lo regali? Ha un profumo buonissimo! Ne berrò solo un goccio al giorno.",
          "AudioPath": "assets/it-IT/Jas/4.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(TR)FairyBox",
          "DialogueText": "Wow, com'è bella! E dentro c'è una fatina? *risolino*... La sento cantare. Sarà la mia amichetta segreta.",
          "AudioPath": "assets/it-IT/Jas/5.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Negative_alcohol_item",
          "DialogueText": "Ehmmm... Questa è una cosa che non dovrei bere. Non è solo per i grandi?",
          "AudioPath": "assets/it-IT/Jas/6.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Negative_coffee_item",
          "DialogueText": "Oh... Zia Marnie non mi fa ancora bere il caffè...",
          "AudioPath": "assets/it-IT/Jas/7.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Positive_doll_item",
          "DialogueText": "Oh, mi piace! Si troverà benissimo con gli altri miei amici.",
          "AudioPath": "assets/it-IT/Jas/8.ogg"
        },
        {
          "DialogueFrom": "cropMatured_595",
          "DialogueText": "'Ehm... Hai mai visto una fata?_Sì_Wow! Anch'io voglio vederla._No_...Neppure io.'",
          "AudioPath": "assets/it-IT/Jas/9.ogg"
        },
        {
          "DialogueFrom": "eventSeen_3910974",
          "DialogueText": "*risatina* ...guarda, ho delle vere pantofole a coniglietto!",
          "AudioPath": "assets/it-IT/Jas/10.ogg"
        },
        {
          "DialogueFrom": "eventSeen_3910975",
          "DialogueText": "...Shane sta bene, vero?",
          "AudioPath": "assets/it-IT/Jas/11.ogg"
        },
        {
          "DialogueFrom": "eventSeen_3910975_memory_oneweek",
          "DialogueText": "Ho fatto un incubo dove la casa andava a fuoco...",
          "AudioPath": "assets/it-IT/Jas/12.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "... Non ti conosco.",
          "AudioPath": "assets/it-IT/Jas/13.ogg"
        },
        {
          "DialogueFrom": "Fri2",
          "DialogueText": "Ti piace il mio fiocco?",
          "AudioPath": "assets/it-IT/Jas/14.ogg"
        },
        {
          "DialogueFrom": "Fri4",
          "DialogueText": "Mi piace vivere in una fattoria... La cosa più bella è il gelato fresco fatto col latte delle nostre mucche.",
          "AudioPath": "assets/it-IT/Jas/15.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Ho paura...",
          "AudioPath": "assets/it-IT/Jas/16.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "Spero che la pioggia non mi farà diventare verde il vestito...",
          "AudioPath": "assets/it-IT/Jas/17.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "...",
          "AudioPath": "assets/it-IT/Jas/18.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "Ciao...",
          "AudioPath": "assets/it-IT/Jas/19.ogg"
        },
        {
          "DialogueFrom": "married",
          "DialogueText": "*sospira*... Spero di potermi sposare anch'io, un giorno.",
          "AudioPath": "assets/it-IT/Jas/20.ogg"
        },
        {
          "DialogueFrom": "married_Shane",
          "DialogueText": "Spero che Shane verrà comunque a trovarmi...",
          "AudioPath": "assets/it-IT/Jas/21.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Oh... Cerchi zia Marnie?",
          "AudioPath": "assets/it-IT/Jas/22.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "Ciao, signor .",
          "AudioPath": "assets/it-IT/Jas/23_male.ogg"
        },
        {
          "DialogueFrom": "Mon4_1",
          "DialogueText": "Ciao, signorina .",
          "AudioPath": "assets/it-IT/Jas/24_female.ogg"
        },
        {
          "DialogueFrom": "Mon4_2",
          "DialogueText": "È divertente vivere in una fattoria.",
          "AudioPath": "assets/it-IT/Jas/25.ogg"
        },
        {
          "DialogueFrom": "movieTheater",
          "DialogueText": "Zia Marnie dice che non sono abbastanza grande per vedere un film da sola...",
          "AudioPath": "assets/it-IT/Jas/26.ogg"
        },
        {
          "DialogueFrom": "reject_869",
          "DialogueText": "Bleah, che schifo!",
          "AudioPath": "assets/it-IT/Jas/27.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Zia Marnie mi ha detto di non scottarmi... Ma quando cammino in fretta sento fresco, credo di essere a posto.",
          "AudioPath": "assets/it-IT/Jas/28.ogg"
        },
        {
          "DialogueFrom": "Resort_Entering",
          "DialogueText": "Vedo un granchio!",
          "AudioPath": "assets/it-IT/Jas/29.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Ho la sabbia nelle scarpe...",
          "AudioPath": "assets/it-IT/Jas/30.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Sto raccogliendo conchiglie... Ci farò una collana!",
          "AudioPath": "assets/it-IT/Jas/31.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Profumi di erba.",
          "AudioPath": "assets/it-IT/Jas/32.ogg"
        },
        {
          "DialogueFrom": "Sat2",
          "DialogueText": "Ogni tanto vado in cerca di gamberi nel fiume. Non dirlo a zia Marnie... ma una volta ne ho dato uno a una mucca.",
          "AudioPath": "assets/it-IT/Jas/33.ogg"
        },
        {
          "DialogueFrom": "Sat2_1",
          "DialogueText": "Si è leccata i baffi.",
          "AudioPath": "assets/it-IT/Jas/34.ogg"
        },
        {
          "DialogueFrom": "Sat8",
          "DialogueText": "Sei sempre così gentile con me, ${signor",
          "AudioPath": "assets/it-IT/Jas/35_male.ogg"
        },
        {
          "DialogueFrom": "Sat8_1",
          "DialogueText": "signorina}$ .",
          "AudioPath": "assets/it-IT/Jas/36_female.ogg"
        },
        {
          "DialogueFrom": "Sat8_2",
          "DialogueText": "Di solito i grandi sono noiosi e cattivi, ma tu sei differente.",
          "AudioPath": "assets/it-IT/Jas/37.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Ehm... v...vuoi qualcosa?",
          "AudioPath": "assets/it-IT/Jas/38.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Non devo parlare con gli sconosciuti.",
          "AudioPath": "assets/it-IT/Jas/39.ogg"
        },
        {
          "DialogueFrom": "Sun10",
          "DialogueText": "Sapevi che Shane è il mio padrino? Era amico dei miei genitori.",
          "AudioPath": "assets/it-IT/Jas/40.ogg"
        },
        {
          "DialogueFrom": "Sun10_1",
          "DialogueText": "...loro sono morti.*sniff*",
          "AudioPath": "assets/it-IT/Jas/41.ogg"
        },
        {
          "DialogueFrom": "Sun2",
          "DialogueText": "Shane non c'è quasi mai, e zia Marnie ha sempre da fare... perciò mi devo inventare dei giochi da sola.",
          "AudioPath": "assets/it-IT/Jas/42.ogg"
        },
        {
          "DialogueFrom": "Sun2_1",
          "DialogueText": "Per fortuna che ho tanti giocattoli.",
          "AudioPath": "assets/it-IT/Jas/43.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Non devo parlare con gli sconosciuti...",
          "AudioPath": "assets/it-IT/Jas/44.ogg"
        },
        {
          "DialogueFrom": "Thu2",
          "DialogueText": "La signorina Penny mi sta insegnando a scrivere in corsivo.",
          "AudioPath": "assets/it-IT/Jas/45.ogg"
        },
        {
          "DialogueFrom": "Thu2_1",
          "DialogueText": "Ha una scrittura così bella, lei.",
          "AudioPath": "assets/it-IT/Jas/46.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "Che c'è? *risatina*",
          "AudioPath": "assets/it-IT/Jas/47.ogg"
        },
        {
          "DialogueFrom": "Tue_1",
          "DialogueText": "...ciao.",
          "AudioPath": "assets/it-IT/Jas/48.ogg"
        },
        {
          "DialogueFrom": "Tue6",
          "DialogueText": "Se vuoi puoi giocare con le mie bambole. Basta che ti ricordi di spazzolargli i capelli, quando hai fatto.",
          "AudioPath": "assets/it-IT/Jas/49.ogg"
        },
        {
          "DialogueFrom": "Wed4",
          "DialogueText": "Zia Marnie non mi fa uscire dopo le 6. Non è giusto!",
          "AudioPath": "assets/it-IT/Jas/50.ogg"
        },
        {
          "DialogueFrom": "Wed4_1",
          "DialogueText": "Non ho più paura del buio, io!",
          "AudioPath": "assets/it-IT/Jas/51.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3900074/f Shane 2000/e 2118991/p Shane",
          "DialogueText": "Zio Shane è nel granaio. Ha detto... Ehm... Ha detto di dirti che vuole mostrarti una cosa.",
          "AudioPath": "assets/it-IT/Jas/52.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3900074/f Shane 2000/e 2118991/p Shane_implied",
          "DialogueText": "White Chicken",
          "AudioPath": "assets/it-IT/Jas/53.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3900074/f Shane 2000/e 2118991/p Shane_implied_1",
          "DialogueText": "Blue Chicken",
          "AudioPath": "assets/it-IT/Jas/54.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910674/f Shane 1000_alt",
          "DialogueText": "...*singhiozza*...",
          "AudioPath": "assets/it-IT/Jas/55.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane",
          "DialogueText": "Qu... Queste sono delle vere pantofole-coniglietto ingioiellate!!",
          "AudioPath": "assets/it-IT/Jas/56.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane_1",
          "DialogueText": "M... Ma sono costosissime!",
          "AudioPath": "assets/it-IT/Jas/57.ogg"
        },
        {
          "DialogueFrom": "Event:AnimalShop/3910974/f Shane 1700/e 3910975/p Shane_1_1",
          "DialogueText": "Come le hai comprate?",
          "AudioPath": "assets/it-IT/Jas/58.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L",
          "DialogueText": "Mi è piaciuta tantissimo!",
          "AudioPath": "assets/it-IT/Jas/59.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19",
          "DialogueText": "Una risorsa naturale è una fonte materiale di ricchezza che si trova allo stato naturale e ha valore economico.",
          "AudioPath": "assets/it-IT/Jas/60.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19_1",
          "DialogueText": "Sììì!",
          "AudioPath": "assets/it-IT/Jas/61.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat",
          "DialogueText": "Ooh... puzza.",
          "AudioPath": "assets/it-IT/Jas/62.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Mi sa che la chiave ce l'ha Gunther.",
          "AudioPath": "assets/it-IT/Jas/63.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_2",
          "DialogueText": "Una volta ho visto cadergli di tasca una grossa chiave arrugginita.",
          "AudioPath": "assets/it-IT/Jas/64.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_2_1",
          "DialogueText": "Una portaccia di fogna come questa deve per forza fare il paio con una grossa chiave arrugginita.",
          "AudioPath": "assets/it-IT/Jas/65.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_3",
          "DialogueText": "C'è qualcosa che si muove là dentro!",
          "AudioPath": "assets/it-IT/Jas/66.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny",
          "DialogueText": "Sono troppo piccanti per me!",
          "AudioPath": "assets/it-IT/Jas/67.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "Ehm... Cosa ci fai con gli insetti, Vincent?",
          "AudioPath": "assets/it-IT/Jas/68.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_2",
          "DialogueText": "Ma è orribile! No, no, no, no, no...",
          "AudioPath": "assets/it-IT/Jas/69.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3",
          "DialogueText": "Signor ? Per favore... rimetti gli insetti nell’erba, così possono vivere in pace.",
          "AudioPath": "assets/it-IT/Jas/70_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3_1",
          "DialogueText": "Signorina ? Per favore... rimetti gli insetti nell’erba, così possono vivere in pace.",
          "AudioPath": "assets/it-IT/Jas/71_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_alt",
          "DialogueText": "*sobbalza*!",
          "AudioPath": "assets/it-IT/Jas/72.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_alt_1",
          "DialogueText": "Hai imparato a pulire le cipolle primaverili... senza far male agli insetti.",
          "AudioPath": "assets/it-IT/Jas/73.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_alt_2",
          "DialogueText": "Ora le cipolle primaverili valgono 5x tanto!",
          "AudioPath": "assets/it-IT/Jas/74.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny",
          "DialogueText": "Signorina Penny? Mi è caduta la copertina dal libro di matematica...",
          "AudioPath": "assets/it-IT/Jas/75.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Ahia! Una gallina mi ha beccato il mignolino!",
          "AudioPath": "assets/it-IT/Jas/76.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Muuu! Muuuuuu!Sto provando a parlare con le mucchette.",
          "AudioPath": "assets/it-IT/Jas/77.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "Ma zia Marnie! Voglio andare nel labirinto! Non ho paura!",
          "AudioPath": "assets/it-IT/Jas/78.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Andiamo da questa parte!",
          "AudioPath": "assets/it-IT/Jas/79.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "Spero di trovare qualche uovo prima che Vincent se le prenda tutte...",
          "AudioPath": "assets/it-IT/Jas/80.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13_1",
          "DialogueText": "Quest'anno farò del mio meglio per vincere!",
          "AudioPath": "assets/it-IT/Jas/81.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "Un giorno sarò io la regina dei fiori!",
          "AudioPath": "assets/it-IT/Jas/82.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "*risatina* Vado bene così?",
          "AudioPath": "assets/it-IT/Jas/83.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Vincent non è un gran ballerino.",
          "AudioPath": "assets/it-IT/Jas/84.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "M...Ma... Devo mangiarla per forza?",
          "AudioPath": "assets/it-IT/Jas/85.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "Spero che quest'anno ci siano i piccoli!",
          "AudioPath": "assets/it-IT/Jas/86.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "Come fanno le meduse a sapere dove nuotare se non hanno gli occhi?",
          "AudioPath": "assets/it-IT/Jas/87.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Ohh! Regali!",
          "AudioPath": "assets/it-IT/Jas/88.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "Non è ancora ora di aprire i regali?",
          "AudioPath": "assets/it-IT/Jas/89.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Voglio mettergli una carota per naso!",
          "AudioPath": "assets/it-IT/Jas/90.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "I pesci non avranno freddo là sotto?",
          "AudioPath": "assets/it-IT/Jas/91.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "*Sussulta*...Wow!! Grazie!",
          "AudioPath": "assets/it-IT/Jas/92.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Adoro i regali! Grazie!",
          "AudioPath": "assets/it-IT/Jas/93.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Questo è un regalo? Oh...",
          "AudioPath": "assets/it-IT/Jas/94.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "Bleah. Che schifo.",
          "AudioPath": "assets/it-IT/Jas/95.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Grazie!",
