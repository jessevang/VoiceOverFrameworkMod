        {
          "DialogueFrom": "AcceptGift_Negative_alcohol_item",
          "DialogueText": "*koklar*... Bu da ne? Çok kötü kokuyor... Annneeeeee!",
          "AudioPath": "assets/tr-TR/Vincent/6.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Öf, bu akşam yine kaynamış pancar var...",
          "AudioPath": "assets/tr-TR/Vincent/7.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Ah, aslında hayır, annem bu akşam mercimek çorbası yapacaktı.",
          "AudioPath": "assets/tr-TR/Vincent/8.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Anne! Dışarı çıkıp oynamak istiyorum!",
          "AudioPath": "assets/tr-TR/Vincent/9.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "yosun ile kaplı.",
          "AudioPath": "assets/tr-TR/Vincent/10.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "Ah, tanımadığım biri. Ben Vincent.",
          "AudioPath": "assets/tr-TR/Vincent/11.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "Annem yabancılarla konuşma dedi... ama sen iyi birine benziyorsun.",
          "AudioPath": "assets/tr-TR/Vincent/12.ogg"
        },
        {
          "DialogueFrom": "married_Sam",
          "DialogueText": "Sammy'mi benden alma!",
          "AudioPath": "assets/tr-TR/Vincent/13.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Büyüdüğümde abim gibi olmak istiyorum!",
          "AudioPath": "assets/tr-TR/Vincent/14.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Oldu, sonra görüşürüz!",
          "AudioPath": "assets/tr-TR/Vincent/15.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "kent#Babamın dönmesine sevindim... artık hatırladığımdan çok daha farklı...|Babam hiç buralarda olmuyor... onunla oynamayı özledim.",
          "AudioPath": "assets/tr-TR/Vincent/16.ogg"
        },
        {
          "DialogueFrom": "reject_869",
          "DialogueText": "Ah! Ne güzel bir şey bulmuşsun!",
          "AudioPath": "assets/tr-TR/Vincent/17.ogg"
        },
        {
          "DialogueFrom": "reject_869_1",
          "DialogueText": "Annem bunu almama izin vermez...",
          "AudioPath": "assets/tr-TR/Vincent/18.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Yakından bakarsan kumdan küçük böceklerin çıktığını görebilirsin.",
          "AudioPath": "assets/tr-TR/Vincent/19.ogg"
        },
        {
          "DialogueFrom": "Resort_Entering",
          "DialogueText": "Yolculuk eğlenceliydi! Ben de bir gün kayıkçı olmak istiyorum!",
          "AudioPath": "assets/tr-TR/Vincent/20.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Buraya yeniden gelmek istiyorum!",
          "AudioPath": "assets/tr-TR/Vincent/21.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Köpek balığı görmek istiyorum!",
          "AudioPath": "assets/tr-TR/Vincent/22.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "Bayan Penny her hafta bana yeni kitaplar okutuyor.",
          "AudioPath": "assets/tr-TR/Vincent/23.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Bazen de kitabı benim seçmeme izin veriyor!",
          "AudioPath": "assets/tr-TR/Vincent/24.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Babam yakında dönecekmiş!",
          "AudioPath": "assets/tr-TR/Vincent/25.ogg"
        },
        {
          "DialogueFrom": "Sat_1_1",
          "DialogueText": "Umarım bana oyuncak getirir.",
          "AudioPath": "assets/tr-TR/Vincent/26.ogg"
        },
        {
          "DialogueFrom": "Sat8",
          "DialogueText": "kent#Babam döndüğünden beri çok garip davranıyor. Ona ne oldu acaba?",
          "AudioPath": "assets/tr-TR/Vincent/27.ogg"
        },
        {
          "DialogueFrom": "Sat8_1",
          "DialogueText": "Artık benimle oynamak istemiyor.|Acaba babam eve dönecek mi...",
          "AudioPath": "assets/tr-TR/Vincent/28.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Merhaba, bayım!",
          "AudioPath": "assets/tr-TR/Vincent/29_male.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Merhaba, hanımefendi!",
          "AudioPath": "assets/tr-TR/Vincent/30_female.ogg"
        },
        {
          "DialogueFrom": "Sun10",
          "DialogueText": "Merhaba Amca!",
          "AudioPath": "assets/tr-TR/Vincent/31_male.ogg"
        },
        {
          "DialogueFrom": "Sun10_1",
          "DialogueText": "Merhaba Teyze!",
          "AudioPath": "assets/tr-TR/Vincent/32_female.ogg"
        },
        {
          "DialogueFrom": "Sun10_2",
          "DialogueText": "En sevdiğim büyük sensin... Anneme söyleme ama.",
          "AudioPath": "assets/tr-TR/Vincent/33.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Acıktım... Annem nerde?",
          "AudioPath": "assets/tr-TR/Vincent/34.ogg"
        },
        {
          "DialogueFrom": "Thu2",
          "DialogueText": "Çoğu yetişkin kadar sıkıcı değilsin!",
          "AudioPath": "assets/tr-TR/Vincent/35.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "*iç çeker*... Annem bugün şeker almama izin vermedi...",
          "AudioPath": "assets/tr-TR/Vincent/36.ogg"
        },
        {
          "DialogueFrom": "Tue6",
          "DialogueText": "Bir sır tutabilir misiniz bayım?",
          "AudioPath": "assets/tr-TR/Vincent/37_male.ogg"
        },
        {
          "DialogueFrom": "Tue6_1",
          "DialogueText": "Bir sır tutabilir misiniz, hanımefendi?",
          "AudioPath": "assets/tr-TR/Vincent/38_female.ogg"
        },
        {
          "DialogueFrom": "Tue6_2",
          "DialogueText": "Büyüdüğümde Bayan Penny ile evlenmek istiyorum.",
          "AudioPath": "assets/tr-TR/Vincent/39.ogg"
        },
        {
          "DialogueFrom": "Tue6_3",
          "DialogueText": "Ona söylemeyin, tamam mı?",
          "AudioPath": "assets/tr-TR/Vincent/40.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "Böcek aramaya çıkmak istiyorum ama üstümü kirletince de annem kızıyor.",
          "AudioPath": "assets/tr-TR/Vincent/41.ogg"
        },
        {
          "DialogueFrom": "Wed_1",
          "DialogueText": "Arada kaldım...",
          "AudioPath": "assets/tr-TR/Vincent/42.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1",
          "DialogueText": "Sam...?",
          "AudioPath": "assets/tr-TR/Vincent/43.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_1",
          "DialogueText": "Babam geri gelecek mi?",
          "AudioPath": "assets/tr-TR/Vincent/44.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_2",
          "DialogueText": "Ama bazı yetişkinlerin şey dediklerini duydum... tüm askerlerimiz vurulup ölüyorlarmış.",
          "AudioPath": "assets/tr-TR/Vincent/45.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_3",
          "DialogueText": "Tamam!",
          "AudioPath": "assets/tr-TR/Vincent/46.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L",
          "DialogueText": "Çok heyecanlıyım Sam!",
          "AudioPath": "assets/tr-TR/Vincent/47.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L_alt",
          "DialogueText": "Anlaştık!",
          "AudioPath": "assets/tr-TR/Vincent/48.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L_alt_1",
          "DialogueText": "Ben de!",
          "AudioPath": "assets/tr-TR/Vincent/49.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19",
          "DialogueText": "Bay , Bayan Penny ile evlenecek misiniz?",
          "AudioPath": "assets/tr-TR/Vincent/50_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19_1",
          "DialogueText": "Bayan ... Şey, erkek arkadaşınız var mı?",
          "AudioPath": "assets/tr-TR/Vincent/51_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat",
          "DialogueText": "...",
          "AudioPath": "assets/tr-TR/Vincent/52.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Neden kilitli ki? Lağımı keşfe çıkmak istiyorum.",
          "AudioPath": "assets/tr-TR/Vincent/53.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Profesör Gunther mı? Müzeyi yöneten adam mı? Neden anahtarın onda olduğunu düşünüyorsun?",
          "AudioPath": "assets/tr-TR/Vincent/54.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny",
          "DialogueText": "Selam, Bay !",
          "AudioPath": "assets/tr-TR/Vincent/55_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "Selam, Bayan !",
          "AudioPath": "assets/tr-TR/Vincent/56_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "Soğanlar... Annem bazen beni bunlardan almaya gönderiyor...",
          "AudioPath": "assets/tr-TR/Vincent/57.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_2",
          "DialogueText": "Yetişkin biri için oldukça iyisin... bu yüzden sana bir sırrımı göstereceğim.",
          "AudioPath": "assets/tr-TR/Vincent/58.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3",
          "DialogueText": "İyi görünüyor, değil mi?",
          "AudioPath": "assets/tr-TR/Vincent/59.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_4",
          "DialogueText": "Ama birazcık kabuğunu soyarsan...",
          "AudioPath": "assets/tr-TR/Vincent/60.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_5",
          "DialogueText": "...İçi böceklerle dolu!",
          "AudioPath": "assets/tr-TR/Vincent/61.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_6",
          "DialogueText": "Havalı, ha?",
          "AudioPath": "assets/tr-TR/Vincent/62.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_7",
          "DialogueText": "Genelde onları eziyorum!",
          "AudioPath": "assets/tr-TR/Vincent/63.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseAnimals",
          "DialogueText": "Çiftçi , ineklerinizden birine bir eyer takıp şehre gidebilir miyim? Ben gerçek bir kovboy olmak istiyorum!",
          "AudioPath": "assets/tr-TR/Vincent/64.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseFarming",
          "DialogueText": "Şey, çiftçi , eğer sebzelerimi yersem ben de bir gün abim Sam gibi güçlü olur muyum?",
          "AudioPath": "assets/tr-TR/Vincent/65.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseFarming_1",
          "DialogueText": "O çok güçlü, geçen gece ağaca tırmanırken Bayan Penny'yi âdeta taşıdığını gördüm.",
          "AudioPath": "assets/tr-TR/Vincent/66.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseMinerals",
          "DialogueText": "Çiftçi , goblinlerin derin mağaralarda yaşadığı doğru mu... ve küçük kızlarını akşam yemeği için kaçırmaktan hoşlanıyorlar mı?",
          "AudioPath": "assets/tr-TR/Vincent/67.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny",
          "DialogueText": "Bu, ödevimi yapmak zorunda olmadığım anlamına mı geliyor?",
          "AudioPath": "assets/tr-TR/Vincent/68.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny_implied",
          "DialogueText": "Bayağıdır bir şey yapmayı düşünüyordum...",
          "AudioPath": "assets/tr-TR/Vincent/69.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Möööö!",
          "AudioPath": "assets/tr-TR/Vincent/70.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Babacığım, sapan oyununu bir daha oynayabilir miyim?",
          "AudioPath": "assets/tr-TR/Vincent/71.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "Annem beni labirente sokmuyor. *hıçkırır*",
          "AudioPath": "assets/tr-TR/Vincent/72.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Labirenti geçtik mi? Ödülüm nerede?",
          "AudioPath": "assets/tr-TR/Vincent/73.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "Yumurta avı ne zaman başlayacak?! Çok heyecanlıyım!",
          "AudioPath": "assets/tr-TR/Vincent/74.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13_1",
          "DialogueText": "Kocaman bir yumurtanın içinde yaşamak istiyorum!",
          "AudioPath": "assets/tr-TR/Vincent/75.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "Adil değil... Ben neden Çiçek Kraliçesi olamıyorum?",
          "AudioPath": "assets/tr-TR/Vincent/76.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "Eğer bir çiçek kraliçesi varsa... Mutlaka bir çiçek kralı da olmalı!",
          "AudioPath": "assets/tr-TR/Vincent/77.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Sıkıldım...",
          "AudioPath": "assets/tr-TR/Vincent/78.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "Neden havuzun orada oynayamıyorum ki?",
          "AudioPath": "assets/tr-TR/Vincent/79.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "Nadir bulunan yeşil denizanasını görmek istiyorum!",
          "AudioPath": "assets/tr-TR/Vincent/80.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "Daha önce bir denizanasına dokunmuştum, elimi yakmıştı.",
          "AudioPath": "assets/tr-TR/Vincent/81.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Hediyeleri açabilir miyiz?!",
          "AudioPath": "assets/tr-TR/Vincent/82.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "İstediğim oyuncak dinozoru aldım! Bak!?İsmi şey... Ste... Stego... Stegozor! ...Sanırım.",
          "AudioPath": "assets/tr-TR/Vincent/83.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Şurada kocaman bir balık gördüm!",
          "AudioPath": "assets/tr-TR/Vincent/84.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "Abim kardan adam yapmama yardım ediyor!",
          "AudioPath": "assets/tr-TR/Vincent/85.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "Vay be, çok teşekkürler! Dehşet bir şey bu!",
          "AudioPath": "assets/tr-TR/Vincent/86.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Bana mı? Vay canına, teşekkürler!",
          "AudioPath": "assets/tr-TR/Vincent/87.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Bu çok da hoş değil hani...",
          "AudioPath": "assets/tr-TR/Vincent/88.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "Öğk, bu ne?",
          "AudioPath": "assets/tr-TR/Vincent/89.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Teşekkürler!",
          "AudioPath": "assets/tr-TR/Vincent/90.ogg"
        }
      ]
    }
