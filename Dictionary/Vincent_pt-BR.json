      "Entries": [
        {
          "DialogueFrom": "AcceptBirthdayGift_Negative",
          "DialogueText": "Um presente de aniversário? Oh... isso não é legal.",
          "AudioPath": "assets/pt-BR/Vincent/1.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive",
          "DialogueText": "Isso é um presente de aniversário? Posso abrir agora?",
          "AudioPath": "assets/pt-BR/Vincent/2.ogg"
        },
        {
          "DialogueFrom": "AcceptBirthdayGift_Positive_1",
          "DialogueText": "Oh, uau! Isto é exatamente o que eu queria! Iaiaiaia!",
          "AudioPath": "assets/pt-BR/Vincent/3.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(O)StardropTea",
          "DialogueText": "Uau! É tão doce! Hmmmm....",
          "AudioPath": "assets/pt-BR/Vincent/4.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_(TR)FrogEgg",
          "DialogueText": "*Aperta* *aperta* ... Que nojo! Posso ficar com isso? Posso ficar? Ebaaa!",
          "AudioPath": "assets/pt-BR/Vincent/5.ogg"
        },
        {
          "DialogueFrom": "AcceptGift_Negative_alcohol_item",
          "DialogueText": "*cheira*... o que é isso? O cheiro é horrível... Mãeee!!!",
          "AudioPath": "assets/pt-BR/Vincent/6.ogg"
        },
        {
          "DialogueFrom": "Fri",
          "DialogueText": "Eca! É Noite de beterraba hoje...",
          "AudioPath": "assets/pt-BR/Vincent/7.ogg"
        },
        {
          "DialogueFrom": "Fri_1",
          "DialogueText": "Ah, não! Mamãe vai fazer sopa de lentilhas hoje.",
          "AudioPath": "assets/pt-BR/Vincent/8.ogg"
        },
        {
          "DialogueFrom": "GreenRain",
          "DialogueText": "Mãe! Eu quero ir lá fora para brincar!",
          "AudioPath": "assets/pt-BR/Vincent/9.ogg"
        },
        {
          "DialogueFrom": "GreenRain_2",
          "DialogueText": "está coberto de musgos.",
          "AudioPath": "assets/pt-BR/Vincent/10.ogg"
        },
        {
          "DialogueFrom": "Introduction",
          "DialogueText": "Ah, um estranho! Meu nome é Vincent.",
          "AudioPath": "assets/pt-BR/Vincent/11.ogg"
        },
        {
          "DialogueFrom": "Introduction_1",
          "DialogueText": "Mamãe me disse para nunca falar com estranhos... Mas você parece legal.",
          "AudioPath": "assets/pt-BR/Vincent/12.ogg"
        },
        {
          "DialogueFrom": "married_Sam",
          "DialogueText": "Não leve o meu Sammy embora!",
          "AudioPath": "assets/pt-BR/Vincent/13.ogg"
        },
        {
          "DialogueFrom": "Mon",
          "DialogueText": "Só quero ser como meu irmãozão quando eu crescer!",
          "AudioPath": "assets/pt-BR/Vincent/14.ogg"
        },
        {
          "DialogueFrom": "Mon_1",
          "DialogueText": "Tá?! Tchau!",
          "AudioPath": "assets/pt-BR/Vincent/15.ogg"
        },
        {
          "DialogueFrom": "Mon4",
          "DialogueText": "kent#Ainda bem que o pai voltou... mas ele está diferente do que eu lembrava...|Papai nunca fica muito em casa... Sinto falta de brincar com ele.",
          "AudioPath": "assets/pt-BR/Vincent/16.ogg"
        },
        {
          "DialogueFrom": "reject_869",
          "DialogueText": "Oooh! Belo achado!",
          "AudioPath": "assets/pt-BR/Vincent/17.ogg"
        },
        {
          "DialogueFrom": "reject_869_1",
          "DialogueText": "Mas a minha mãe não vai deixar eu ficar com ele...",
          "AudioPath": "assets/pt-BR/Vincent/18.ogg"
        },
        {
          "DialogueFrom": "Resort",
          "DialogueText": "Se você olhar de perto, consegue ver pequenos bichinhos pulando da areia.",
          "AudioPath": "assets/pt-BR/Vincent/19.ogg"
        },
        {
          "DialogueFrom": "Resort_Entering",
          "DialogueText": "Isso foi divertido! Eu quero ser um capitão de barco algum dia!",
          "AudioPath": "assets/pt-BR/Vincent/20.ogg"
        },
        {
          "DialogueFrom": "Resort_Leaving",
          "DialogueText": "Eu quero voltar logo!",
          "AudioPath": "assets/pt-BR/Vincent/21.ogg"
        },
        {
          "DialogueFrom": "Resort_Shore",
          "DialogueText": "Eu quero ver um tubarão!",
          "AudioPath": "assets/pt-BR/Vincent/22.ogg"
        },
        {
          "DialogueFrom": "Sat",
          "DialogueText": "A tia Penny me faz ler um livro novo toda semana.",
          "AudioPath": "assets/pt-BR/Vincent/23.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "E ela me deixa escolhê-los às vezes!",
          "AudioPath": "assets/pt-BR/Vincent/24.ogg"
        },
        {
          "DialogueFrom": "Sat_1",
          "DialogueText": "Papai vai voltar em breve!",
          "AudioPath": "assets/pt-BR/Vincent/25.ogg"
        },
        {
          "DialogueFrom": "Sat_1_1",
          "DialogueText": "Espero que ele me traga brinquedos.",
          "AudioPath": "assets/pt-BR/Vincent/26.ogg"
        },
        {
          "DialogueFrom": "Sat8",
          "DialogueText": "kent#Papai está meio estranho desde que voltou. O que será que aconteceu com ele?",
          "AudioPath": "assets/pt-BR/Vincent/27.ogg"
        },
        {
          "DialogueFrom": "Sat8_1",
          "DialogueText": "Ele não brinca mais comigo.|Meu pergunto se meu pai vai voltar um dia...",
          "AudioPath": "assets/pt-BR/Vincent/28.ogg"
        },
        {
          "DialogueFrom": "Sun",
          "DialogueText": "Oi, moço!",
          "AudioPath": "assets/pt-BR/Vincent/29_male.ogg"
        },
        {
          "DialogueFrom": "Sun_1",
          "DialogueText": "Oi, moça!",
          "AudioPath": "assets/pt-BR/Vincent/30_female.ogg"
        },
        {
          "DialogueFrom": "Sun10",
          "DialogueText": "Oi, tio !",
          "AudioPath": "assets/pt-BR/Vincent/31_male.ogg"
        },
        {
          "DialogueFrom": "Sun10_1",
          "DialogueText": "Oi, tia!",
          "AudioPath": "assets/pt-BR/Vincent/32_female.ogg"
        },
        {
          "DialogueFrom": "Sun10_2",
          "DialogueText": "Não diga à mamãe... mas você é meu adulto favorito.",
          "AudioPath": "assets/pt-BR/Vincent/33.ogg"
        },
        {
          "DialogueFrom": "Thu",
          "DialogueText": "Estou com fome! Cadê a minha mãe?",
          "AudioPath": "assets/pt-BR/Vincent/34.ogg"
        },
        {
          "DialogueFrom": "Thu2",
          "DialogueText": "Você não é chato como a maioria dos adultos!",
          "AudioPath": "assets/pt-BR/Vincent/35.ogg"
        },
        {
          "DialogueFrom": "Tue",
          "DialogueText": "*Aaah*... Mamãe não quer deixar eu comer mais bala de goma hoje.",
          "AudioPath": "assets/pt-BR/Vincent/36.ogg"
        },
        {
          "DialogueFrom": "Tue6",
          "DialogueText": "Você guarda segredo, moço?",
          "AudioPath": "assets/pt-BR/Vincent/37_male.ogg"
        },
        {
          "DialogueFrom": "Tue6_1",
          "DialogueText": "Você guarda segredo, moça?",
          "AudioPath": "assets/pt-BR/Vincent/38_female.ogg"
        },
        {
          "DialogueFrom": "Tue6_2",
          "DialogueText": "Eu quero casar com a tia Penny quando crescer.",
          "AudioPath": "assets/pt-BR/Vincent/39.ogg"
        },
        {
          "DialogueFrom": "Tue6_3",
          "DialogueText": "Mas não diz para ela, tá?",
          "AudioPath": "assets/pt-BR/Vincent/40.ogg"
        },
        {
          "DialogueFrom": "Wed",
          "DialogueText": "Eu quero caçar insetos, mas mamãe fica muito zangada quando me sujo.",
          "AudioPath": "assets/pt-BR/Vincent/41.ogg"
        },
        {
          "DialogueFrom": "Wed_1",
          "DialogueText": "É uma escolha difícil.",
          "AudioPath": "assets/pt-BR/Vincent/42.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1",
          "DialogueText": "Sam?...",
          "AudioPath": "assets/pt-BR/Vincent/43.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_1",
          "DialogueText": "Papai vai voltar para casa um dia?",
          "AudioPath": "assets/pt-BR/Vincent/44.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_2",
          "DialogueText": "Mas, ouvi alguns adultos dizerem que... nossas tropas estão sendo destroçadas.",
          "AudioPath": "assets/pt-BR/Vincent/45.ogg"
        },
        {
          "DialogueFrom": "Event:Beach/733330/f Sam 750/w sunny/t 700 1500/z winter/y 1_3",
          "DialogueText": "Certo!",
          "AudioPath": "assets/pt-BR/Vincent/46.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L",
          "DialogueText": "Estou bem animado, Sam!",
          "AudioPath": "assets/pt-BR/Vincent/47.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L_alt",
          "DialogueText": "OK!",
          "AudioPath": "assets/pt-BR/Vincent/48.ogg"
        },
        {
          "DialogueFrom": "Event:FarmHouse/3918603/e 3918602/O Sam/t 610 1700/A samJob3/p Sam/L_alt_1",
          "DialogueText": "Eu também!",
          "AudioPath": "assets/pt-BR/Vincent/49.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19",
          "DialogueText": "Sr. , você vai casar com a srta. Penny?",
          "AudioPath": "assets/pt-BR/Vincent/50_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/181928/f Penny 2000/t 900 1600/w sunny/G !IS_PASSIVE_FESTIVAL_TODAY TroutDerby, !SEASON_DAY summer 17 summer 18 summer 19_1",
          "DialogueText": "Sra. ... Há... você tem namorado?",
          "AudioPath": "assets/pt-BR/Vincent/51_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat",
          "DialogueText": "...",
          "AudioPath": "assets/pt-BR/Vincent/52.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Por quê está trancado? Quero explorar o esgoto!",
          "AudioPath": "assets/pt-BR/Vincent/53.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/318560/j 10/t 900 1600/d Tue Wed Fri Sat_1",
          "DialogueText": "Professor Gunther? O homem responsável pelo museu? Por que você acha que ele tem a chave?",
          "AudioPath": "assets/pt-BR/Vincent/54.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny",
          "DialogueText": "Olá, senhor !",
          "AudioPath": "assets/pt-BR/Vincent/55_male.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "Olá, senhora !",
          "AudioPath": "assets/pt-BR/Vincent/56_female.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_1",
          "DialogueText": "C...cebolinha. Às vezes a mamãe me manda colher algumas...",
          "AudioPath": "assets/pt-BR/Vincent/57.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_2",
          "DialogueText": "Você é muito legal para um adulto... então vou te mostrar um segredo.",
          "AudioPath": "assets/pt-BR/Vincent/58.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_3",
          "DialogueText": "Quando você olha assim, parece bom, não é?",
          "AudioPath": "assets/pt-BR/Vincent/59.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_4",
          "DialogueText": "Mas se você descascar um pouco...",
          "AudioPath": "assets/pt-BR/Vincent/60.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_5",
          "DialogueText": "...Ela está repleta de insetos!",
          "AudioPath": "assets/pt-BR/Vincent/61.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_6",
          "DialogueText": "Legal, não é?",
          "AudioPath": "assets/pt-BR/Vincent/62.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/3910979/f Vincent 2000/f Jas 2000/t 600 1700/z summer/z fall/z winter/w sunny_7",
          "DialogueText": "Normalmente eu só amasso eles!",
          "AudioPath": "assets/pt-BR/Vincent/63.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseAnimals",
          "DialogueText": "? Posso colocar uma sela em uma de suas vacas e ir até a cidade? Quero ser um caubói de verdade!",
          "AudioPath": "assets/pt-BR/Vincent/64.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseFarming",
          "DialogueText": "Hã... ? Se eu comer muitas verduras, vou ficar forte como o meu irmão Sam?",
          "AudioPath": "assets/pt-BR/Vincent/65.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseFarming_1",
          "DialogueText": "Ele é tão forte que o vi levantando a srta. Penny do chão ontem à noite quando eles estavam escalando uma árvore.",
          "AudioPath": "assets/pt-BR/Vincent/66.ogg"
        },
        {
          "DialogueFrom": "Event:Forest/choseMinerals",
          "DialogueText": "? É verdade que os goblins moram no fundo das cavernas... e que gostam de sequestrar garotinhas para o jantar?",
          "AudioPath": "assets/pt-BR/Vincent/67.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny",
          "DialogueText": "Isso quer dizer que eu não tenho que fazer a tarefa de casa?",
          "AudioPath": "assets/pt-BR/Vincent/68.ogg"
        },
        {
          "DialogueFrom": "Event:Town/6184643/f Haley 3500/O Haley/t 800 1500/w sunny_implied",
          "DialogueText": "Eu tenho pensado muito sobre bolo ultimamente...",
          "AudioPath": "assets/pt-BR/Vincent/69.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16",
          "DialogueText": "Muuuu!",
          "AudioPath": "assets/pt-BR/Vincent/70.ogg"
        },
        {
          "DialogueFrom": "Festival/fall16_1",
          "DialogueText": "Posso jogar o jogo do estilingue de novo, Pai!? Por favor?",
          "AudioPath": "assets/pt-BR/Vincent/71.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27",
          "DialogueText": "Mamãe não quer me deixar ir no labirinto. (*Chuif, chuif*)",
          "AudioPath": "assets/pt-BR/Vincent/72.ogg"
        },
        {
          "DialogueFrom": "Festival/fall27_1",
          "DialogueText": "Completamos o labirinto? Cadê o meu prêmio?",
          "AudioPath": "assets/pt-BR/Vincent/73.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13",
          "DialogueText": "Quando a caça aos ovos começa?! Mal posso esperar!",
          "AudioPath": "assets/pt-BR/Vincent/74.ogg"
        },
        {
          "DialogueFrom": "Festival/spring13_1",
          "DialogueText": "Eu queria morar dentro de um ovo grande!",
          "AudioPath": "assets/pt-BR/Vincent/75.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24",
          "DialogueText": "Isso não é justo... Por que não posso ser a rainha das flores?",
          "AudioPath": "assets/pt-BR/Vincent/76.ogg"
        },
        {
          "DialogueFrom": "Festival/spring24_1",
          "DialogueText": "Se existe uma rainha das flores... então deve existir um rei das flores!",
          "AudioPath": "assets/pt-BR/Vincent/77.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11",
          "DialogueText": "Que tédio...",
          "AudioPath": "assets/pt-BR/Vincent/78.ogg"
        },
        {
          "DialogueFrom": "Festival/summer11_1",
          "DialogueText": "Por que eu não posso ir brincar na água?",
          "AudioPath": "assets/pt-BR/Vincent/79.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28",
          "DialogueText": "Quero ver a rara água-viva verde!",
          "AudioPath": "assets/pt-BR/Vincent/80.ogg"
        },
        {
          "DialogueFrom": "Festival/summer28_1",
          "DialogueText": "Uma vez eu toquei em uma medusa e minha mão ficou ardendo.",
          "AudioPath": "assets/pt-BR/Vincent/81.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25",
          "DialogueText": "Podemos abri-los agora?!",
          "AudioPath": "assets/pt-BR/Vincent/82.ogg"
        },
        {
          "DialogueFrom": "Festival/winter25_1",
          "DialogueText": "Eu ganhei o dinossauro de brinquedo que eu queria! Olha!?O nome dele é... este-...estego-...estogossonauro! ...Eu acho.",
          "AudioPath": "assets/pt-BR/Vincent/83.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8",
          "DialogueText": "Eu vi um peixe ali embaixo!",
          "AudioPath": "assets/pt-BR/Vincent/84.ogg"
        },
        {
          "DialogueFrom": "Festival/winter8_1",
          "DialogueText": "Meu irmãozão está me ajudando a fazer um boneco de neve!",
          "AudioPath": "assets/pt-BR/Vincent/85.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes",
          "DialogueText": "Nossa, muito obrigado! Isso é incrível!",
          "AudioPath": "assets/pt-BR/Vincent/86.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_1",
          "DialogueText": "Para mim? Nossa, obrigado!",
          "AudioPath": "assets/pt-BR/Vincent/87.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_2",
          "DialogueText": "Não foi muito engraçado...",
          "AudioPath": "assets/pt-BR/Vincent/88.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_3",
          "DialogueText": "Eca! O que é isso?",
          "AudioPath": "assets/pt-BR/Vincent/89.ogg"
        },
        {
          "DialogueFrom": "NPCGiftTastes_4",
          "DialogueText": "Obrigado!",
          "AudioPath": "assets/pt-BR/Vincent/90.ogg"
        }
      ]
    }
  ]
