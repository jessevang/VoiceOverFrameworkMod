{
  "VoicePacks": [
    {
      "Format": "1.0.0",
      "VoicePackId": "Dictionary.Grandpa.de-DE",
      "VoicePackName": "Dictionary (Grandpa - de-DE)",
      "Character": "Grandpa",
      "Language": "de-DE",
      "Entries": [
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H",
          "DialogueText": "Mein lieber Junge...",
          "AudioPath": "assets/de-DE/Grandpa/1_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Meine liebste Enkelin...",
          "AudioPath": "assets/de-DE/Grandpa/2_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "Es ist viele Jahre her, seit wir das letzte Mal gesprochen haben. Du warst noch ein kleiner Junge... Erinnerst du dich?",
          "AudioPath": "assets/de-DE/Grandpa/3_male.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_3",
          "DialogueText": "Es ist viele Jahre her, seit wir das letzte Mal gesprochen haben. Du warst noch ein kleines Mädchen... Erinnerst du dich?",
          "AudioPath": "assets/de-DE/Grandpa/4_female.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_4",
          "DialogueText": "Sieh an, wie weit du gekommen bist!",
          "AudioPath": "assets/de-DE/Grandpa/5.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_5",
          "DialogueText": "Du magst mich vielleicht vergessen haben, aber ich war immer hier.",
          "AudioPath": "assets/de-DE/Grandpa/6.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_6",
          "DialogueText": "Hör zu... Mein Körper hat diese Welt verlassen, aber mein Herz wird immer im Sternentautal bleiben.",
          "AudioPath": "assets/de-DE/Grandpa/7.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_1",
          "DialogueText": "Du bist jetzt schon seit zwei Jahren hier...",
          "AudioPath": "assets/de-DE/Grandpa/8.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2",
          "DialogueText": "Die Zukunft von Hof liegt jetzt in deiner Hand.",
          "AudioPath": "assets/de-DE/Grandpa/9.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558291/y 3/H_2_1",
          "DialogueText": "Leb wohl!",
          "AudioPath": "assets/de-DE/Grandpa/10.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H",
          "DialogueText": "Es freut mich, dass du mich gerufen hast. Ich... war das letzte Mal vielleicht ein wenig zu hart zu dir.",
          "AudioPath": "assets/de-DE/Grandpa/11.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "Vergib mir. Du hast wirklich hart gearbeitet und ich bin stolz auf dich.",
          "AudioPath": "assets/de-DE/Grandpa/12.ogg"
        },
        {
          "DialogueFrom": "Event:AbandonedJojaMart/558292/e 321777/t 600 620/H_1",
          "DialogueText": "Mal sehen...",
          "AudioPath": "assets/de-DE/Grandpa/13.ogg"
        }
      ]
    }
  ]
}